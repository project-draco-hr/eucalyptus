{
  return ImmutableList.<SystemAccountRole>of(new BasicSystemAccountRole(StorageProperties.EBS_ROLE_NAME,"/blockstorage",StorageProperties.DEFAULT_ASSUME_ROLE_POLICY,ImmutableList.<AttachedPolicy>of(new BasicAttachedPolicy(StorageProperties.S3_BUCKET_ACCESS_POLICY_NAME,StorageProperties.S3_SNAPSHOT_BUCKET_ACCESS_POLICY),new BasicAttachedPolicy(StorageProperties.S3_OBJECT_ACCESS_POLICY_NAME,StorageProperties.S3_SNAPSHOT_OBJECT_ACCESS_POLICY))));
}

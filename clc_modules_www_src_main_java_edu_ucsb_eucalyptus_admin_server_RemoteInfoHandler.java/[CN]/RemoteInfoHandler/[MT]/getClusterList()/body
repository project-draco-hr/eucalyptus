{
  List<ClusterInfoWeb> clusterList=new ArrayList<ClusterInfoWeb>();
  try {
    for (    ClusterConfiguration c : ServiceConfigurations.getConfigurations(ClusterConfiguration.class))     clusterList.add(new ClusterInfoWeb(c.getName(),c.getHostName(),c.getPort(),c.getMinVlan(),c.getMaxVlan()));
  }
 catch (  Throwable e) {
    LOG.debug("Got an error while trying to retrieving storage controller configuration list",e);
  }
  return clusterList;
}

{
  final RunInstancesType req=new RunInstancesType();
  req.setImageId(this.imageId);
  if (this.groupName != null)   req.setGroupSet(Lists.newArrayList(this.groupName));
  if (this.userData != null)   req.setUserData(this.userData);
  if (this.instanceType != null)   req.setInstanceType(this.instanceType);
  if (this.availabilityZone != null)   req.setAvailabilityZone(this.availabilityZone);
  req.setMonitoring(this.monitoringEnabled);
  req.setMinCount(1);
  req.setMaxCount(1);
  return req;
}

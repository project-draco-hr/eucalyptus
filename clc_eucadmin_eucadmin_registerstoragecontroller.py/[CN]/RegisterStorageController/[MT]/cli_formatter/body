def cli_formatter(self, data):
    eucadmin.registerrequest.RegisterRequest.cli_formatter(self, data)
    if (not self.sc_preexists):
        print  >> sys.stderr, "Registered the first storage controller in partition '{0}'.  You must choose a storage back end with ``euca-modify-property -p {0}.storage.blockstoragemanager=$BACKEND''".format(self.partition)

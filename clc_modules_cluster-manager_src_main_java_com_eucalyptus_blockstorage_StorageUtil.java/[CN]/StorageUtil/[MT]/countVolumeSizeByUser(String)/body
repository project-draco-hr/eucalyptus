{
  EntityWrapper<Volume> db=EntityWrapper.get(Volume.class);
  try {
    List<Volume> vols=db.query(new Volume(UserFullName.getInstance(Accounts.lookupUserById(userId)),null));
    long size=0;
    for (    Volume v : vols) {
      size+=v.getSize();
    }
    db.commit();
    return size;
  }
 catch (  Exception e) {
    db.rollback();
    throw new AuthException("Failed to search volume info",e);
  }
}

{
  EntityWrapper<Snapshot> db=EntityWrapper.get(Snapshot.class);
  try {
    List<Snapshot> vols=db.query(new Snapshot(){
{
        this.setOwnerAccountNumber(accountId);
      }
    }
);
    db.commit();
    return vols.size();
  }
 catch (  Exception e) {
    db.rollback();
    throw new AuthException("Failed to search snapshot info",e);
  }
}

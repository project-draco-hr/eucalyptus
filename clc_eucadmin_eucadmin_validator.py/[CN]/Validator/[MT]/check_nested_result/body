@staticmethod
def check_nested_result(parent, result, print_output=False):
    failed = False
    for key in result.keys():
        if (type(result[key]) != dict):
            raise Exception('Error parsing validation data')
        if result[key].has_key('cmd'):
            failed |= Validator.check_nested_result(((parent + key) + ':'), result[key]['output'], print_output=print_output)
        elif (not result[key].has_key('failed')):
            failed |= Validator.check_nested_result(((parent + key) + ':'), result[key], print_output=print_output)
        elif int(result[key]['failed']):
            if print_output:
                print ('%s%s: %s' % (parent, key, result[key].get('error', 'No details provided')))
            failed = True
    return failed

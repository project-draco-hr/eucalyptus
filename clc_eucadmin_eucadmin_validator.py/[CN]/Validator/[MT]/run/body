@classmethod
def run(cls):
    parser = build_parser()
    args = parser.parse_args()
    obj = cls(**vars(args))
    result = obj.main()
    if args.json:
        print json.dumps(result, indent=4)
    elif args.quiet:
        sys.exit(Validator.check_nested_result('', result))
    else:
        sys.exit(Validator.check_nested_result('', result, print_output=True))

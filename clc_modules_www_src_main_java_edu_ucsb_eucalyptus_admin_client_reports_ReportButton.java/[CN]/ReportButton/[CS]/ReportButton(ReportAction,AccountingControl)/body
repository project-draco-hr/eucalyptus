{
  super(action.getImageText(controller),"Go to page " + action.name().toLowerCase() + ".",AccountingControl.RESOURCES.BUTTON_STYLE,action.getImageName(),new ClickHandler(){
    @Override public void onClick(    ClickEvent clickevent){
      action.apply(controller);
    }
  }
);
  this.setWidth("200px");
  this.action=action;
  this.controller=controller;
}

def get_heartbeat_data(self):
    resp = boto.utils.retry_url(self.url)
    lines = resp.splitlines()
    self.data = {}
    for line in lines:
        pairs = line.split()
        t = pairs[0].split('=')
        d = {}
        self.data[t[1]] = d
        for pair in pairs[1:]:
            t = pair.split('=')
            d[t[0]] = self._get_value(t[1])

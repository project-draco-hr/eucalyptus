{
  if (users.keySet().size() == 0) {
    EntityWrapper<ReportingUser> entityWrapper=EntityWrapper.get(ReportingUser.class);
    try {
      @SuppressWarnings("rawtypes") List reportingUsers=(List)entityWrapper.createQuery("from ReportingUser").list();
      for (      Object obj : reportingUsers) {
        ReportingUser user=(ReportingUser)obj;
        users.put(user.getId(),user.getName());
      }
      entityWrapper.commit();
    }
 catch (    Exception ex) {
      log.error(ex);
      entityWrapper.rollback();
      throw new RuntimeException(ex);
    }
  }
}

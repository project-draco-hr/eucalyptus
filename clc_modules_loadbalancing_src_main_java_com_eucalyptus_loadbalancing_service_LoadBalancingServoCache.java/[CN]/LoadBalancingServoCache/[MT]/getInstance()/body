{
  if (_instance == null) {
    _instance=new LoadBalancingServoCache();
  }
  if (cachedEntities == null) {
    cachedEntities=CacheBuilder.newBuilder().maximumSize(10000).expireAfterWrite(getCacheDuration(),TimeUnit.MINUTES).build(new CacheLoader<String,CachedEntities>(){
      @Override public CachedEntities load(      String instanceId) throws Exception {
        return CachedEntities.loadEntities(instanceId);
      }
    }
);
  }
  return _instance;
}

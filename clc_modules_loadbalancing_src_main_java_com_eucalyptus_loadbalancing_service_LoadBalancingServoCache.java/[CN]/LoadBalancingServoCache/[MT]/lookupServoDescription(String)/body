{
  final LoadBalancer lb;
  try {
    lb=this.getLoadBalancer(instanceId);
  }
 catch (  final Exception ex) {
    LOG.error("Failed to lookup loadbalancer for VM - " + instanceId);
    return null;
  }
  final LoadBalancerZone zone;
  try {
    zone=this.getLoadBalancerZone(instanceId);
  }
 catch (  final Exception ex) {
    LOG.error("Failed to lookup loadbalancer zone VM - " + instanceId);
    return null;
  }
  List<LoadBalancerListener> lbListeners=Lists.newArrayList();
  try {
    lbListeners=this.getLoadBalancerListeners(instanceId);
  }
 catch (  final Exception ex) {
    ;
  }
  List<LoadBalancerBackendServerDescription> backendServers=Lists.newArrayList();
  try {
    backendServers=this.getLoadBalancerBackendServerDescriptions(instanceId);
  }
 catch (  final Exception ex) {
    ;
  }
  List<LoadBalancerPolicyDescription> lbPolicies=Lists.newArrayList();
  try {
    lbPolicies=this.getLoadBalancerPolicyDescriptions(instanceId);
  }
 catch (  final Exception ex) {
    LOG.error("Failed to retrieve loadbalancer's policies",ex);
  }
  return LoadBalancers.getServoDescription(lb,zone,lbListeners,backendServers,lbPolicies);
}

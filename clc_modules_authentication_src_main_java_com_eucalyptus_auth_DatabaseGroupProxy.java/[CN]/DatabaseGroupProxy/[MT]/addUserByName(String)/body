{
  EntityWrapper<GroupEntity> db=EntityWrapper.get(GroupEntity.class);
  EntityManager em=db.getEntityManager();
  try {
    GroupEntity groupEntity=em.find(GroupEntity.class,this.delegate.getId());
    UserEntity userEntity=DatabaseAuthUtils.getUniqueUser(db.getSession(),userName,groupEntity.getAccount().getName());
    groupEntity.getUsers().add(userEntity);
    db.commit();
  }
 catch (  Throwable e) {
    db.rollback();
    Debugging.logError(LOG,e,"Failed to add user " + userName + " to group "+ this.delegate);
    throw new AuthException(e);
  }
}

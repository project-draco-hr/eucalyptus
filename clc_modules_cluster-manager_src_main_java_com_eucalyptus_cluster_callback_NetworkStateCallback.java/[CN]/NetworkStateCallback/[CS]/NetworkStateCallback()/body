{
  this.setRequest(new DescribeNetworksType(){
{
      regarding();
      setClusterControllers(Lists.newArrayList(Clusters.getInstance().getClusterAddresses()));
      try {
        setNameserver(NetworkUtil.getAllAddresses().get(0));
      }
 catch (      SocketException e) {
        LOG.error(e,e);
      }
    }
  }
);
}

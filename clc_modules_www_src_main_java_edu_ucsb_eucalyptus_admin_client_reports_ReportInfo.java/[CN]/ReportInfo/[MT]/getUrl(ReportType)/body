{
  if (this.controller != null) {
    return "/reports?name=" + this.controller.getCurrentFileName() + "&type="+ type.name().toLowerCase()+ "&session="+ this.controller.getSessionid()+ "&page="+ this.controller.getCurrentPage()+ "&flush="+ this.controller.getForceFlush();
  }
 else {
    return BOGUS.getUrl(type);
  }
}

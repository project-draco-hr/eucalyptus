{
  return "/reports?name=" + this.controller.getCurrentFileName() + "&type="+ type.name().toLowerCase()+ "&session="+ this.controller.getSessionid()+ "&page="+ this.controller.getCurrentPage()+ "&flush="+ this.controller.getForceFlush()+ "&start="+ this.controller.getStartMillis()+ "&end="+ this.controller.getEndMillis()+ (this.remote ? "&component=" + this.component + "&cluster="+ this.clusterName+ "&host="+ this.hostName : "");
}

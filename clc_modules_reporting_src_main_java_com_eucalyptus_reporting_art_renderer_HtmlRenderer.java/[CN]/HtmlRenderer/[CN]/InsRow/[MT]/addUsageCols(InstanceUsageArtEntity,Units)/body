{
  addCol(entity.getCpuPercentAvg()).addCol(UnitUtil.convertSizeTime(entity.getNetIoBetweenZoneInMegSecs(),SizeUnit.MB,units.getSizeUnit(),TimeUnit.SECS,units.getTimeUnit())).addCol(UnitUtil.convertSizeTime(entity.getNetIoWithinZoneInMegSecs(),SizeUnit.MB,units.getSizeUnit(),TimeUnit.SECS,units.getTimeUnit())).addCol(UnitUtil.convertSizeTime(entity.getNetIoPublicIpInMegSecs(),SizeUnit.MB,units.getSizeUnit(),TimeUnit.SECS,units.getTimeUnit())).addCol(UnitUtil.convertSizeTime(entity.getNetIoBetweenZoneOutMegSecs(),SizeUnit.MB,units.getSizeUnit(),TimeUnit.SECS,units.getTimeUnit())).addCol(UnitUtil.convertSizeTime(entity.getNetIoWithinZoneOutMegSecs(),SizeUnit.MB,units.getSizeUnit(),TimeUnit.SECS,units.getTimeUnit())).addCol(UnitUtil.convertSizeTime(entity.getNetIoPublicIpOutMegSecs(),SizeUnit.MB,units.getSizeUnit(),TimeUnit.SECS,units.getTimeUnit()));
  return this;
}

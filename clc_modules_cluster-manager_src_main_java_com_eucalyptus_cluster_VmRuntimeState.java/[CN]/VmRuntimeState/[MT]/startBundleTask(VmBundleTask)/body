{
  if (this.bundleTask == null) {
    this.bundleTask=task;
    return true;
  }
 else {
    if ((this.getBundleTask() != null) && BundleState.failed.equals(BundleState.valueOf(this.getBundleTask().getState()))) {
      this.resetBundleTask();
      this.bundleTask=task;
      return true;
    }
 else {
      return false;
    }
  }
}

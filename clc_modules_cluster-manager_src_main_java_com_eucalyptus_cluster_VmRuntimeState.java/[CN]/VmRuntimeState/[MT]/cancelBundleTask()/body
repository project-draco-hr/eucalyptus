{
  if (this.getBundleTask() != null) {
    this.getBundleTask().setState(BundleState.canceling.name());
    EventRecord.here(BundleCallback.class,EventType.BUNDLE_CANCELING,this.vmInstance.getOwner().toString(),this.getBundleTask().getBundleId(),this.getVmInstance().getInstanceId(),this.getBundleTask().getState()).info();
    return true;
  }
 else {
    return false;
  }
}

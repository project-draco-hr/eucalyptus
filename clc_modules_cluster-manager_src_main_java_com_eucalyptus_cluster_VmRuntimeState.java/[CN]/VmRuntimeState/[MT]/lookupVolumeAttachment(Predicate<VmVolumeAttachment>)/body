{
  final VmVolumeAttachment v=Iterables.find(this.getTransientVolumes().values(),pred);
  if (v == null) {
    throw new NoSuchElementException("Failed to find volume attachment for instance " + this.getVmInstance().getInstanceId() + " using predicate "+ pred.getClass().getCanonicalName());
  }
 else {
    return v;
  }
}

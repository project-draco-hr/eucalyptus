{
  return new Function<BundleTask,VmBundleTask>(){
    @Override public VmBundleTask apply(    final BundleTask input){
      return new VmBundleTask(vm,input.getState(),input.getStartTime(),input.getUpdateTime(),input.getProgress(),input.getBucket(),input.getPrefix(),input.getErrorMessage(),input.getErrorCode());
    }
  }
;
}

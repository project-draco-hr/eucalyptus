{
  return new Function<VmBundleTask,BundleTask>(){
    @Override public BundleTask apply(    final VmBundleTask input){
      return new BundleTask(vm.getInstanceId(),input.getBundleId(),input.getState().name(),input.getStartTime(),input.getUpdateTime(),input.getProgress(),input.getBucket(),input.getPrefix(),input.getErrorMessage(),input.getErrorCode());
    }
  }
;
}

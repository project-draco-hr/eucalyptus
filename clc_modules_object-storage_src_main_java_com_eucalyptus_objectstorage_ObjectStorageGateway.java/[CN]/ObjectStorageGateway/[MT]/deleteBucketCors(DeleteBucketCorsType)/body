{
  String bucketName="nullBucket";
  DeleteBucketCorsResponseType response=null;
  try {
    Bucket bucket=getBucketAndCheckAuthorization(request);
    bucketName=request.getBucket();
    response=(DeleteBucketCorsResponseType)request.getReply();
    BucketCorsManagers.getInstance().deleteCorsRules(bucket.getBucketUuid());
  }
 catch (  S3Exception s3e) {
    LOG.warn("Caught S3Exception while deleting the CORS configuration for bucket <" + bucketName + ">, CorrelationId: "+ Contexts.lookup().getCorrelationId()+ ", responding to client with: ",s3e);
    throw s3e;
  }
catch (  Exception ex) {
    LOG.warn("Caught general exception while deleting the CORS configuration for bucket <" + bucketName + ">, CorrelationId: "+ Contexts.lookup().getCorrelationId()+ ", responding to client with 500 InternalError because of: ",ex);
    throw new InternalErrorException(bucketName,ex);
  }
  return response;
}

{
  if (this.inFlightMessage.get()) {
    if (e != null && e.getCause() != null) {
      LOG.debug(e.getCause(),e.getCause());
    }
 else {
      Exception ex=new RuntimeException("Exception even has a null-valued cause.");
      LOG.error(ex,ex);
    }
    this.clearPending(ctx.getChannel());
  }
}

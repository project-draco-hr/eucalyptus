{
  try {
    if (channelFuture.isSuccess()) {
      channel=channelFuture.getChannel();
      channelWriteFuture=channelFuture.getChannel().write(request);
    }
 else {
      LOG.debug(channelFuture.getCause(),channelFuture.getCause());
    }
  }
 catch (  Exception e) {
    LOG.debug(e,e);
  }
}

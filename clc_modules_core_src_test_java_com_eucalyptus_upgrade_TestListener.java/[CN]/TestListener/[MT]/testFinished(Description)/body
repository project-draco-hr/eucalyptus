{
  try {
    timerMap.put(key(description),System.currentTimeMillis() - timerMap.get(key(description)));
    if (!failedMap.containsKey(key(description))) {
      LOG.info("PASSED " + key(description));
    }
    passedMap.put(key(description),description);
  }
 catch (  Throwable e) {
    LOG.error(e,e);
  }
}

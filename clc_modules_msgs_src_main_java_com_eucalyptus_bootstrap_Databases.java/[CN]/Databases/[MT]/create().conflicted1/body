{
  try {
    if (getLockFile().createNewFile()) {
      LOG.debug(this.getLockName() + ": The " + this.getLockFile().getAbsolutePath()+ " file was created.");
    }
  }
 catch (  IOException e) {
    LOG.debug("Unable to create the " + this.getLockFile().getAbsolutePath() + " file: "+ e.getMessage());
  }
}

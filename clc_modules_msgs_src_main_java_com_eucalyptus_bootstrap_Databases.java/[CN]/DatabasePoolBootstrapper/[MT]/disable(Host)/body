{
  if (!host.hasDatabase()) {
    return;
  }
 else {
    if (host.isLocalHost()) {
      syncState.set(SyncState.DESYNCING);
      try {
        runDbStateChange(DeactivateHostFunction.INSTANCE.apply(host));
        syncState.set(SyncState.NOTSYNCED);
      }
 catch (      Exception ex) {
        LOG.error(ex);
        Logs.exhaust().error(ex,ex);
        syncState.set(SyncState.NOTSYNCED);
      }
    }
 else {
      runDbStateChange(DeactivateHostFunction.INSTANCE.apply(host));
    }
  }
}

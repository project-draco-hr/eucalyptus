{
  for (  String ctx : PersistenceContexts.list()) {
    try {
      DriverDatabaseClusterMBean db=lookup(ctx);
      for (      String host : db.getActiveDatabases()) {
        if (Hosts.lookup(host) == null) {
          disable(host);
        }
      }
    }
 catch (    NoSuchElementException ex) {
      LOG.error(ex,ex);
    }
    return;
  }
}

{
  if (!Internets.testLocal(hostName)) {
    try {
      runDbStateChange(LivenessCheckHostFunction.INSTANCE.apply(hostName));
      return true;
    }
 catch (    Exception ex) {
      LOG.error(ex);
      Logs.extreme().error(ex,ex);
      return disable(hostName);
    }
  }
 else {
    try {
      runDbStateChange(LivenessCheckHostFunction.INSTANCE.apply(hostName));
      return true;
    }
 catch (    Exception ex) {
      LOG.error(ex);
      Logs.extreme().error(ex,ex);
      return false;
    }
  }
}

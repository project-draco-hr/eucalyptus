def get_all_launch_configurations(self, configuration_names=None, max_records=None, next_token=None):
    params = {}
    if configuration_names:
        self.__build_list_params__(params, configuration_names, 'LaunchConfigurationNames.member.%d')
    if max_records:
        params['MaxRecords'] = max_records
    if next_token:
        params['NextToken'] = next_token
    return self.__make_scale_request__('DescribeLaunchConfigurations', params)

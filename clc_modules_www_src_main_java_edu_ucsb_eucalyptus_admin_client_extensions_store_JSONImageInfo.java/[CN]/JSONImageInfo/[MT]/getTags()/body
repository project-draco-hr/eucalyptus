{
  ArrayList<String> tags=new ArrayList<String>();
  JSONValue value=object.get("tags");
  if (value != null) {
    JSONArray array=value.isArray();
    if (array != null) {
      for (int i=0; i != array.size(); i++) {
        String item=JSONUtil.asString(array.get(i));
        if (item != null) {
          tags.add(item);
        }
      }
    }
  }
  return tags;
}

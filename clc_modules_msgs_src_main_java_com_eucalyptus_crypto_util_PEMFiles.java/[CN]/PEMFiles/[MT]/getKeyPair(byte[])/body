{
  KeyPair keyPair=null;
  ByteArrayInputStream pemByteIn=new ByteArrayInputStream(o);
  try (PEMParser in=new PEMParser(new InputStreamReader(pemByteIn))){
    keyPair=new JcaPEMKeyConverter().setProvider(BouncyCastleProvider.PROVIDER_NAME).getKeyPair((PEMKeyPair)in.readObject());
  }
 catch (  IOException e) {
    LOG.error(e,e);
  }
  return keyPair;
}

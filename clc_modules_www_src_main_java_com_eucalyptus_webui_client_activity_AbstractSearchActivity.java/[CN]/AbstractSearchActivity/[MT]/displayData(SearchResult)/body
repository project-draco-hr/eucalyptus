{
  if (result != null) {
    LOG.log(Level.INFO,"Received " + result);
    cache.update(result);
    showView(result);
  }
 else {
    ErrorSinkView errorView=this.clientFactory.getErrorSinkView();
    errorView.setMessage("Search '" + search + "' failed.");
    container.setWidget(errorView);
  }
}

{
  try {
    scalingActivities.update(activity.getOwner(),activity.getActivityId(),new Callback<ScalingActivity>(){
      @Override public void fire(      final ScalingActivity input){
        input.setActivityStatusCode(status);
        input.setEndTime(new Date());
      }
    }
);
  }
 catch (  AutoScalingMetadataException e) {
    logger.error(e,e);
  }
}

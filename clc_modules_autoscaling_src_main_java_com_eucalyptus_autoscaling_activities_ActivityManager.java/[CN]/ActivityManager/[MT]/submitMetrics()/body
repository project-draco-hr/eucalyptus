{
  try {
    for (    final AutoScalingGroup group : autoScalingGroups.listRequiringMonitoring(10000L)) {
      if (!group.getEnabledMetrics().isEmpty()) {
        final List<AutoScalingInstance> groupInstances=Sets.intersection(group.getEnabledMetrics(),instanceMetrics).isEmpty() ? Collections.<AutoScalingInstance>emptyList() : autoScalingInstances.listByGroup(group);
        runTask(new MetricsSubmissionScalingProcessTask(group,groupInstances));
      }
    }
  }
 catch (  Exception e) {
    logger.error(e,e);
  }
}

{
  try {
    final AutoScalingInstance instance=autoScalingInstances.lookup(getOwner(),instanceId);
    autoScalingInstances.delete(instance);
    terminated=true;
  }
 catch (  AutoScalingMetadataNotFoundException e) {
    terminated=true;
  }
catch (  AutoScalingMetadataException e) {
    logger.error(e,e);
  }
  setActivityFinalStatus(terminated ? ActivityStatusCode.Successful : ActivityStatusCode.Failed);
}

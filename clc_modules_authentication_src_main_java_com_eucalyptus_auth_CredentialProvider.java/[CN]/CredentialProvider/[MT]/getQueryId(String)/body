{
  String queryId=null;
  EntityWrapper<User> db=Credentials.getEntityWrapper();
  User searchUser=new User(userName,true);
  try {
    User user=db.getUnique(searchUser);
    queryId=user.getQueryId();
    db.commit();
  }
 catch (  Throwable e) {
    db.rollback();
    throw new GeneralSecurityException(e);
  }
  return queryId;
}

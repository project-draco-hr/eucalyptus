{
  User user=null;
  EntityWrapper<User> db=Credentials.getEntityWrapper();
  User searchUser=new User(userName,true);
  try {
    user=db.getUnique(searchUser);
    db.commit();
  }
 catch (  EucalyptusCloudException e) {
    db.rollback();
    throw new NoSuchUserException(e);
  }
  return user;
}

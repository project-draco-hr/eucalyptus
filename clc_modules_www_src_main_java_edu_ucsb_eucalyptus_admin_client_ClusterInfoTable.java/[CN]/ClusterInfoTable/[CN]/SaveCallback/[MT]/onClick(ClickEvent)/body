{
  this.parent.statusLabel.setText("Saving...");
  this.parent.statusLabel.setStyleName("euca-greeting-pending");
  EucalyptusWebBackend.App.getInstance().setClusterList(this.parent.sessionId,this.parent.clusterList,this);
  EucalyptusWebBackend.App.getInstance().setSystemConfig(this.parent.sessionId,this.parent.systemConfig,this);
  EucalyptusWebBackend.App.getInstance().setStorageList(this.parent.sessionId,this.parent.storageList,this);
}

{
  try {
    Groups.lookupGroup("all");
  }
 catch (  NoSuchGroupException e) {
    try {
      Groups.addGroup("all");
    }
 catch (    GroupExistsException e1) {
      LOG.error(e1,e1);
      LOG.error("Failed to add the 'all' group.  The system may not be able to store group information.");
    }
  }
}

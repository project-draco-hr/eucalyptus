{
  if (!(Bootstrap.isFinished() && Topology.isEnabledLocally(Eucalyptus.class) && Topology.isEnabled(Dns.class)))   return;
  if (EventCounter++ >= 3) {
    try {
      populateRecords();
    }
 catch (    final Exception ex) {
      LOG.error("Failed to populate DNS records");
    }
    EventCounter=0;
  }
}

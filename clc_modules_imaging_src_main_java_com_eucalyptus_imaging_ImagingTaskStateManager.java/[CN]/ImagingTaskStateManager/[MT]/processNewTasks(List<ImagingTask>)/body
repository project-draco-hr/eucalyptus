{
  for (  final ImagingTask task : tasks) {
    try {
      if (task instanceof VolumeImagingTask)       processNewVolumeImagingTask((VolumeImagingTask)task);
 else       if (task instanceof InstanceImagingTask)       processNewInstanceImagingTask((InstanceImagingTask)task);
 else       throw new Exception("Invalid ImagingTask");
    }
 catch (    final Exception ex) {
      try {
        ImagingTasks.transitState(task,ImportTaskState.NEW,ImportTaskState.FAILED,"Failed to create the volume");
      }
 catch (      final Exception ex2) {
        ;
      }
      LOG.error("Failed to process new task",ex);
    }
  }
}

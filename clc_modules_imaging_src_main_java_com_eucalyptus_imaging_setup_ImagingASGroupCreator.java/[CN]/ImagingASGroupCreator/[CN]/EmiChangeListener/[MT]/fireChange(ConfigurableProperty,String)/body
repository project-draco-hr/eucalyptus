{
  try {
    if (t.getValue() != null && !t.getValue().equals(newValue)) {
      final List<ImageDetails> images=describeImages(Lists.newArrayList(newValue));
      if (images == null || images.size() <= 0)       throw new EucalyptusCloudException("No such EMI is found in the system");
      if (!images.get(0).getImageId().toLowerCase().equals(newValue.toLowerCase()))       throw new EucalyptusCloudException("No such EMI is found in the system");
    }
  }
 catch (  Exception e) {
    throw new ConfigurablePropertyException("Could not change EMI ID. " + e.getMessage());
  }
}

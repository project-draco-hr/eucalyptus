{
  try {
    HostManager.getInstance();
    LOG.info("Started membership channel " + HostManager.getMembershipGroupName());
    return true;
  }
 catch (  Exception ex) {
    LOG.fatal(ex,ex);
    BootstrapException.throwFatal("Failed to connect membership channel because of " + ex.getMessage(),ex);
    return false;
  }
}

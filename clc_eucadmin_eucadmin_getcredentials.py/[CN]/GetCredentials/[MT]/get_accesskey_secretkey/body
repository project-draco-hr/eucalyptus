def get_accesskey_secretkey(self, **args):
    self.args.update(args)
    self.process_args()
    self.setup_query()
    con1 = db.connect(host='localhost:8777', user='eucalyptus', password=self.db_pass, database='eucalyptus_auth')
    cur1 = con1.cursor()
    cur1.execute('select k.auth_access_key_query_id, k.auth_access_key_key \n                          from auth_access_key k \n                    inner join auth_user u on k.auth_access_key_owning_user=u.id \n                          join auth_group_has_users gu on u.id=gu.auth_user_id \n                          join auth_group g on gu.auth_group_id=g.id \n                          join auth_account a on g.auth_group_owning_account=a.id \n                         where a.auth_account_name=%(acctname)s and g.auth_group_name=%(grpname)s and k.auth_access_key_active=TRUE', params={'acctname': 'eucalyptus', 'grpname': '_admin', })
    result = cur1.fetchall()
    return '\t'.join(result[0])

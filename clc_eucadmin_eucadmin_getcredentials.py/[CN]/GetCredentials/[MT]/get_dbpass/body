def get_dbpass(self):

    def passphrase_callback():
        return 'eucalyptus'
    d = hashlib.sha256()
    d.update('eucalyptus')
    pk = RSA.load_key(self.cloudpk_file, passphrase_callback)
    bs = pk.sign(d.digest(), algo='sha256')
    h = hashlib.sha256()
    h.update(bs)
    self.db_pass = binascii.hexlify(h.digest())

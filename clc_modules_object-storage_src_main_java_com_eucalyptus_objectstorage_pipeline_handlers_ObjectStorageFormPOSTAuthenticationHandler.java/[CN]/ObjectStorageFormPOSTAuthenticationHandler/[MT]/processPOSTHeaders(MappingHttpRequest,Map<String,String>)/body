{
  String contentType=httpRequest.getHeader(ObjectStorageProperties.CONTENT_TYPE);
  String boundary=null;
  if (contentType != null) {
    if (contentType.startsWith(ObjectStorageProperties.MULTIFORM_DATA_TYPE)) {
      boundary=getFormFieldKeyName(contentType,"boundary");
      boundary="--" + boundary + "\r\n";
      this.boundary=boundary;
    }
    String operationPath=httpRequest.getServicePath().replaceAll(ObjectStorageProperties.objectStorageServicePath,"");
    String[] target=OSGUtil.getTarget(operationPath);
    formFields.put(ObjectStorageProperties.FormField.bucket.toString(),target[0]);
    return boundary;
  }
 else {
    throw new AuthenticationException("No Content-Type specified");
  }
}

{
  for (  com.eucalyptus.component.Component c : Components.list()) {
    if (c.isAvailableLocally()) {
      try {
        if (!EucaKeyStore.getCleanInstance().containsEntry(c.getName())) {
          return false;
        }
      }
 catch (      Exception e) {
        return false;
      }
    }
  }
  return true;
}

{
  EntityWrapper<ReportingInstanceUsageEvent> entityWrapper=EntityWrapper.get(ReportingInstanceUsageEvent.class);
  try {
    ReportingInstanceUsageEvent event=new ReportingInstanceUsageEvent(uuid,timestampMs,cumulativeNetIoMegs,cumulativeDiskIoMegs,cpuUtilizationPercent);
    entityWrapper.add(event);
    entityWrapper.commit();
    LOG.debug("Added event to db:" + event);
  }
 catch (  Exception ex) {
    LOG.error(ex);
    entityWrapper.rollback();
    throw new RuntimeException(ex);
  }
}

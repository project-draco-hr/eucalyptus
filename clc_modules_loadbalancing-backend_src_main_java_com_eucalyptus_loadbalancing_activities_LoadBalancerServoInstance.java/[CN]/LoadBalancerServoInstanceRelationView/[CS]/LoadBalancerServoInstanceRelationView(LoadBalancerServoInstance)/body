{
  if (servo.zone != null)   this.zone=TypeMappers.transform(servo.zone,LoadBalancerZoneCoreView.class);
  if (servo.security_group != null)   this.security_group=TypeMappers.transform(servo.security_group,LoadBalancerSecurityGroupCoreView.class);
  if (servo.dns != null)   this.dns=TypeMappers.transform(servo.dns,LoadBalancerDnsRecordCoreView.class);
  if (servo.autoscaling_group != null)   this.autoscaling_group=TypeMappers.transform(servo.autoscaling_group,LoadBalancerAutoScalingGroupCoreView.class);
}

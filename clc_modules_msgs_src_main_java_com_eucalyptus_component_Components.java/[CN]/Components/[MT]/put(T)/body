{
  Map<String,T> infoMap=lookup(componentInfo.getClass());
  if (infoMap.containsKey(componentInfo.getName())) {
    throw BootstrapException.throwFatal("Failed bootstrapping component registry.  Duplicate information for component '" + componentInfo.getName() + "': "+ componentInfo.getClass().getSimpleName()+ " as "+ getRealType(componentInfo.getClass()));
  }
 else {
    infoMap.put(componentInfo.getName(),componentInfo);
  }
}

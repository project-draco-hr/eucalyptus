{
  if (!contains(type,name)) {
    try {
      Components.create(name,null);
      return Components.lookup(type,name);
    }
 catch (    ServiceRegistrationException ex) {
      throw new NoSuchElementException("Missing entry for component '" + name + "' info type: "+ type.getSimpleName()+ " ("+ getRealType(type).getCanonicalName());
    }
  }
 else {
    return (T)Components.lookup(type).get(name);
  }
}

{
  log.debug("owner id:" + key.getOwnerId());
  UserFullName userFullName=UserFullName.getInstance(key.getOwnerId());
switch (criterion) {
case ACCOUNT:
    return userFullName.getName();
case USER:
  return userFullName.getUserName();
case CLUSTER:
return key.getClusterName();
case AVAILABILITY_ZONE:
return key.getAvailabilityZone();
default :
return key.getOwnerId();
}
}

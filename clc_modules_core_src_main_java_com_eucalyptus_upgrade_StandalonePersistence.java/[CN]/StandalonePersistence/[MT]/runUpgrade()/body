{
  Collections.sort(upgradeScripts);
  LOG.info(upgradeScripts);
  for (  UpgradeScript up : upgradeScripts) {
    up.setLogger(LOG);
    up.upgrade(oldLibDir,newLibDir);
  }
  LOG.info("=============================");
  LOG.info("= DATABASE UPGRADE COMPLETE =");
  LOG.info("=============================");
}

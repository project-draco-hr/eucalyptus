{
  List<Class> classList=ServiceJarDiscovery.classesInDir(new File(BaseDirectory.LIB.toString()));
  PersistenceContextDiscovery ctxDisc=new PersistenceContextDiscovery();
  for (  Class c : classList) {
    try {
      ctxDisc.checkClass(c);
    }
 catch (    Throwable t) {
      LOG.debug(t,t);
    }
  }
}

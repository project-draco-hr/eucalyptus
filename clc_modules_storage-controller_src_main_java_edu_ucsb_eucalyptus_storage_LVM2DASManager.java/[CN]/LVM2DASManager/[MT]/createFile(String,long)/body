{
  if (!LVM2DASManager.zeroFillVolumes)   return SystemUtil.run(new String[]{eucaHome + EUCA_ROOT_WRAPPER,"dd","if=/dev/zero","of=" + fileName,"count=1","bs=" + blockSize,"seek=" + (size - 1)});
 else   return SystemUtil.run(new String[]{eucaHome + EUCA_ROOT_WRAPPER,"dd","if=/dev/zero","of=" + fileName,"count=" + size,"bs=" + blockSize});
}

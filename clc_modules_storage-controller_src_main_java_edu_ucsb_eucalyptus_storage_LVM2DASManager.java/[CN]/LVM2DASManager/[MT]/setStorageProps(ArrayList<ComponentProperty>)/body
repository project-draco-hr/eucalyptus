{
  for (  ComponentProperty param : storageParams) {
    if ("Host".equals(param.getKey()))     StorageProperties.NAME=param.getValue();
 else     if ("Interface".equals(param.getKey()))     StorageProperties.iface=param.getValue();
 else     if ("Volumes Path".equals(param.getKey()))     StorageProperties.storageRootDirectory=param.getValue();
 else     if ("DAS Device".equals(param.getKey()))     StorageProperties.DAS_DEVICE=param.getValue();
 else     if ("Zero-fill Volumes".equals(param.getKey()))     StorageProperties.zeroFillVolumes=Boolean.parseBoolean(param.getValue());
  }
}

{
  if (DEBUG) {
    LOG.debug("<deleteEntryAttribute> " + dn + ": "+ attrs);
  }
  try {
    context.modifyAttributes(dn,DirContext.REMOVE_ATTRIBUTE,attrs);
  }
 catch (  NamingException e) {
    LOG.error(e,e);
    throw new LdapException("Can not delete entry attribute " + dn + ": "+ attrs,e);
  }
}

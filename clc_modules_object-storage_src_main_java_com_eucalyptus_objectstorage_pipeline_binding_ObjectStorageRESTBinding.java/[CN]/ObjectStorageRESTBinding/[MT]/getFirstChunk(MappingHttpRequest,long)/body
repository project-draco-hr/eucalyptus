{
  ChannelBuffer buffer=httpRequest.getContent();
  try {
    correlationId=httpRequest.getCorrelationId();
    buffer.markReaderIndex();
    byte[] read=new byte[buffer.readableBytes()];
    buffer.readBytes(read);
    return read;
  }
 catch (  Exception ex) {
    LOG.error(ex,ex);
    throw ex;
  }
}

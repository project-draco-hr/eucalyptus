{
  EntityTransaction db=Entities.get(ImageInfo.class);
  try {
    ImageInfo entity=Entities.merge(this);
    boolean ret=this.getPermissions().contains(accountId) || this.getOwner().isOwner(accountId);
    db.commit();
    return ret;
  }
 catch (  Exception ex) {
    Logs.exhaust().error(ex,ex);
    db.rollback();
    return false;
  }
}

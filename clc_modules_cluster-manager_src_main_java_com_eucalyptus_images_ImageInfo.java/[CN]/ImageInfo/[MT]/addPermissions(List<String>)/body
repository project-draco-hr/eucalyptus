{
  for (  String aid : accountIds) {
    try {
      Account account=Accounts.lookupAccountById(aid);
      LaunchPermission perm=new LaunchPermission(this,account.getAccountNumber());
      if (!getPermissions().contains(perm)) {
        getPermissions().add(perm);
      }
    }
 catch (    Exception e) {
      LOG.error(e,e);
    }
  }
}

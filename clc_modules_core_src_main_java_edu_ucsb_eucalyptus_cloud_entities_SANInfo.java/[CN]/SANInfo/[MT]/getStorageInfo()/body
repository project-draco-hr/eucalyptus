{
  EntityWrapper<SANInfo> storageDb=new EntityWrapper<SANInfo>(StorageProperties.DB_NAME);
  SANInfo conf=null;
  try {
    conf=storageDb.getUnique(new SANInfo(StorageProperties.NAME));
    storageDb.commit();
  }
 catch (  EucalyptusCloudException e) {
    LOG.warn("Failed to get storage info for: " + StorageProperties.NAME + ". Loading defaults.");
    conf=new SANInfo(StorageProperties.NAME,StorageProperties.SAN_HOST,StorageProperties.SAN_USERNAME,StorageProperties.SAN_PASSWORD);
    storageDb.add(conf);
    storageDb.commit();
  }
catch (  Throwable t) {
    LOG.error("Unable to get storage info for: " + StorageProperties.NAME);
    storageDb.rollback();
    return new SANInfo(StorageProperties.NAME,StorageProperties.SAN_HOST,StorageProperties.SAN_USERNAME,StorageProperties.SAN_PASSWORD);
  }
  return conf;
}

{
  final String userData=B64.standard.encString(String.format("%s\n%s",ImagingServiceProperties.CREDENTIALS_STR,getUserDataMap(ImagingServiceProperties.IMAGING_WORKER_NTP_SERVER,ImagingServiceProperties.IMAGING_WORKER_LOG_SERVER,ImagingServiceProperties.IMAGING_WORKER_LOG_SERVER_PORT)));
  return userData;
}

def delete_snapshot(self, snapshot_id):
    result = self.conn.delete_snapshot(snapshot_id)
    retries = 0
    while (result == False):
        if (retries == 5):
            break
        time.sleep((random.random() * (2 ** retries)))
        result = self.conn.delete_snapshot(snapshot_id)
        retries += 1
    return result

{
  for (  com.eucalyptus.component.Component c : Components.list()) {
    for (    Service s : c.lookupServices()) {
      if (s.isLocal()) {
        try {
          c.startService(s.getServiceConfiguration());
        }
 catch (        ServiceRegistrationException ex) {
          LOG.error(ex,ex);
          System.exit(123);
        }
      }
    }
  }
  return true;
}

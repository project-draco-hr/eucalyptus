{
  String directory=SubDirectory.KEYS.toString() + File.separator + name;
  File keyDir=new File(directory);
  if (!keyDir.exists()) {
    try {
      keyDir.mkdir();
      keyDir.delete();
      return true;
    }
 catch (    Exception e) {
      return false;
    }
  }
 else {
    return keyDir.canWrite();
  }
}

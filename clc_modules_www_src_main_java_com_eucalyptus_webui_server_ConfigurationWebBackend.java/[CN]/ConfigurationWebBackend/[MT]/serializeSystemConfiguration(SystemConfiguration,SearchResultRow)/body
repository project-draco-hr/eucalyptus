{
  result.addField(makeConfigId(CLOUD_NAME,CLOUD_TYPE));
  result.addField(CLOUD_NAME);
  result.addField(CLOUD_TYPE);
  result.addField(Internets.localHostAddress());
  result.addField("");
  result.addField(ImageConfiguration.getInstance().getDefaultKernelId());
  result.addField(ImageConfiguration.getInstance().getDefaultRamdiskId());
  result.addField(sysConf.getDnsDomain());
  result.addField(sysConf.getNameserver());
  result.addField(sysConf.getNameserverAddress());
  result.addField(AddressingConfiguration.getInstance().getDoDynamicPublicAddresses().toString());
  result.addField(AddressingConfiguration.getInstance().getMaxUserPublicAddresses().toString());
}

{
  result.addField(makeConfigId(CLOUD_NAME,CLOUD_TYPE));
  result.addField(CLOUD_NAME);
  result.addField(CLOUD_TYPE);
  result.addField(sysConf.getCloudHost());
  result.addField("");
  result.addField(sysConf.getDefaultKernel());
  result.addField(sysConf.getDefaultRamdisk());
  result.addField(sysConf.getDnsDomain());
  result.addField(sysConf.getNameserver());
  result.addField(sysConf.getNameserverAddress());
  result.addField(sysConf.isDoDynamicPublicAddresses().toString());
  result.addField(sysConf.getMaxUserPublicAddresses().toString());
}

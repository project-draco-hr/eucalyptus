{
  if (COMPONENT_PROPERTY_TYPE_KEY_VALUE.equals(propertyType)) {
    return Type.TEXT;
  }
 else   if (COMPONENT_PROPERTY_TYPE_KEY_VALUE_HIDDEN.equals(propertyType)) {
    return Type.HIDDEN;
  }
 else   if (COMPONENT_PROPERTY_TYPE_BOOLEAN.equals(propertyType)) {
    return Type.BOOLEAN;
  }
  return Type.TEXT;
}

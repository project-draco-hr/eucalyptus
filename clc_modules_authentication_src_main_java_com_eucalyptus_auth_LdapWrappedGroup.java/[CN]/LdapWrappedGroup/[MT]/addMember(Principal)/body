{
  try {
    LdapUserGroups search=new LdapUserGroups();
    search.addGroupId(this.group.getName());
    EucaLdapHelper.addUserAttribute(principal.getName(),search);
    return true;
  }
 catch (  EntryNotFoundException e) {
    LOG.error(e,e);
  }
catch (  LdapException e) {
    LOG.error(e,e);
  }
  return false;
}

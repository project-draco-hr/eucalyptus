{
  try {
    Contexts.clear(Contexts.lookup(ctx.getChannel()));
    EventRecord.here(this.messageType.getClass(),EventClass.MESSAGE,EventType.MSG_SERVICED,"rtt-ms",Long.toString(System.currentTimeMillis() - this.openTime.get(ctx.getChannel()))).debug();
  }
 catch (  Exception e1) {
    LOG.warn("Failed to remove the channel context on connection close.",e1);
  }
}

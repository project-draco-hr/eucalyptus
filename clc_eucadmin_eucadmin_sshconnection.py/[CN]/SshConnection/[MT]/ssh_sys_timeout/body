def ssh_sys_timeout(self, chan, start, cmd):
    '\n        callback to be scheduled during ssh cmds which have timed out.\n        :param chan: - paramiko channel to be closed\n        :param start - time.time() used to calc elapsed time when this fired for debug\n        :param cmd - the command ran\n        '
    chan.close()
    elapsed = (time.time() - start)
    raise CommandTimeoutException((((('SSH Command timer fired after ' + str(int(elapsed))) + " seconds. Cmd:'") + str(cmd)) + "'"))

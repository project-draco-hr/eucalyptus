def sftp_put(self, localfilepath, remotefilepath):
    '\n        sftp transfer file from localfilepath to remote system at remotefilepath\n        :param localfilepath: path to file on local system\n        :param remotefilepath: destination path for put on remote system\n        '
    if (not self.connection._transport):
        self.refresh_connection()
    transport = self.connection._transport
    sftp = paramiko.SFTPClient.from_transport(transport)
    sftp.put(remotepath=remotefilepath, localpath=localfilepath)
    sftp.close()

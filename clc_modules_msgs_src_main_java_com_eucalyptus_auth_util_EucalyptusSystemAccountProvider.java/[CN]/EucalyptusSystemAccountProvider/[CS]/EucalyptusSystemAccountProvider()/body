{
  super(AccountIdentifiers.SYSTEM_ACCOUNT,true,ImmutableList.<SystemAccountRole>of(newSystemAccountRole("AccountAdministrator","/eucalyptus",ImmutableList.<AttachedPolicy>of(newAttachedPolicy("AccountAdministrator"))),newSystemAccountRole("InfrastructureAdministrator","/eucalyptus",ImmutableList.<AttachedPolicy>of(newAttachedPolicy("InfrastructureAdministrator"))),newSystemAccountRole("ResourceAdministrator","/eucalyptus",ImmutableList.<AttachedPolicy>of(newAttachedPolicy("ResourceAdministrator")))));
}

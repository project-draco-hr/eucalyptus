{
  Map<T,Long> rv=new HashMap<T,Long>();
  for (  T key : startTimesMs.keySet()) {
    for (    Long startMs : startTimesMs.get(key)) {
      Long endMs=endTimesMs.get(key).higher(startMs);
      if (endMs == null)       endMs=defaultEndMs;
      rv.put(key,endMs - startMs);
    }
  }
  return rv;
}

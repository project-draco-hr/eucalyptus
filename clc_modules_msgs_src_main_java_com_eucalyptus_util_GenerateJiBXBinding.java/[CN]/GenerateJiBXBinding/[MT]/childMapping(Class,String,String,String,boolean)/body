{
  append("<mapping " + (isAbstract ? "abstract=\"true\"" : "name=\"" + name + "\"") + " extends=\""+ extendsName+ "\" class=\""+ className+ "\" >");
  indent++;
  append("<structure map-as=\"" + extendsName + "\"/>");
  for (  Field f : clazz.getDeclaredFields()) {
    processField(f);
  }
  indent--;
  append("</mapping>");
}

{
  String bindingName=ns.replaceAll("(http://)|(/$)","").replaceAll("[./-]","_");
  append("<binding xmlns:euca=\"" + ns + "\" name=\""+ bindingName+ "\">");
  indent++;
  append("<namespace uri=\"" + ns + "\" default=\"elements\" prefix=\"euca\"/>");
  for (  Class clazz : classList) {
    processClass(clazz);
  }
  indent--;
  append("</binding>");
}

def __sanitize_and_copy__(self, dict):
    try:
        ret = copy.copy(dict)
        for key in ret.keys():
            if (key in self.FIELD_WHITELIST):
                continue
            if isinstance(ret[key], basestring):
                ret[key] = self.codec.encode(self.IMMUNE_HTML, ret[key])
        return ret
    except Exception as e:
        logging.error(e)

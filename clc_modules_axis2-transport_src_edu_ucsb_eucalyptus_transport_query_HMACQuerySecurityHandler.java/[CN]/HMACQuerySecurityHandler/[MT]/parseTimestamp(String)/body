{
  boolean found=false;
  Calendar ts=Calendar.getInstance();
  for (  SimpleDateFormat tsFormat : iso8601) {
    try {
      found=true;
      ts.setTime(tsFormat.parse(timestamp));
      break;
    }
 catch (    ParseException e) {
    }
  }
  if (!found)   throw new QuerySecurityException("Invalid timestamp format.");
  return ts;
}

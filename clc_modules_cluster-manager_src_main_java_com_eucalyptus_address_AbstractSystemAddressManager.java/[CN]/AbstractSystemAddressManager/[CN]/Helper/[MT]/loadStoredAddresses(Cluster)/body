{
  final Address clusterAddr=new Address();
  clusterAddr.setCluster(cluster.getPartition());
  final EntityTransaction db=Entities.get(Address.class);
  try {
    for (    Address addr : Entities.query(clusterAddr)) {
      try {
        addr.init();
      }
 catch (      Exception ex) {
        LOG.error(ex,ex);
      }
    }
    db.commit();
  }
 catch (  final Exception e) {
    LOG.debug(e,e);
    db.rollback();
  }
}

{
  StringBuilder sb=new StringBuilder();
  if (groupIds != null) {
    sb.append("(|");
    for (    String groupId : groupIds) {
      if (groupId != null) {
        sb.append("(").append(GROUP_ENTITY_FIELD_ATTRIBUTE_NAME).append("=").append(groupId).append(")");
      }
    }
    sb.append(")");
  }
  return sb.toString();
}

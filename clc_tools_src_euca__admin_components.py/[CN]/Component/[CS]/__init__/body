def __init__(self, component_name=None, host_name=None, port=None, partition=None, state=None):
    self.component_name = component_name
    self.host_name = host_name
    self.port = port
    self.partition = partition
    self.state = state
    self.euca = EucaAdmin(path=SERVICE_PATH)
    self.verbose = False

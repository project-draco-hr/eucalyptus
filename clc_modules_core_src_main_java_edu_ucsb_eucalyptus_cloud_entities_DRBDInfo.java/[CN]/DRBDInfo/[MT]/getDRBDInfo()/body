{
  EntityWrapper<DRBDInfo> db=new EntityWrapper<DRBDInfo>(WalrusProperties.DB_NAME);
  DRBDInfo drbdInfo;
  try {
    drbdInfo=db.getUnique(new DRBDInfo());
  }
 catch (  EucalyptusCloudException ex) {
    drbdInfo=new DRBDInfo(WalrusProperties.NAME,"/dev/unknown");
    db.add(drbdInfo);
  }
 finally {
    db.commit();
  }
  return drbdInfo;
}

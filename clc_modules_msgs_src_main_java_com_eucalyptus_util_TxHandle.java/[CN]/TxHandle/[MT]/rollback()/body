{
  try {
    if (this.delegate != null && this.delegate.isActive()) {
      this.delegate.rollback();
    }
  }
 catch (  Throwable e) {
    LOG.trace(LogUtil.dumpObject(Thread.currentThread().getStackTrace()),e);
  }
 finally {
    this.session=null;
    outstanding.remove(this.txUuid);
    if (this.em != null) {
      this.em.close();
    }
  }
}

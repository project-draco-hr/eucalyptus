{
  final ImportToDownloadManifestUrl mapping=new ImportToDownloadManifestUrl(importManifestUrl,downloadManifestUrl);
  mapping.setParentTask(this);
  try (final TransactionResource db=Entities.transactionFor(ImagingTask.class)){
    final ImagingTask entity=Entities.merge(this);
    entity.getDownloadManifestUrl().add(mapping);
    db.commit();
  }
 }

{
  Context context=null;
  try {
    context=Contexts.lookup();
    context.getContracts().putAll(policyEngine.evaluateAuthorization(resourceType,resourceName,accountId,context.getRequest(),context.getUser()));
    return true;
  }
 catch (  AuthException e) {
    LOG.debug("Resource access to " + resourceType.getCanonicalName() + ":"+ resourceName+ " of "+ accountId+ " for "+ context.getUser(),e);
    return false;
  }
}

def authorize_security_group(self, name=None, src_security_group_name=[], src_security_group_owner_id=[], ip_protocol=[], from_port=[], to_port=[], cidr_ip=[], group_id=[], src_security_group_group_id=[], callback=None):
    self.groups.expireCache()
    Threads.instance().runThread(self.__authorize_security_group_cb__, ({'name': name, 'src_security_group_name': src_security_group_name, 'src_security_group_owner_id': src_security_group_owner_id, 'ip_protocol': ip_protocol, 'from_port': from_port, 'to_port': to_port, 'cidr_ip': cidr_ip, 'group_id': group_id, 'src_security_group_group_id': src_security_group_group_id, }, callback))

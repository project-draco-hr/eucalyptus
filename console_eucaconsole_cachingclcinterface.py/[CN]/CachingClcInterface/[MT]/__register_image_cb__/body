def __register_image_cb__(self, kwargs, callback):
    try:
        ret = self.clc.register_image(kwargs['name'], kwargs['image_location'], kwargs['description'], kwargs['architecture'], kwargs['kernel_id'], kwargs['ramdisk_id'], kwargs['root_dev_name'], kwargs['block_device_map'])
        Threads.instance().invokeCallback(callback, Response(data=ret))
    except Exception as ex:
        Threads.instance().invokeCallback(callback, Response(error=ex))

{
  EntityTransaction db=Entities.get(ExtantNetwork.class);
  try {
    this.extantNet=Entities.merge(this.extantNet);
    NetworkGroup net=this.extantNet.getNetworkGroup();
    net.setState(NetworkGroup.State.ACTIVE);
    Entities.merge(net);
    db.commit();
  }
 catch (  Exception ex) {
    LOG.error(ex,ex);
    db.rollback();
  }
}

{
  this.networkGroup=networkGroup;
  Integer ttag=-1;
  try {
    ttag=networkGroup.extantNetwork().getTag();
  }
 catch (  NotEnoughResourcesAvailable ex) {
    Logs.extreme().error(ex,ex);
  }
  this.tag=ttag;
  StartNetworkType msg=new StartNetworkType(networkGroup.getOwnerUserId(),this.tag,networkGroup.getNaturalId(),networkGroup.getNaturalId());
  this.setRequest(msg);
}

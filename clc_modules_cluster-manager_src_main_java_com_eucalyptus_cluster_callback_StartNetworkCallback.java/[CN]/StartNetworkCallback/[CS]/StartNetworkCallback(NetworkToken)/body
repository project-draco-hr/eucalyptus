{
  this.networkToken=networkToken;
  StartNetworkType msg=new StartNetworkType(networkToken.getAccountId(),networkToken.getVlan(),networkToken.getNetworkName(),networkToken.getNetworkUuid());
  msg.setUser(FakePrincipals.SYSTEM_USER);
  msg.setUserId(networkToken.getAccountId());
  this.setRequest(msg);
}

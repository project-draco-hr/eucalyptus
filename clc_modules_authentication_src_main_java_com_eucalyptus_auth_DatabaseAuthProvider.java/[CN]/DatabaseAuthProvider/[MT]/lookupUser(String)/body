{
  EntityWrapper<UserEntity> db=Authentication.getEntityWrapper();
  UserEntity searchUser=new UserEntity(userName);
  UserEntity user=null;
  try {
    user=db.getUnique(searchUser);
    db.commit();
  }
 catch (  Throwable e) {
    db.rollback();
    throw new NoSuchUserException(e);
  }
  return user;
}

{
  EntityWrapper<UserGroupEntity> db=Groups.getEntityWrapper();
  UserGroupEntity delGroup=new UserGroupEntity(groupName);
  try {
    UserGroupEntity g=db.getUnique(delGroup);
    db.delete(g);
    db.commit();
  }
 catch (  Throwable t) {
    db.rollback();
    throw new NoSuchGroupException(t);
  }
}

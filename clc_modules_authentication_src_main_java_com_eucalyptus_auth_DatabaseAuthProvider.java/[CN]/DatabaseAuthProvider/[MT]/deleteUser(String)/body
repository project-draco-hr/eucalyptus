{
  UserEntity user=new UserEntity(userName);
  EntityWrapper<User> db=Authentication.getEntityWrapper();
  try {
    User foundUser=db.getUnique(user);
    db.delete(foundUser);
    db.commit();
    return foundUser;
  }
 catch (  Exception e) {
    db.rollback();
    throw new NoSuchUserException(e);
  }
}

{
  EntityWrapper<GroupEntity> db=Authentication.getEntityWrapper();
  GroupEntity newGroup=new GroupEntity(groupName);
  try {
    db.add(newGroup);
    db.commit();
  }
 catch (  Throwable t) {
    db.rollback();
    throw new GroupExistsException(t);
  }
  return DatabaseWrappedGroup.newInstance(newGroup);
}

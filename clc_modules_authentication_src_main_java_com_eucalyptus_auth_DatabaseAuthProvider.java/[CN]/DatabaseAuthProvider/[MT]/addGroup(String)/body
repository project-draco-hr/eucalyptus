{
  EntityWrapper<UserGroupEntity> db=Groups.getEntityWrapper();
  UserGroupEntity newGroup=new UserGroupEntity(groupName);
  try {
    db.add(newGroup);
    db.commit();
  }
 catch (  Throwable t) {
    db.rollback();
    throw new GroupExistsException(t);
  }
  return new DatabaseWrappedGroup(newGroup);
}

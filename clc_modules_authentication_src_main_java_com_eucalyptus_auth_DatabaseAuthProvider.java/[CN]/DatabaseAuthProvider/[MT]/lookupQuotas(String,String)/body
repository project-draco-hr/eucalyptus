{
  EntityWrapper<AuthorizationEntity> db=EntityWrapper.get(AuthorizationEntity.class);
  Session session=db.getSession();
  try {
    @SuppressWarnings("unchecked") List<AuthorizationEntity> authorizations=(List<AuthorizationEntity>)session.createCriteria(AuthorizationEntity.class).setCacheable(true).add(Restrictions.eq("effect",EffectType.Limit)).createCriteria("statement").setCacheable(true).createCriteria("policy").setCacheable(true).createCriteria("group").setCacheable(true).createCriteria("users").add(Restrictions.idEq(userId)).list();
    db.commit();
    List<DatabaseAuthorizationProxy> results=Lists.newArrayList();
    for (    AuthorizationEntity auth : authorizations) {
      results.add(new DatabaseAuthorizationProxy(auth));
    }
    return results;
  }
 catch (  Throwable e) {
    db.rollback();
    Debugging.logError(LOG,e,"Failed to lookup quotas for user with ID " + userId + ", type="+ resourceType);
    throw new AuthException("Failed to lookup quota",e);
  }
}

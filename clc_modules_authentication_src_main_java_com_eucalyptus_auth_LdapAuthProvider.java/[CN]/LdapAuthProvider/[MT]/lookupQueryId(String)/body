{
  try {
    UserEntity searchUser=new UserEntity();
    searchUser.setQueryId(queryId);
    return EucaLdapHelper.getUsers(searchUser,null).get(0);
  }
 catch (  EntryNotFoundException e) {
    LOG.error(e,e);
    throw new NoSuchUserException(e);
  }
catch (  LdapException e) {
    LOG.error(e,e);
  }
  return null;
}

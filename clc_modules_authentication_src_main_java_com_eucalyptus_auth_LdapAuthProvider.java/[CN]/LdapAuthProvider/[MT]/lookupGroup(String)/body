{
  try {
    GroupEntity group=new GroupEntity();
    group.setName(groupName);
    return EucaLdapHelper.getGroups(group).get(0);
  }
 catch (  EntryNotFoundException e) {
    LOG.error(e,e);
    throw new NoSuchGroupException(e);
  }
catch (  LdapException e) {
    LOG.error(e,e);
  }
  return null;
}

{
  try {
    if (userTx != null) {
      UserEntity user=new UserEntity(userName);
      userTx.fire(user);
      EucaLdapHelper.updateUser(user,null);
    }
  }
 catch (  EntryNotFoundException e) {
    LOG.error(e,e);
    throw new NoSuchUserException(e);
  }
catch (  LdapException e) {
    LOG.error(e,e);
  }
catch (  Throwable e) {
    LOG.error(e,e);
  }
}

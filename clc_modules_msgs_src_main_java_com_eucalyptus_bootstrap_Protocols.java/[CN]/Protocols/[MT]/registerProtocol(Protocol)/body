{
  if (ClassConfigurator.getProtocolId(p.getClass()) == 0) {
    ClassConfigurator.addProtocol(++PROTOCOL_ID,p.getClass());
  }
  return "euca-" + (p.getClass().isAnonymousClass() ? p.getClass().getSuperclass().getSimpleName().toLowerCase() : p.getClass().getSimpleName().toLowerCase()) + "-protocol";
}

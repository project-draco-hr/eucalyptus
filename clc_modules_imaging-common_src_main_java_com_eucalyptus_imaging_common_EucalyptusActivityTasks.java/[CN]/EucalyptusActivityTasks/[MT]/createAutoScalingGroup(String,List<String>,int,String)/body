{
  final AutoScalingCreateGroupTask task=new AutoScalingCreateGroupTask(groupName,availabilityZones,capacity,launchConfigName);
  final CheckedListenableFuture<Boolean> result=task.dispatch(new AutoScalingSystemActivity());
  try {
    if (result.get()) {
      return;
    }
 else     throw new EucalyptusActivityException("failed to create autoscaling group");
  }
 catch (  Exception ex) {
    throw Exceptions.toUndeclared(ex);
  }
}

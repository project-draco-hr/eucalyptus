{
  EntityWrapper<StorageInfo> db=new EntityWrapper<StorageInfo>();
  StorageInfo storageInfo;
  try {
    storageInfo=db.getUnique(new StorageInfo());
  }
 catch (  EucalyptusCloudException ex) {
    storageInfo=new StorageInfo(StorageProperties.NAME,StorageProperties.MAX_TOTAL_VOLUME_SIZE,StorageProperties.iface,StorageProperties.MAX_VOLUME_SIZE,StorageProperties.storageRootDirectory);
    db.add(storageInfo);
  }
 finally {
    db.commit();
  }
  return storageInfo;
}

{
  final Map<String,String> cidrMappingText=Splitter.on(CharMatcher.anyOf(",;:")).omitEmptyStrings().trimResults().withKeyValueSeparator(Splitter.on(Pattern.compile("-?>")).omitEmptyStrings().trimResults().limit(2)).split(Objects.toString(cidrMappingList,""));
  return CollectionUtils.transform(cidrMappingText,Maps.<Cidr,Cidr>newHashMap(),cidrTransform,cidrTransform);
}

{
  try {
    jettyServer.stop();
    for (int i=0; i < 10 && !jettyServer.isStopped() && jettyServer.isStopping(); i++) {
      try {
        TimeUnit.MILLISECONDS.sleep(500);
      }
 catch (      InterruptedException e) {
      }
    }
    jettyServer.destroy();
  }
 catch (  Exception e) {
    LOG.debug(e,e);
  }
  try {
    setupJettyServer();
    startJettyServer();
  }
 catch (  Exception e) {
    LOG.debug(e,e);
  }
}

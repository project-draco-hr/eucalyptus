def get_single_parser(self):
    parser = OptionParser('usage: %prog GROUPNAME', version='Eucalyptus %prog VERSION')
    (options, args) = parser.parse_args()
    if (len(args) != 1):
        print 'ERROR  Required argument GROUPNAME is missing or malformed.'
        parser.print_help()
        sys.exit(1)
    else:
        return (options, args)

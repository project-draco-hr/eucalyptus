{
  if (reply.get_return()) {
    VmInstance vm=VmInstances.getInstance().lookup(this.getRequest().getInstanceId());
    vm.removeVolumeAttachment(this.getRequest().getVolumeId());
  }
 else {
    VmInstance vm=VmInstances.getInstance().lookup(this.getRequest().getInstanceId());
    vm.updateVolumeAttachment(this.getRequest().getVolumeId(),"attached");
  }
}

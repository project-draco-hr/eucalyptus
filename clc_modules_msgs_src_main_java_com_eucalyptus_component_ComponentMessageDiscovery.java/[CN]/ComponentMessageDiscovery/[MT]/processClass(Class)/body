{
  if (BaseMessage.class.isAssignableFrom(candidate) && Ats.from(candidate).has(ComponentMessage.class) && !Modifier.isAbstract(candidate.getModifiers())&& !Modifier.isInterface(candidate.getModifiers())) {
    try {
      ComponentMessages.register(candidate);
    }
 catch (    Exception ex) {
      LOG.error(ex,ex);
    }
    return true;
  }
 else {
    return false;
  }
}

{
  final EuareException cause=Exceptions.findCause(e,EuareException.class);
  if (cause != null) {
    throw cause;
  }
  logger.error(e,e);
  final EuareException exception=new EuareException(HttpResponseStatus.INTERNAL_SERVER_ERROR,"InternalError",String.valueOf(e.getMessage()));
  if (Contexts.lookup().hasAdministrativePrivileges()) {
    exception.initCause(e);
  }
  throw exception;
}

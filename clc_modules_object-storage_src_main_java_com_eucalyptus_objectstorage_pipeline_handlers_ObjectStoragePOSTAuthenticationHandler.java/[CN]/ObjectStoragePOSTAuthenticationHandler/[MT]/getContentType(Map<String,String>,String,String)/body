{
  int endValue=part.indexOf("\r\n\r\n");
  int startValue=part.indexOf(ObjectStorageProperties.CONTENT_TYPE + ":") + ObjectStorageProperties.CONTENT_TYPE.length() + 1;
  if (endValue > startValue) {
    String contentType=part.substring(startValue,endValue);
    return contentType;
  }
  return null;
}

{
  if (!VmState.TERMINATED.equals(vm.getState()) && !VmState.BURIED.equals(vm.getState())) {
    SystemState.returnPublicAddress(vm);
    SystemState.returnNetworkIndex(vm);
  }
  try {
    Clusters.sendClusterEvent(vm.getPlacement(),QueuedEvent.make(new TerminateCallback(),Admin.makeMsg(TerminateInstancesType.class,vm.getInstanceId())));
    try {
    }
 catch (    Exception e) {
    }
  }
 catch (  Exception e) {
    LOG.debug(e);
  }
}

{
  try {
    if (ComponentIds.lookup(Eucalyptus.class).name().equals(userId) || "admin".equals(userId) || Contexts.lookup().getUser().isSystem()) {
      return true;
    }
 else {
      return false;
    }
  }
 catch (  IllegalContextAccessException ex) {
    LOG.trace(ex,ex);
    return false;
  }
}

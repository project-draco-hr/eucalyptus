{
  Field ret=null;
  Exception e=null;
  while (!EucalyptusMessage.class.equals(clazz) || !Object.class.equals(clazz)) {
    try {
      ret=clazz.getDeclaredField(fieldName);
      return ret;
    }
 catch (    Exception e1) {
      e=e1;
    }
    clazz=clazz.getSuperclass();
  }
  if (ret == null) {
    throw e;
  }
  return ret;
}

{
  try {
    this.context=this.contextFactory.createMuleContext(this.builder);
  }
 catch (  Exception e) {
    LOG.fatal("Failed to configure services.",e);
    return false;
  }
  try {
    this.context.start();
  }
 catch (  Exception e) {
    LOG.fatal("Failed to start services.",e);
    return false;
  }
  return true;
}

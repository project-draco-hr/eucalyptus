{
  String stackName=(getStackEntity() != null && getStackEntity().getStackName() != null) ? getStackEntity().getStackName() : "UNKNOWN";
  String logicalResourceId=(getResourceInfo() != null && getResourceInfo().getLogicalResourceId() != null) ? getResourceInfo().getLogicalResourceId() : "UNKNOWN";
  return ResourceActionHelper.getDefaultPhysicalResourceId(stackName,logicalResourceId,maxLength);
}

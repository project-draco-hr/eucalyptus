def get_user_info(account=AUTH_SYS_ACCT, user=AUTH_DEFAULT_ADMIN):
    conn = MySQLdb.connect(host='127.0.0.1', user='eucalyptus', passwd=db_pass(), db='eucalyptus_auth', port=8777)
    cursor = conn.cursor()
    query = (AUTH_USER_QUERY % (account, user))
    cursor.execute(query)
    row = cursor.fetchone()
    result = list(row)
    cursor.close()
    conn.close()
    return result

def db_get(field=None):
    conn = MySQLdb.connect(host='127.0.0.1', user='eucalyptus', passwd=db_pass(), db='eucalyptus_auth', port=8777)
    cursor = conn.cursor()
    cursor.execute(("select %s from auth_users where auth_user_name='admin';" % field))
    row = cursor.fetchone()
    result = row[0]
    cursor.close()
    conn.close()
    return result

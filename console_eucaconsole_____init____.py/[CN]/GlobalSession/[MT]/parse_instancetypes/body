def parse_instancetypes(self, instancetypes):
    self.instancetypes = {}
    for vmt in instancetypes:
        if isinstance(vmt, dict):
            self.instancetypes[vmt['name']] = [vmt['cores'], vmt['memory'], vmt['disk']]
        else:
            self.instancetypes[vmt.name] = [vmt.cores, vmt.memory, vmt.disk]

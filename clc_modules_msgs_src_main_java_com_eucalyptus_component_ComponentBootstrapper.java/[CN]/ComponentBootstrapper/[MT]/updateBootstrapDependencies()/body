{
  for (  Entry<Stage,Bootstrapper> entry : Lists.newArrayList(this.bootstrappers.entries())) {
    if (!entry.getValue().checkLocal()) {
      EventRecord.here(Bootstrap.class,EventType.BOOTSTRAPPER_SKIPPED,"stage:" + Bootstrap.getCurrentStage(),this.getClass().getSimpleName(),"Depends.local=" + entry.getValue().toString(),"Component." + entry.getValue().toString() + "=remote").info();
      this.bootstrappers.remove(entry.getKey(),entry.getValue());
    }
 else     if (!entry.getValue().checkRemote()) {
      EventRecord.here(Bootstrap.class,EventType.BOOTSTRAPPER_SKIPPED,"stage:" + Bootstrap.getCurrentStage(),this.getClass().getSimpleName(),"Depends.remote=" + entry.getValue().toString(),"Component." + entry.getValue().toString() + "=local").info();
      this.bootstrappers.remove(entry.getKey(),entry.getValue());
    }
  }
}

{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  DBPoolInfo other=(DBPoolInfo)obj;
  if (alias == null) {
    if (other.alias != null)     return false;
  }
 else   if (!alias.equals(other.alias))   return false;
  return true;
}

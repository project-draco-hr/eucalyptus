{
  R reply=null;
  try {
    reply=this.future.get();
    if (reply == null) {
      this.LOG.warn("Application of callback resulted in null value: " + this.getClass().getSimpleName());
      Exceptions.eat("Callback marked as done has null valued response: " + reply);
    }
    try {
      this.LOG.trace(EventRecord.here(this.getClass(),EventType.CALLBACK,"fire(" + reply.getClass().getSimpleName() + ")").toString());
      this.callback.fire(reply);
    }
 catch (    Throwable ex) {
      this.LOG.error(EventRecord.here(this.getClass(),EventType.CALLBACK,"FAILED","fire(" + reply.getClass().getSimpleName() + ")",ex.getMessage()).toString(),ex);
      this.doFail(ex);
    }
  }
 catch (  Throwable e) {
    this.LOG.error(EventRecord.here(this.getClass(),EventType.FUTURE,"FAILED","get()",e.getMessage()).toString(),Exceptions.filterStackTrace(e));
    this.doFail(e);
  }
}

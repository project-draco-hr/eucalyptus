{
  if ("FULL_CONTROL".equals(permission)) {
    setFullControl();
  }
 else   if ("READ".equals(permission)) {
    setCanRead(true);
  }
 else   if ("WRITE".equals(permission)) {
    setCanWrite(true);
  }
 else   if ("READ_ACP".equals(permission)) {
    setCanReadACP(true);
  }
 else   if ("WRITE_ACP".equals(permission)) {
    setCanWriteACP(true);
  }
}

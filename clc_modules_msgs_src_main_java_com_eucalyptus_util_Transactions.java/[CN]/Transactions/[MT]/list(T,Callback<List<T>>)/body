{
  assertThat(search,notNullValue());
  EntityWrapper<T> db=EntityWrapper.get(search);
  dbtl.set(db);
  try {
    List<T> res=db.query(search);
    c.fire(res);
    db.commit();
    return res;
  }
 catch (  Throwable e) {
    db.rollback();
    LOG.error(e,e);
  }
 finally {
    dbtl.remove();
  }
  return Lists.newArrayList();
}

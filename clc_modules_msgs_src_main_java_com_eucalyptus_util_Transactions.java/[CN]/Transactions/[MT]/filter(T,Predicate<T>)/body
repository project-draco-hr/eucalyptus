{
  assertThat(search,notNullValue());
  List<T> res=Lists.newArrayList();
  EntityWrapper<T> db=EntityWrapper.get(search);
  dbtl.set(db);
  try {
    List<T> queryResults=db.query(search);
    for (    T t : queryResults) {
      if (c.apply(t)) {
        res.add(t);
      }
    }
    db.commit();
  }
 catch (  Throwable e) {
    db.rollback();
    LOG.error(e,e);
  }
 finally {
    dbtl.remove();
  }
  return res;
}

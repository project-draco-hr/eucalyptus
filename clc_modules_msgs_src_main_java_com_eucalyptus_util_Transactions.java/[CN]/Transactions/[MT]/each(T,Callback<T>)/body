{
  assertThat(search,notNullValue());
  assertThat(c,notNullValue());
  EntityWrapper<T> db=Transactions.joinOrCreate(search);
  try {
    List<T> res=db.query(search);
    for (    T t : res) {
      c.fire(t);
    }
    db.commit();
    return res;
  }
 catch (  UndeclaredThrowableException e) {
    db.rollback();
    LOG.error(e,e);
  }
catch (  Throwable e) {
    db.rollback();
    LOG.error(e,e);
  }
 finally {
    dbtl.remove();
  }
  return Lists.newArrayList();
}

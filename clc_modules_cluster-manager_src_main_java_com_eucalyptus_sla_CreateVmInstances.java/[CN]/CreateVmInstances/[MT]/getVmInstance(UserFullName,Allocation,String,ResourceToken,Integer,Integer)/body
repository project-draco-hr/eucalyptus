{
  VmInstance vmInst=new VmInstance(userFullName,VmInstances.getId(allocInfo.getReservationIndex(),index),token.getInstanceUuids().get(index),reservationId,index,token.getCluster(),allocInfo.getUserData(),allocInfo.getKeyInfo(),allocInfo.getVmTypeInfo(),allocInfo.getBootSet().getMachine().getPlatform().name(),allocInfo.getNetworks(),networkIndex.toString());
  return vmInst;
}

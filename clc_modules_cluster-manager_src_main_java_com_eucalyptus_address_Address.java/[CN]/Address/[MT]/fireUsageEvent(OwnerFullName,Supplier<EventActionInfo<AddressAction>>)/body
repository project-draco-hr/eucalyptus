{
  if (!Principals.isFakeIdentityAccountNumber(ownerFullName.getAccountNumber())) {
    try {
      ListenerRegistry.getInstance().fireEvent(AddressEvent.with(getNaturalId(),getDisplayName(),ownerFullName,Accounts.lookupAccountById(ownerFullName.getAccountNumber()).getName(),actionInfoSupplier.get()));
    }
 catch (    final Exception e) {
      LOG.error(e,e);
    }
  }
}

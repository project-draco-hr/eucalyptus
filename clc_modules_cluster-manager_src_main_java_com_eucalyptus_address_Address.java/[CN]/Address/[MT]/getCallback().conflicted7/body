{
  try {
    Class cbClass=this.transition.getName().getCallback();
    Constructor cbCons=cbClass.getConstructor(Address.class);
    return (RemoteCallback)cbCons.newInstance(this);
  }
 catch (  Exception ex) {
    LOG.error(ex);
    Logs.extreme().error(ex,ex);
    try {
      this.clearPending();
    }
 catch (    Exception ex1) {
    }
    return new NOOP();
  }
}

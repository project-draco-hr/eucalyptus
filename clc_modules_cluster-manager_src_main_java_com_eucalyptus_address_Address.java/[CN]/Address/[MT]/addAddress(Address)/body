{
  EntityWrapper<Address> db=new EntityWrapper<Address>();
  try {
    Address addr=db.getUnique(new Address(address.getName()));
    addr.setUserId(address.getUserId());
    db.commit();
  }
 catch (  Throwable e) {
    try {
      db.add(address);
      db.commit();
    }
 catch (    Throwable e1) {
      db.rollback();
    }
  }
}

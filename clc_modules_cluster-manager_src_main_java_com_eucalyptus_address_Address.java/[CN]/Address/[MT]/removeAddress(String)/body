{
  try {
    Addresses.getInstance().disable(ipAddress);
  }
 catch (  NoSuchElementException e1) {
    LOG.debug(e1);
  }
  EntityWrapper<Address> db=EntityWrapper.get(Address.class);
  try {
    Address dbAddr=db.getUnique(new Address(ipAddress));
    db.delete(dbAddr);
    db.commit();
  }
 catch (  Exception e) {
    Logs.extreme().error(e,e);
    db.rollback();
  }
}

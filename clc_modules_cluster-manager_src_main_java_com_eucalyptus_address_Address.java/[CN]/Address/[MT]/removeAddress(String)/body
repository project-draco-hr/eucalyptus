{
  try {
    Addresses.getInstance().disable(name);
  }
 catch (  NoSuchElementException e1) {
    LOG.debug(e1);
  }
  EntityWrapper<Address> db=EntityWrapper.get(Address.class);
  try {
    Address dbAddr=db.getUnique(new Address(name));
    db.delete(dbAddr);
    db.commit();
  }
 catch (  Throwable e) {
    db.rollback();
  }
}

{
  EucalyptusWebBackend.App.getInstance().approveUsers(sessionId,userNames,new AsyncCallback<Void>(){
    public void onSuccess(    Void result){
      displaySelectedGroups();
    }
    public void onFailure(    Throwable caught){
      displaySelectedUsers();
      rootPanel.getPropertyPanel().showStatus("Approving users failed! " + getFailureMessage(caught),true);
    }
  }
);
}

{
  EucalyptusWebBackend.App.getInstance().deleteGroups(sessionId,groupNames,new AsyncCallback<Void>(){
    public void onSuccess(    Void result){
      display();
    }
    public void onFailure(    Throwable caught){
      displaySelectedUsers();
      rootPanel.getPropertyPanel().showStatus("Deleting groups failed! " + getFailureMessage(caught),true);
    }
  }
);
}

{
  EucalyptusWebBackend.App.getInstance().getZones(sessionId,new AsyncCallback<List<String>>(){
    public void onSuccess(    List<String> result){
      rootPanel.getPropertyPanel().showAddGroup(result);
    }
    public void onFailure(    Throwable caught){
      rootPanel.getPropertyPanel().showStatus("Loading zone info failed! " + getFailureMessage(caught),true);
    }
  }
);
}

{
  EucalyptusWebBackend.App.getInstance().updateUser(sessionId,user,groupNames,new AsyncCallback<Void>(){
    public void onSuccess(    Void result){
      displaySelectedGroups();
    }
    public void onFailure(    Throwable caught){
      displaySelectedUsers();
      rootPanel.getPropertyPanel().showStatus("Updating user failed! " + getFailureMessage(caught),true);
    }
  }
);
}

{
  EucalyptusWebBackend.App.getInstance().addGroup(sessionId,group,new AsyncCallback<Void>(){
    public void onSuccess(    Void result){
      display();
    }
    public void onFailure(    Throwable caught){
      displaySelectedUsers();
      rootPanel.getPropertyPanel().showStatus("Adding group failed! " + getFailureMessage(caught),true);
    }
  }
);
}

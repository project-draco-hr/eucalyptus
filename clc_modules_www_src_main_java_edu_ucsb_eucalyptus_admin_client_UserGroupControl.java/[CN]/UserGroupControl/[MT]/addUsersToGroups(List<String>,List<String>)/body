{
  EucalyptusWebBackend.App.getInstance().addUsersToGroups(sessionId,userNames,groupNames,new AsyncCallback<Void>(){
    public void onSuccess(    Void result){
      displaySelectedGroups();
    }
    public void onFailure(    Throwable caught){
      displaySelectedUsers();
      rootPanel.getPropertyPanel().showStatus("Adding users to groups failed! " + getFailureMessage(caught),true);
    }
  }
);
}

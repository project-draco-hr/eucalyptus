{
  EucalyptusWebBackend.App.getInstance().removeUsersFromGroups(sessionId,userNames,groupNames,new AsyncCallback<Void>(){
    public void onSuccess(    Void result){
      displaySelectedGroups();
    }
    public void onFailure(    Throwable caught){
      displaySelectedUsers();
      rootPanel.getPropertyPanel().showStatus("Removing users from groups failed! " + getFailureMessage(caught),true);
    }
  }
);
}

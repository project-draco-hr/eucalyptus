{
  final UserInfoWeb user=selectedUsers.get(0);
  EucalyptusWebBackend.App.getInstance().getGroupsByUser(sessionId,user.getUserName(),new AsyncCallback<List<String>>(){
    public void onSuccess(    List<String> result){
      rootPanel.getPropertyPanel().showEditUser(user,getGroupNames(),result);
    }
    public void onFailure(    Throwable caught){
      rootPanel.getPropertyPanel().showStatus("Loading user's groups failed! " + getFailureMessage(caught),true);
    }
  }
);
}

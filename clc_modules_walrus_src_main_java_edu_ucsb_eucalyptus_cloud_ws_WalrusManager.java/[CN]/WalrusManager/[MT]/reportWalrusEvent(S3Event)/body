{
  if (event != null) {
    try {
      ListenerRegistry.getInstance().fireEvent(event);
    }
 catch (    Exception ex) {
      LOG.error("Non-fatal exception: Could not send event: " + event.toString() + " Exception = "+ ex.getMessage());
    }
  }
 else {
    LOG.error("Non-fatal error: cannot report a null Walrus event.");
  }
}

{
  if (!(bucketName.matches("^[A-Za-z0-9].*") || bucketName.contains(".") || bucketName.contains("-")))   return false;
  if (bucketName.length() < 3 || bucketName.length() > 255)   return false;
  String[] addrParts=bucketName.split("\\.");
  boolean ipFormat=true;
  if (addrParts.length == 4) {
    for (    String addrPart : addrParts) {
      try {
        Integer.parseInt(addrPart);
      }
 catch (      NumberFormatException ex) {
        ipFormat=false;
        break;
      }
    }
  }
 else {
    ipFormat=false;
  }
  if (ipFormat)   return false;
  return true;
}

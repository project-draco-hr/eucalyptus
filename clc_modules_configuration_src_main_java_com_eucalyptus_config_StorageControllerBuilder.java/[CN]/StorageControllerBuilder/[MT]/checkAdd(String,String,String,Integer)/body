{
  try {
    ServiceConfigurations.getConfiguration(ClusterConfiguration.class,name);
  }
 catch (  NoSuchElementException e1) {
    throw new ServiceRegistrationException("Storage controllers may only be registered with a corresponding Cluster of the same name." + "  No cluster found with the name: " + name);
  }
  return super.checkAdd(partition,name,host,port);
}

{
  GetObjectResponseType reply=null;
  try {
    GetObjectType msg=new GetObjectType(bucketName,objectName,false,true);
    msg.regarding();
    reply=(GetObjectResponseType)ServiceDispatcher.lookupSingle(Components.lookup(ObjectStorage.class)).send(msg);
  }
 catch (  Exception e) {
    throw new EucalyptusCloudException("Failed to read manifest file: " + bucketName + "/"+ objectName,e);
  }
  Document inputSource=null;
  try {
    DocumentBuilder builder=XMLParser.getDocBuilder();
    if (builder != null) {
      inputSource=builder.parse(new ByteArrayInputStream(B64.url.dec(reply.getBase64Data())));
    }
  }
 catch (  Exception e) {
    throw new EucalyptusCloudException("Failed to read manifest file: " + bucketName + "/"+ objectName,e);
  }
  return inputSource;
}

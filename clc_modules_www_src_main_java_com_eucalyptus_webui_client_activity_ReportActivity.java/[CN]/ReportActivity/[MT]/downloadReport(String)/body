{
  if (this.sessionId == null)   return;
  final String reportUrl="https://localhost:8443/reportservlet" + "?session=" + sessionId + "&type="+ type+ "&format="+ format+ "&start="+ fromDate.getTime()+ "&end="+ toDate.getTime()+ "&criterion="+ criteria+ "&groupByCriterion="+ groupBy;
  Timer t=new Timer(){
    @Override public void run(){
      clientFactory.getReportView().loadReport(reportUrl);
    }
  }
;
  t.schedule(2000);
}

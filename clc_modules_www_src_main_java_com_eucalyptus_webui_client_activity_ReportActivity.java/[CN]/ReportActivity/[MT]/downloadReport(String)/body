{
  if (this.sessionId == null)   return;
  final String reportUrl="/reportservlet" + "?session=" + sessionId + "&type="+ type+ "&format="+ format+ "&start="+ fromDate.getTime()+ "&end="+ toDate.getTime()+ "&criterion="+ criteria+ "&groupByCriterion="+ groupBy;
  Window.open(reportUrl,"_self","");
}

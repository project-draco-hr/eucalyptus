{
  if (this.sessionId == null) {
    return;
  }
  final String reportUrl="/reportservlet" + "?session=" + sessionId + "&type="+ type+ "&format="+ format+ "&start="+ fromDate.getTime()+ "&end="+ (toDate.getTime() + (1000 * 60 * 60* 24))+ "&criterion="+ criteria+ "&groupByCriterion="+ groupBy;
  Window.open(reportUrl,"_self","");
}

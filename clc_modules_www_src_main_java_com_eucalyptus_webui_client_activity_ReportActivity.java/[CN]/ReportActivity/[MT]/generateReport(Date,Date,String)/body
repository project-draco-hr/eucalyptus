{
  final String sessionId=clientFactory.getLocalSession().getSession().getId();
  final String reportUrl="/reportservlet" + "?session=" + sessionId + "&type="+ type+ "&format=HTML"+ "&start="+ fromDate.getTime()+ "&end="+ (toDate.getTime() + (1000 * 60 * 60* 24));
  clientFactory.getReportView().loadReport(reportUrl);
  this.sessionId=sessionId;
  this.fromDate=fromDate;
  this.toDate=toDate;
  this.type=type;
}

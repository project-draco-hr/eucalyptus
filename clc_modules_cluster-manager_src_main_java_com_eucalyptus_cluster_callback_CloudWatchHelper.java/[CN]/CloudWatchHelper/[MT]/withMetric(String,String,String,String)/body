{
  return new Predicate<PutMetricDataType>(){
    private final Predicate<MetricDatum> metricDatumPredicate=Predicates.and(name == null ? Predicates.<MetricDatum>alwaysTrue() : withMetric(name),withMetricDimension(dimensionName,dimensionValue));
    @Override public boolean apply(    @Nullable final PutMetricDataType putMetricDataType){
      return putMetricDataType != null && namespace.equals(putMetricDataType.getNamespace()) && putMetricDataType.getMetricData() != null && putMetricDataType.getMetricData().getMember() != null && Iterables.tryFind(putMetricDataType.getMetricData().getMember(),metricDatumPredicate).isPresent();
    }
  }
;
}

{
  List<FieldType> fields=configurableKlasses.get(klass.getName());
  if (fields != null) {
    for (    ComponentProperty property : storageParams) {
      for (      FieldType field : fields) {
        if (field.getDisplayName().equals(property.getKey())) {
          try {
            Field f=klass.getField(field.getKey());
            try {
              if ("KEYVALUE".equals(property.getType()))               f.set(klass,property.getValue());
 else               if ("BOOLEAN".equals(property.getType()))               f.set(klass,Boolean.parseBoolean(property.getValue()));
            }
 catch (            IllegalArgumentException e) {
              LOG.error(e);
            }
catch (            IllegalAccessException e) {
              LOG.error(e);
            }
          }
 catch (          SecurityException e) {
            LOG.error(e);
          }
catch (          NoSuchFieldException e) {
            LOG.error(e);
          }
        }
      }
    }
  }
}

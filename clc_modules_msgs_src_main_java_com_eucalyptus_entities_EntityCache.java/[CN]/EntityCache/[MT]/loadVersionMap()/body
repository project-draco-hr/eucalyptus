{
  try (final TransactionResource db=Entities.transactionFor(example)){
    List<Object[]> idVersionList=(List<Object[]>)Entities.createCriteria(example.getClass()).add(Example.create(example)).setReadOnly(true).setCacheable(false).setFetchSize(1000).setProjection(Projections.projectionList().add(Projections.id()).add(Projections.property("version"))).list();
    return Lists.newArrayList(Iterables.transform(idVersionList,ObjectArrayToStringIntPair.INSTANCE));
  }
 }

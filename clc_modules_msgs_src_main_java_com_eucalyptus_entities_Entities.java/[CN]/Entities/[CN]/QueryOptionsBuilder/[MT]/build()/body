{
  return new QueryOptions(){
    @Override public MatchMode getMatchMode(){
      return matchMode;
    }
    @Override public Integer getMaxResults(){
      return maxResults;
    }
    @Override public Integer getFetchSize(){
      return fetchSize;
    }
    @Override public Boolean getCacheable(){
      return cacheable;
    }
    @Override public Boolean getReadonly(){
      return readonly;
    }
    @Override public Criterion getCriterion(){
      return criterion;
    }
  }
;
}

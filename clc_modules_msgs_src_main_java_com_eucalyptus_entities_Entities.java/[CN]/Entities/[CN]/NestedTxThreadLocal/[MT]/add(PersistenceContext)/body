{
  NestedTx tx=null;
  if (this.get().containsKey(persistenceContext.name())) {
    if (this.clearStale(persistenceContext)) {
      tx=this.addNestedTx(persistenceContext);
    }
 else {
      tx=this.get().get(persistenceContext.name());
    }
  }
 else {
    tx=this.addNestedTx(persistenceContext);
  }
  return tx;
}

{
  final CascadingTx tx=this.get().get(persistenceContext.name());
  if (!tx.isActive()) {
    try {
      tx.getTxState().doCleanup();
    }
 catch (    final Exception ex) {
      LOG.error(ex,ex);
    }
 finally {
      this.get().remove(persistenceContext.name());
    }
    return true;
  }
 else {
    return false;
  }
}

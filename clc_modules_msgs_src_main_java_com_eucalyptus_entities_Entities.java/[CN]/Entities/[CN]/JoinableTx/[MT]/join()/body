{
  return new EntityTransaction(){
    @Override public void setRollbackOnly(){
    }
    @Override public void rollback(){
      Logs.exhaust().trace("Child call to rollback() is ignored: " + Threads.currentStackRange(2,8));
    }
    @Override public boolean isActive(){
      return JoinableTx.this.isActive();
    }
    @Override public boolean getRollbackOnly(){
      return JoinableTx.this.getRollbackOnly();
    }
    @Override public void commit(){
      JoinableTx.this.getTxState().getEntityManager().flush();
    }
    @Override public void begin(){
    }
  }
;
}

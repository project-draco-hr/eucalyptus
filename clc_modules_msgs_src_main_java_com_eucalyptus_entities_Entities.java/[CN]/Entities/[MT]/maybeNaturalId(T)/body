{
  final String natId=((HasNaturalId)example).getNaturalId();
  @SuppressWarnings("unchecked") final T ret=(T)Entities.getTransaction(example).getTxState().getSession().byNaturalId(example.getClass()).using("naturalId",natId).load();
  if (ret == null) {
    throw new NoSuchElementException("@NaturalId: " + natId);
  }
  return ret;
}

{
  Object id=resolvePrimaryKey(example);
  if (id == null) {
    return null;
  }
 else {
    final T res=(T)Entities.getTransactionState(example).getEntityManager().find(example.getClass(),id);
    if (res == null) {
      throw new NoSuchElementException("@Id: " + id);
    }
 else {
      return res;
    }
  }
}

{
  @SuppressWarnings("unchecked") final T ret=(T)Entities.getTransaction(example).getTxState().getSession().createCriteria(example.getClass()).add(Example.create(example).enableLike(MatchMode.EXACT)).setCacheable(true).setMaxResults(1).setFetchSize(1).setFirstResult(0).uniqueResult();
  if (ret == null) {
    throw new NoSuchElementException("example: " + LogUtil.dumpObject(example));
  }
  return ret;
}

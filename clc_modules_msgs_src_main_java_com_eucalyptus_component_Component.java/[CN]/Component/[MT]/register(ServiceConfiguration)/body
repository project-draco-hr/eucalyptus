{
  Service service=Services.newServiceInstance(config);
  if (config.isLocal() || Internets.testLocal(config.getHostName())) {
    this.localService.set(service);
  }
  this.services.put(config.getFullName(),service);
  EventRecord.caller(Component.class,EventType.COMPONENT_SERVICE_REGISTERED,Component.this.getName(),service.getServiceConfiguration().isLocal() ? "local" : "remote",config.getName(),config.getUri()).info();
  return service;
}

{
  return new Callable<Component>(){
    @Override public Component call() throws Exception {
      try {
        transitionFuture.set(Component.this.enableService(configuration).get());
      }
 catch (      Throwable ex) {
        transitionFuture.setException(ex);
        LOG.error(ex);
      }
      return Component.this;
    }
  }
;
}

{
  if (service.getServiceConfiguration().isLocal()) {
    this.localService.set(service);
  }
  this.services.put(service.getName(),service);
  Components.register(service);
  EventRecord.caller(Component.class,EventType.COMPONENT_SERVICE_REGISTERED,this.getName(),service.getServiceConfiguration().isLocal() ? "local" : "remote",service.getName(),service.getUri(),service.getDispatcher()).info();
  return service;
}

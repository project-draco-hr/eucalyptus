{
  return new Callback.Success<Component>(){
    @Override public void fire(    Component t){
      if (t.isAvailableLocally()) {
        try {
          t.stateMachine.transition(Transition.this);
        }
 catch (        Throwable ex) {
          LOG.error(ex,ex);
        }
      }
    }
  }
;
}

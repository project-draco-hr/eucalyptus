{
  try {
    if (service.isLocal()) {
      EventRecord.caller(Component.class,EventType.COMPONENT_SERVICE_START,this.getName(),service.getName(),service.getUri().toString()).info();
      this.builder.fireStart(service);
    }
 else {
      EventRecord.caller(Component.class,EventType.COMPONENT_SERVICE_START_REMOTE,this.getName(),service.getName(),service.getUri().toString()).info();
    }
  }
 catch (  ServiceRegistrationException e) {
    LOG.debug(e,e);
  }
}

{
  if (service.getServiceConfiguration().isLocal()) {
    this.localService.set(service);
  }
  this.services.put(service.getFullName(),service);
  EventRecord.caller(Component.class,EventType.COMPONENT_SERVICE_REGISTERED,Component.this.getName(),service.getServiceConfiguration().isLocal() ? "local" : "remote",service.getName(),service.getUri(),service.getDispatcher()).info();
}

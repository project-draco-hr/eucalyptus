{
  EventRecord rec=EventRecord.caller(Component.class,EventType.COMPONENT_INFO,this.getName(),"enabled").append(this.lifecycle.isEnabled().toString(),"local",this.lifecycle.isLocal().toString()).next().append("uri",this.lifecycle.getUri().toString(),"host",this.lifecycle.getHost(),"port").append(Integer.toString(this.lifecycle.getPort())).next().append(this.configuration.toString().replaceAll("=|(, )",""));
  for (  Bootstrapper b : this.configuration.getBootstrappers()) {
    rec.next().append(EventRecord.caller(Component.class,EventType.COMPONENT_INFO,this.getName(),b.getClass().getSimpleName()));
  }
  return rec.toString();
}

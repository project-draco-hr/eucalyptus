{
  this(Internets.localHostIdentifier(),Hosts.getLocalGroupAddress(),Internets.localHostInetAddress(),ImmutableList.copyOf(Ordering.from(Internets.INET_ADDRESS_COMPARATOR).sortedCopy(Internets.getAllInetAddresses())),BootstrapArgs.isCloudController(),Databases.isSynchronized(),Bootstrap.isFinished(),Hosts.getStartTime(),Topology.epoch(),Hosts.getDBStatus());
}

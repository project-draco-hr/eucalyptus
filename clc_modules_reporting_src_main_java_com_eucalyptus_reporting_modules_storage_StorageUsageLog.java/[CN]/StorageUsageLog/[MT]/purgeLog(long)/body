{
  log.info(String.format("purge earlierThan:%d ",earlierThanMs));
  EntityWrapper<StorageUsageSnapshot> entityWrapper=EntityWrapper.get(StorageUsageSnapshot.class);
  try {
    entityWrapper.createSQLQuery("DELETE FROM storage_usage_snapshot " + "WHERE timestamp_ms < ?").setLong(0,new Long(earlierThanMs)).executeUpdate();
    entityWrapper.commit();
  }
 catch (  Exception ex) {
    log.error(ex);
    entityWrapper.rollback();
    throw new RuntimeException(ex);
  }
}

{
  if (lastUsageData != null) {
    currentSummary.addVolumesMegsSecs(lastUsageData.getVolumesMegs() * durationSecs);
    currentSummary.addSnapshotsMegsSecs(lastUsageData.getSnapshotsMegs() * durationSecs);
    currentSummary.setVolumesMegsMax(Math.max(currentSummary.getVolumesMegsMax(),lastUsageData.getVolumesMegs()));
    currentSummary.setSnapshotsMegsMax(Math.max(currentSummary.getSnapshotsMegsMax(),lastUsageData.getSnapshotsMegs()));
    log.info("Accumulate " + (lastUsageData.getVolumesMegs() * durationSecs) + " "+ (lastUsageData.getSnapshotsMegs() * durationSecs));
    this.lastUsageData=null;
  }
}

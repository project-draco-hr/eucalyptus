{
  if (this.runtimeState.isMarked() && this.getRuntimeState().ordinal() > VmState.RUNNING.ordinal()) {
    this.runtimeState.set(this.getRuntimeState(),false);
    VmInstances.cleanUp(this);
    return true;
  }
 else {
    this.runtimeState.set(this.getRuntimeState(),false);
    return false;
  }
}

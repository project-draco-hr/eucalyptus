{
  EventRecord.here(VmInstance.class,EventClass.VM,EventType.VM_STATE).withDetails(this.getOwnerId(),this.getInstanceId(),"type",this.getVmTypeInfo().getName()).withDetails("state",this.state.getReference().name()).withDetails("cluster",this.placement).withDetails("image",this.imageInfo.getImageId()).withDetails("started",this.launchTime.getTime() + "").info();
}

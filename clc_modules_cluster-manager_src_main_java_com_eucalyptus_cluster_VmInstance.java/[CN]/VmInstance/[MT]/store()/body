{
  try {
    ListenerRegistry.getInstance().fireEvent(new InstanceEvent(this.getInstanceUuid(),this.getDisplayName(),this.vmType.getName(),this.getOwner().getNamespace(),this.getOwner().getName(),this.clusterName,this.partitionName,this.networkBytes,this.blockBytes));
  }
 catch (  EventFailedException ex) {
    LOG.error(ex,ex);
  }
}

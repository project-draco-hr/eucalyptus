{
  try {
    if ("false".equals(newValue.toLowerCase()) && "true".equals(t.getValue())) {
      ImagingServiceLaunchers.getInstance().disable();
    }
 else     if ("true".equals(newValue.toLowerCase()) && "false".equals(t.getValue())) {
      ImagingServiceLaunchers.getInstance().enable();
    }
 else     if (!("true".equals(newValue.toLowerCase()) || "false".equals(newValue.toLowerCase())))     throw new ConfigurablePropertyException("The '" + newValue + "' is not a valid value.");
  }
 catch (  final ConfigurablePropertyException ex) {
    throw ex;
  }
catch (  final Exception e) {
    throw new ConfigurablePropertyException("Could disable/enable imaging service workers due to: " + e.getMessage(),e);
  }
}

{
  boolean hasAll=false;
  boolean hasDefault=false;
  for (  GroupInfoWeb gi : groups) {
    if (gi.name.equals(GROUP_ALL)) {
      hasAll=true;
    }
 else     if (gi.name.equals(GROUP_DEFAULT)) {
      hasDefault=true;
    }
  }
  if (!hasAll) {
    groups.add(getGroupAll());
  }
  if (!hasDefault) {
    groups.add(getGroupDefault());
  }
}

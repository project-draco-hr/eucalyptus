{
  EntityWrapper<SystemConfiguration> db=new EntityWrapper<SystemConfiguration>();
  SystemConfiguration sysConf=EucalyptusProperties.getSystemConfiguration();
  return new SystemConfigWeb(sysConf.getDefaultKernel(),sysConf.getDefaultRamdisk(),sysConf.getMaxUserPublicAddresses(),sysConf.isDoDynamicPublicAddresses(),sysConf.getSystemReservedPublicAddresses(),sysConf.getDnsDomain(),sysConf.getNameserver(),sysConf.getNameserverAddress(),sysConf.getCloudHost());
}

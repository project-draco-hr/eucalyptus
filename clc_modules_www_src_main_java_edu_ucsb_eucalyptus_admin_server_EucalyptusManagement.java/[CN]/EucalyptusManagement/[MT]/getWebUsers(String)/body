{
  final List<UserInfoWeb> webUsersList=Lists.newArrayList();
  for (  User u : Users.listAllUsers()) {
    try {
      UserInfo userInfo=((WrappedUser)u).getUserInfo();
      webUsersList.add(Composites.composeNew(UserInfoWeb.class,userInfo,u));
    }
 catch (    Exception e) {
      LOG.debug(e,e);
    }
  }
  return webUsersList;
}

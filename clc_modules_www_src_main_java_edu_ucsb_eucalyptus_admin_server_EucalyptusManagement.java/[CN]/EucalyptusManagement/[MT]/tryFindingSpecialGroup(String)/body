{
  if (GROUP_ALL.equals(name)) {
    try {
      Groups.lookupGroup(name);
    }
 catch (    NoSuchGroupException nge) {
      return getGroupAll();
    }
  }
 else   if (GROUP_DEFAULT.equals(name)) {
    try {
      Groups.lookupGroup(name);
    }
 catch (    NoSuchGroupException nge) {
      return getGroupDefault();
    }
  }
  return null;
}

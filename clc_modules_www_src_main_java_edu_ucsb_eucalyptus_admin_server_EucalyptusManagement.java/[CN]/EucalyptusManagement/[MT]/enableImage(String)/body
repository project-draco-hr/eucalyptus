{
  try {
    new Images._byId(imageId){
{
        new _mutator(){
          @Override public void set(          ImageInfo e){
            e.setImageState("available");
          }
        }
.set();
      }
    }
;
  }
 catch (  EucalyptusCloudException e) {
    throw EucalyptusManagement.makeFault("Specified image was not found, sorry.");
  }
}

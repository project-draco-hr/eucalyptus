def endElement(self, name, value, connection):
    if (name == 'euca:detail'):
        self.detail = value
    elif (name == 'euca:state'):
        self.state = value
    elif (name == 'euca:hostName'):
        self.host_name = value
    elif (name == 'euca:name'):
        self.storage_name = value
    elif (name == 'euca:partition'):
        self.partition = value
    else:
        setattr(self, name, value)

{
  try {
    List<Bucket> bucketList=BucketMetadataManagers.getInstance().lookupBucketsByUser(userId);
    long size=0;
    for (    Bucket b : bucketList) {
      size+=b.getBucketSize();
    }
    return size;
  }
 catch (  Exception e) {
    throw new AuthException("Failed to search bucket",e);
  }
}

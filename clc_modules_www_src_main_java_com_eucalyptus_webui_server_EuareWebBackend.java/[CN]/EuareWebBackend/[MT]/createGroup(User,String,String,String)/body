{
  try {
    Account account=Accounts.lookupAccountById(accountId);
    Group group=Privileged.createGroup(requestUser,account,name,path);
    return group.getName();
  }
 catch (  Exception e) {
    LOG.error("Failed to create group " + name + " in "+ accountId);
    LOG.debug(e,e);
  }
  return null;
}

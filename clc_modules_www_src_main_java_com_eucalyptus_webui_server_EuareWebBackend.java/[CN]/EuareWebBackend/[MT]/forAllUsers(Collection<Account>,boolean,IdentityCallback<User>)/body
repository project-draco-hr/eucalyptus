{
  final Function<Collection<String>,List<User>> lister=new Function<Collection<String>,List<User>>(){
    @Override public List<User> apply(    final Collection<String> userIds){
      try {
        return Accounts.listUsersForAccounts(userIds,eager);
      }
 catch (      final AuthException e) {
        throw Exceptions.toUndeclared(e);
      }
    }
  }
;
  forAll(accounts,callback,Accounts.toUserAccountNumber(),Accounts.toUserId(),lister,userCountSupplier);
}

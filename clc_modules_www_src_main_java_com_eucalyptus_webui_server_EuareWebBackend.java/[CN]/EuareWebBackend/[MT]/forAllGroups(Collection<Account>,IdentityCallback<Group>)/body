{
  final Function<Collection<String>,List<Group>> lister=new Function<Collection<String>,List<Group>>(){
    @Override public List<Group> apply(    final Collection<String> groupIds){
      try {
        return Accounts.listGroupsForAccounts(groupIds);
      }
 catch (      final AuthException e) {
        throw Exceptions.toUndeclared(e);
      }
    }
  }
;
  forAll(accounts,callback,Accounts.toGroupAccountNumber(),Accounts.toGroupId(),lister,groupCountSupplier);
}

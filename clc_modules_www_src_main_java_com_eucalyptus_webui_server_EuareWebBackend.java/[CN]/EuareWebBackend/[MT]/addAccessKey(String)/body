{
  try {
    User user=Accounts.lookupUserById(userId);
    user.createKey();
  }
 catch (  Exception e) {
    LOG.error("Failed to create key for user " + userId,e);
    LOG.debug(e,e);
    throw new EucalyptusServiceException("Failed to create key for user " + userId + ": "+ e.getMessage());
  }
}

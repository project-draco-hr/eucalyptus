{
  try {
    Account account=Accounts.lookupAccountById(accountId);
    User user=Privileged.createUser(authContext(requestUser),account,name,path);
    return user.getName();
  }
 catch (  Exception e) {
    LOG.error("Failed to create user " + name + " in "+ accountId);
    LOG.debug(e,e);
  }
  return null;
}

{
  if (CloudMetadatas.isMachineImageIdentifier(IMAGING_WORKER_EMI)) {
    return true;
  }
 else {
    LOG.debug("Imaging service EMI property is unset.  \"\n" + "              + \"Use euca-modify-property -p imaging.imaging_emi=<imaging service emi> \"\n" + "              + \"where the emi should point to the image provided in the eucalyptus-TBD package.\" ");
    return false;
  }
}

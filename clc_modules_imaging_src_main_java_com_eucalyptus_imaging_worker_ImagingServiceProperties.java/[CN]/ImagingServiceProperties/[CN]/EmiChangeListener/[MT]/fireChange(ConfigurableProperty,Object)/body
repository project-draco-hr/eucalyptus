{
  try {
    if (newValue instanceof String) {
      if (t.getValue() != null && !t.getValue().equals(newValue) && ((String)newValue).length() > 0)       onPropertyChange((String)newValue,null,null,null);
    }
  }
 catch (  final Exception e) {
    throw new ConfigurablePropertyException("Could not change EMI ID",e);
  }
}

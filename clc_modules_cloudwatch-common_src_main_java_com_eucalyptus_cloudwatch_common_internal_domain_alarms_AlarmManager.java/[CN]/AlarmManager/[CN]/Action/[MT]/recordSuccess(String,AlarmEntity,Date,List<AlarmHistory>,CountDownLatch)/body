{
  JSONObject historyDataJSON=getSuccessJSON(actionARN,alarmEntity);
  String historyData=historyDataJSON.toString();
  historyList.add(AlarmManager.createAlarmHistoryItem(alarmEntity.getAccountId(),alarmEntity.getAlarmName(),historyData,HistoryItemType.Action," Successfully executed action " + actionARN,now));
  actionCountDownLatch.countDown();
  ;
}

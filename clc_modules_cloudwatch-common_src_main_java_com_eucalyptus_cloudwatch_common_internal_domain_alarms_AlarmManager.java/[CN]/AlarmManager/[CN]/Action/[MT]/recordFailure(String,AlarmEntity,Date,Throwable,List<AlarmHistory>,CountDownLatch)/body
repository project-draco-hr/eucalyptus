{
  JSONObject historyDataJSON=getFailureJSON(actionARN,alarmEntity,cause);
  String historyData=historyDataJSON.toString();
  historyList.add(AlarmManager.createAlarmHistoryItem(alarmEntity.getAccountId(),alarmEntity.getAlarmName(),historyData,HistoryItemType.Action," Failed to execute action " + actionARN,now));
  actionCountDownLatch.countDown();
  ;
}

{
  if (null == nodes || nodes.length < 2)   return;
  if (!(nodes[0] instanceof AnnotationNode))   return;
  if (!(nodes[1] instanceof PackageNode))   return;
  for (  ClassNode cNode : sourceUnit.getAST().getClasses()) {
    if (cNode.getDeclaredField("$EUCA_CLASS_UUID") == null) {
      final FieldNode field=new FieldNode("$EUCA_CLASS_UUID",Opcodes.ACC_PRIVATE | Opcodes.ACC_STATIC | Opcodes.ACC_FINAL| Opcodes.ACC_SYNTHETIC,new ClassNode(String.class),new ClassNode(cNode.getClass()),new ConstantExpression(UUID.randomUUID().toString()));
      cNode.addField(field);
    }
  }
}

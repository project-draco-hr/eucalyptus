{
  final EucaCreateTagsTask task=new EucaCreateTagsTask(tagKey,tagValue,resources);
  final CheckedListenableFuture<Boolean> result=task.dispatch(new EucalyptusSystemActivity());
  try {
    if (result.get()) {
      return;
    }
 else     throw new EucalyptusActivityException("failed to create tags");
  }
 catch (  Exception ex) {
    throw Exceptions.toUndeclared(ex);
  }
}

{
  final String instanceId=addr.getInstanceId();
  try {
    final VmInstance vm=VmInstances.getInstance().lookup(instanceId);
    return new SuccessCallback(){
      public void apply(      Object response){
        addr.release();
        Addresses.system(vm);
      }
    }
;
  }
 catch (  NoSuchElementException e) {
    return new SuccessCallback(){
      public void apply(      Object response){
        addr.release();
      }
    }
;
  }
}

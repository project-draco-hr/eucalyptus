{
  try {
    addr.clearPending();
  }
 catch (  IllegalStateException e1) {
    LOG.debug(e1,e1);
  }
 finally {
    try {
      if (addr.isAssigned()) {
        SuccessCallback release=getReleaseCallback(addr);
        AddressCategory.unassign(addr).onSuccess(release).dispatch(addr.getCluster());
      }
 else {
        addr.release();
      }
    }
 catch (    IllegalStateException e) {
      LOG.debug(e,e);
    }
  }
}

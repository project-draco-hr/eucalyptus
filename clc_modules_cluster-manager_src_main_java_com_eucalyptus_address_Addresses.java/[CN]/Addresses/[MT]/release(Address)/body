{
  try {
    addr.clearPending();
  }
 catch (  IllegalStateException e1) {
    LOG.debug(e1,e1);
  }
 finally {
    try {
      if (addr.isAssigned()) {
        SuccessCallback unassign=getReleaseCallback(addr);
        AddressCategory.unassign(addr).onSuccess(unassign).dispatch(addr.getCluster());
      }
    }
 catch (    IllegalStateException e) {
      LOG.debug(e,e);
    }
  }
}

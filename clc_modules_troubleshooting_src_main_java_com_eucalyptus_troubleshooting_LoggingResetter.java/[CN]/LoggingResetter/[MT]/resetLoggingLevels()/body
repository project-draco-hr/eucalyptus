{
synchronized (LoggingResetter.class) {
    LOG.info("Resetting log levels to " + System.getProperty("euca.log.level"));
    resetLogLevel(LogManager.getRootLogger(),System.getProperty("euca.log.level"));
    Enumeration e=LogManager.getCurrentLoggers();
    while (e.hasMoreElements()) {
      resetLogLevel((Logger)e.nextElement(),System.getProperty("euca.log.level"));
    }
    LOG.info("Finished resetting log levels");
  }
}

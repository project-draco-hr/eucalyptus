{
  LOG.debug("Initializing SSL just in case: " + SslSetup.class);
  try {
    DatabaseConfig.initialize();
  }
 catch (  Exception e) {
    LOG.debug(e,e);
  }
  this.db=new Server();
  this.db.setProperties(new HsqlProperties(DatabaseConfig.getProperties()));
  SystemBootstrapper.makeSystemThread(this).start();
  return true;
}

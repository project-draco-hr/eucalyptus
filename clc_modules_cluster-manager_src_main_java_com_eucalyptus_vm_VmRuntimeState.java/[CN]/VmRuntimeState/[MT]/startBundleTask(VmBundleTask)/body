{
  if (this.bundleTask == null) {
    this.bundleTask=task;
    return true;
  }
 else {
    if ((this.getBundleTask() != null) && (BundleState.failed.equals(task) || BundleState.canceling.equals(task))) {
      this.resetBundleTask();
      this.bundleTask=task;
      return true;
    }
 else {
      return false;
    }
  }
}

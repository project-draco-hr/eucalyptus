{
  if (this.getBundleTask() != null) {
    this.getBundleTask().setState(BundleState.none);
    EventRecord.here(VmRuntimeState.class,EventType.BUNDLE_RESTART,this.vmInstance.getOwner().toString(),this.getBundleTask().getBundleId(),this.getVmInstance().getInstanceId(),"" + this.getBundleTask().getState()).info();
    return true;
  }
  return false;
}

{
  if (this.getBundleTask() != null && this.getBundleTask().getState().ordinal() >= BundleState.storing.ordinal()) {
    this.getBundleTask().setState(BundleState.storing);
    EventRecord.here(VmRuntimeState.class,EventType.BUNDLE_STARTING,this.vmInstance.getOwner().toString(),this.getBundleTask().getBundleId(),this.getVmInstance().getInstanceId(),"" + this.getBundleTask().getState()).info();
    return true;
  }
 else   if (BundleState.canceling.equals(this.getBundleTaskState())) {
    EventRecord.here(VmRuntimeState.class,EventType.BUNDLE_CANCELLED,this.vmInstance.getOwner().toString(),this.getBundleTask().getBundleId(),this.getVmInstance().getInstanceId(),"" + this.getBundleTask().getState()).info();
    this.resetBundleTask();
    return true;
  }
 else {
    return false;
  }
}

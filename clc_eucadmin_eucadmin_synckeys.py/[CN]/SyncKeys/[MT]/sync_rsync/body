def sync_rsync(self):
    if (not self.use_rsync):
        return
    print 
    print ('Trying rsync to sync keys with %s' % self.remote_host)
    cmd = 'rsync -az '
    cmd += ' '.join(self.files)
    cmd += (' %s@%s:%s' % (self.remote_user, self.remote_host, self.dst_dir))
    cmd = Command(cmd)
    if (cmd.status == 0):
        print 'done'
        return True
    else:
        print 'failed.'
        return False

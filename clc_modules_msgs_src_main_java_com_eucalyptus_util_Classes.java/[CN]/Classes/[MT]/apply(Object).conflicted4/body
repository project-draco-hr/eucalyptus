{
  final List<Class<?>> ret=Lists.newArrayList();
  final Class<?> type=WhateverAsClass.INSTANCE.apply(input);
  if (type == Object.class) {
    return ret;
  }
 else   if (type.isInterface()) {
    return ret;
  }
 else {
    ret.add(type);
    final List<Class<?>> next=this.apply(type.getSuperclass());
    ret.addAll(next);
    return ret;
  }
}

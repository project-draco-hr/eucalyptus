{
  LOG.debug(msg.getObject() + " [" + msg.getSrc()+ "]");
  if (!Bootstrap.isFinished()) {
    List<Host> dbHosts=(List<Host>)msg.getObject();
    for (    Host dbHost : dbHosts) {
      if (Eucalyptus.setupLocals(dbHost.getHostAddress())) {
        HostManager.this.view.markReady();
      }
    }
  }
}

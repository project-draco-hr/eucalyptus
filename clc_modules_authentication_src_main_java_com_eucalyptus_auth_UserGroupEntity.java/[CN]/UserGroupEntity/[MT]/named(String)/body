{
  EntityWrapper<UserGroupEntity> db=new EntityWrapper<UserGroupEntity>();
  UserGroupEntity userGroup=null;
  try {
    userGroup=db.getUnique(new UserGroupEntity(name));
    db.commit();
  }
 catch (  Exception e) {
    db.rollback();
    throw new EucalyptusCloudException(e);
  }
  return userGroup;
}

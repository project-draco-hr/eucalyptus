{
  ThreadFactory ret=null;
  if (factories.containsKey(clusterName)) {
    ret=factories.get(clusterName);
  }
 else {
synchronized (ClusterThreadFactory.class) {
      if (!factories.containsKey(clusterName)) {
        factories.put(clusterName,new ClusterThreadFactory(clusterName));
      }
      ret=factories.get(clusterName);
    }
  }
  return ret;
}

{
  try {
    return (T)getGroovyEngine().eval(code);
  }
 catch (  Exception e) {
    Logs.exhaust().debug(e,e);
    throw new ScriptExecutionFailedException("Executing the requested script failed:\n" + "============================\n" + code + "============================\n"+ "\nbecause of:\n"+ Exceptions.causeString(e),e);
  }
}

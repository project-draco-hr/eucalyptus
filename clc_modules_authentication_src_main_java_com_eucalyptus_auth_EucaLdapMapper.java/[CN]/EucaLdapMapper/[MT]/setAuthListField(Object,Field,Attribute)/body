{
  try {
    List<BaseAuthorization> perms=(List<BaseAuthorization>)getField(object,field);
    if (perms == null) {
      setField(object,field,(Object)(new ArrayList<BaseAuthorization>()));
    }
    toAuthListField(attr,perms);
  }

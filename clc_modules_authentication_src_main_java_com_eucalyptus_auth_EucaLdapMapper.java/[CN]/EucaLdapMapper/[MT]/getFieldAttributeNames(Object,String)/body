{
  try {
    if (object != null) {
      Class clazz=classType(object);
      Field field=clazz.getDeclaredField(fieldName);
      Ldap annotation=field.getAnnotation(Ldap.class);
{
        if (annotation != null) {
          return annotation.names();
        }
      }
    }
  }
 catch (  Exception e) {
    LOG.error(e,e);
  }
  return null;
}

{
  EntityWrapper<ReportingVolumeCreateEvent> entityWrapper=EntityWrapper.get(ReportingVolumeCreateEvent.class);
  try {
    ReportingVolumeCreateEvent volume=new ReportingVolumeCreateEvent(uuid,volumeId,timestampMs,userId,clusterName,availabilityZone,sizeGB);
    entityWrapper.add(volume);
    entityWrapper.commit();
    LOG.debug("Added reporting volume to db:" + volume);
  }
 catch (  Exception ex) {
    LOG.error(ex);
    entityWrapper.rollback();
    throw new RuntimeException(ex);
  }
}

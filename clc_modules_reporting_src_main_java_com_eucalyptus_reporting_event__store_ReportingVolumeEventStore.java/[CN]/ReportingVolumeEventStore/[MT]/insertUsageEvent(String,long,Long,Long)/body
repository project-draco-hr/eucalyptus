{
  EntityWrapper<ReportingVolumeUsageEvent> entityWrapper=EntityWrapper.get(ReportingVolumeUsageEvent.class);
  try {
    ReportingVolumeUsageEvent event=new ReportingVolumeUsageEvent(uuid,timestampMs,cumulativeMegsRead,cumulativeMegsWritten);
    entityWrapper.add(event);
    entityWrapper.commit();
    LOG.debug("Added event to db:" + event);
  }
 catch (  Exception ex) {
    LOG.error(ex);
    entityWrapper.rollback();
    throw new RuntimeException(ex);
  }
}

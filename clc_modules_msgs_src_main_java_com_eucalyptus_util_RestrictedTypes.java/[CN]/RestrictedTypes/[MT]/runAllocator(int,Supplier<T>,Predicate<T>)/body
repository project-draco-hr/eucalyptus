{
  List<T> res=Lists.newArrayList();
  try {
    for (int i=0; i < quantity; i++) {
      res.add(allocator.get());
    }
  }
 catch (  Exception ex) {
    for (    T rsc : res) {
      try {
        rollback.apply(rsc);
      }
 catch (      Exception ex1) {
        LOG.trace(ex1,ex1);
      }
    }
  }
  return res;
}

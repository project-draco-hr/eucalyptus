{
  Map<String,Map<String,S3UsageSummary>> summaryMap=usageLog.scanSummarize(period,groupByCrit,crit);
  List<S3DisplayBean> items=new ArrayList<S3DisplayBean>(summaryMap.size());
  for (  String outerKey : summaryMap.keySet()) {
    Map<String,S3UsageSummary> innerMap=summaryMap.get(outerKey);
    for (    String innerKey : innerMap.keySet()) {
      items.add(new S3DisplayBean(innerKey,outerKey,innerMap.get(innerKey),displayUnits));
    }
  }
  return items;
}

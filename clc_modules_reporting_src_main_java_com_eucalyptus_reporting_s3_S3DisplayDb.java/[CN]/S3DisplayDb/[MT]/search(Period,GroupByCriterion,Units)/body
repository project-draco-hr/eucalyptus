{
  Map<String,S3UsageSummary> summaryMap=usageLog.scanSummarize(period,criterion);
  List<S3DisplayBean> items=new ArrayList<S3DisplayBean>(summaryMap.size());
  for (  String key : summaryMap.keySet()) {
    items.add(new S3DisplayBean(key,null,summaryMap.get(key),displayUnits));
  }
  return items;
}

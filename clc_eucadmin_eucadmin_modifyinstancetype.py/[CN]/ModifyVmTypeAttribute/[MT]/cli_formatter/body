def cli_formatter(self, data):
    previousVmtype = getattr(data, 'euca:previousVmType')
    vmtype = getattr(data, 'euca:vmType')
    fmt = 'TYPE\t%s\t%-10.10s%-10d%-10d%-10d'
    print (fmt % ('OLD', vmtype['euca:name'], int(vmtype['euca:cpu']), int(vmtype['euca:disk']), int(vmtype['euca:memory'])))
    print (fmt % ('NEW', vmtype['euca:name'], int(vmtype['euca:cpu']), int(vmtype['euca:disk']), int(vmtype['euca:memory'])))

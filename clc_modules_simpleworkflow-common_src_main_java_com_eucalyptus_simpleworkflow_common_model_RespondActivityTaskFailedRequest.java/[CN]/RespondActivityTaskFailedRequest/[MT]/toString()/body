{
  StringBuilder sb=new StringBuilder();
  sb.append("{");
  if (getTaskToken() != null)   sb.append("TaskToken: " + getTaskToken() + ",");
  if (getReason() != null)   sb.append("Reason: " + getReason() + ",");
  if (getDetails() != null)   sb.append("Details: " + getDetails());
  sb.append("}");
  return sb.toString();
}

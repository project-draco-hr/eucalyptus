{
  ClusterInfo clusterInfo=new ClusterInfo(c.getName(),c.getHost(),c.getPort());
  try {
    Clusters.addClusterKeys(clusterInfo.getName());
  }
 catch (  Exception e1) {
    LOG.error(e1,e1);
  }
  this.register(new Cluster(clusterInfo));
  this.lookup(clusterInfo.getName()).start();
  return clusterInfo;
}

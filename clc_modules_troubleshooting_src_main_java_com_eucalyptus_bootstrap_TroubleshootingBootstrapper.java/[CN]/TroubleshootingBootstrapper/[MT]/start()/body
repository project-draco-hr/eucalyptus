{
  LOG.info("Starting troubleshooting interface.");
  DiskResourceCheck check=new DiskResourceCheck();
  check.addLocationInfo(BaseDirectory.HOME.getFile(),50 * 1024 * 1024);
  check.start();
  new SimpleMemoryResourceCheck(512 * 1024).start();
  new MXBeanMemoryResourceCheck().start();
  return true;
}

{
  final Context context=Contexts.lookup();
  if (context.hasAdministrativePrivileges()) {
    final Future<PollForNotificationResponseType> response=addPoller(poll.getChannel(),new Poller(poll.getChannel(),poll.getCorrelationId()));
    checkNotify(poll.getChannel());
    try {
      return response.get();
    }
 catch (    final Exception e) {
      return poll.getReply();
    }
  }
 else {
    return poll.getReply();
  }
}

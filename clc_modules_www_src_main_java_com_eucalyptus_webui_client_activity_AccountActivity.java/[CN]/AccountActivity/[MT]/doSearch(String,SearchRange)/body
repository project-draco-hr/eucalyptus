{
  this.clientFactory.getBackendService().lookupAccount(this.clientFactory.getLocalSession().getSession(),search,range,new AsyncCallback<SearchResult>(){
    @Override public void onFailure(    Throwable caught){
      LOG.log(Level.WARNING,"Search failed: " + caught);
      displayData(null);
    }
    @Override public void onSuccess(    SearchResult result){
      LOG.log(Level.WARNING,"Search success:" + result.getLength());
      displayData(result);
    }
  }
);
}

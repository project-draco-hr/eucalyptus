{
  final List<RunningInstancesItemType> instances=Ec2Client.getInstance().describeInstances(null,Lists.newArrayList(workerId));
  if (instances != null && instances.size() == 1) {
    setWorkerState(workerId,ImagingWorker.STATE.RETIRING);
  }
 else {
    LOG.debug("Forgetting about imaging worker " + workerId);
    removeWorker(workerId);
  }
}

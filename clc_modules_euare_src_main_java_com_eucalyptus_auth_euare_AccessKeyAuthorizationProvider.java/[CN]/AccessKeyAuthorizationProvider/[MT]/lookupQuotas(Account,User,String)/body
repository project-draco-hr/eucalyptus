{
  final List<Authorization> authorizations=Lists.newArrayList();
  if (PolicySpec.qualifiedName(PolicySpec.VENDOR_IAM,PolicySpec.IAM_RESOURCE_ACCESS_KEY).equals(resourceType)) {
    authorizations.add(new QuotaAuthorization(account.getAccountNumber(),user.getUserId(),PolicySpec.IAM_RESOURCE_ACCESS_KEY,new QuotaCondition(Keys.IAM_QUOTA_ACCESS_KEY_NUMBER_PER_USER,Objects.toString(AuthenticationProperties.ACCESS_KEYS_LIMIT,"2"))));
  }
  return authorizations;
}

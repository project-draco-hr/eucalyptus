{
  try {
    for (    final AutoScalingGroupMetricsView group : autoScalingGroups.listRequiringMonitoring(10000L,TypeMappers.lookup(AutoScalingGroup.class,AutoScalingGroupMetricsView.class))) {
      if (!group.getEnabledMetrics().isEmpty()) {
        final List<AutoScalingInstanceCoreView> groupInstances=Sets.intersection(group.getEnabledMetrics(),instanceMetrics).isEmpty() ? Collections.<AutoScalingInstanceCoreView>emptyList() : autoScalingInstances.listByGroup(group,Predicates.alwaysTrue(),TypeMappers.lookup(AutoScalingInstance.class,AutoScalingInstanceCoreView.class));
        runTask(new MetricsSubmissionScalingProcessTask(group,groupInstances));
      }
    }
  }
 catch (  Exception e) {
    logger.error(e,e);
  }
}

{
  String bucketName=resource.split("/",2)[0];
switch (scope) {
case ACCOUNT:
    return Long.toString(ObjectStorageQuotaUtil.countBucketObjects(bucketName) + 1);
case GROUP:
  return NOT_SUPPORTED;
case USER:
return Long.toString(ObjectStorageQuotaUtil.countBucketObjects(bucketName) + 1);
}
throw new AuthException("Invalid scope");
}

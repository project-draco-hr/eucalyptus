{
  if (channelFuture.isSuccess()) {
    channelFuture.getChannel().write(request).addListener(callback);
  }
 else {
    LOG.debug(channelFuture.getCause(),channelFuture.getCause());
    try {
      callback.operationComplete(channelFuture);
    }
 catch (    Throwable e) {
      LOG.debug(e,e);
    }
  }
}

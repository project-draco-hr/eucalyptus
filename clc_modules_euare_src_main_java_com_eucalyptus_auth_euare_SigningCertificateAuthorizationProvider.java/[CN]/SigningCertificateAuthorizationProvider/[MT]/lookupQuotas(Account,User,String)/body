{
  final List<Authorization> authorizations=Lists.newArrayList();
  if (PolicySpec.qualifiedName(PolicySpec.VENDOR_IAM,PolicySpec.IAM_RESOURCE_SIGNING_CERTIFICATE).equals(resourceType)) {
    authorizations.add(new QuotaAuthorization(account.getAccountNumber(),user.getUserId(),PolicySpec.IAM_RESOURCE_SIGNING_CERTIFICATE,new QuotaCondition(Keys.IAM_QUOTA_SIGNING_CERTIFICATE_NUMBER_PER_USER,Objects.toString(AuthenticationProperties.SIGNING_CERTIFICATES_LIMIT,"2"))));
  }
  return authorizations;
}

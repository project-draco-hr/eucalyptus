{
  this.clientFactory.getBackendService().getCategory(this.clientFactory.getLocalSession().getSession(),new AsyncCallback<ArrayList<CategoryTag>>(){
    @Override public void onFailure(    Throwable caught){
      LOG.log(Level.WARNING,"Cannot get category: " + caught);
      clientFactory.getLifecyclePlaceController().goTo(new LoginPlace(LoginPlace.LOADING_FAILURE_PROMPT));
    }
    @Override public void onSuccess(    ArrayList<CategoryTag> result){
      category=result;
      clientFactory.getLoadingProgressView().setProgress(100);
      startMain();
    }
  }
);
}

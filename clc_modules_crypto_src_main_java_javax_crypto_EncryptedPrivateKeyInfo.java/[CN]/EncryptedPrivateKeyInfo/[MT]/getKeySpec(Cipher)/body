{
  byte[] encoded=null;
  try {
    encoded=cipher.doFinal((byte[])encryptedData);
    checkPKCS8Encoding(encoded);
  }
 catch (  GeneralSecurityException gse) {
    InvalidKeySpecException ikse=new InvalidKeySpecException("Cannot retrieve the PKCS8EncodedKeySpec");
    ikse.initCause(gse);
    throw ikse;
  }
catch (  IOException ioe) {
    InvalidKeySpecException ikse=new InvalidKeySpecException("Cannot retrieve the PKCS8EncodedKeySpec");
    ikse.initCause(ioe);
    throw ikse;
  }
catch (  IllegalStateException ise) {
    InvalidKeySpecException ikse=new InvalidKeySpecException("Cannot retrieve the PKCS8EncodedKeySpec");
    ikse.initCause(ise);
    throw ikse;
  }
  return new PKCS8EncodedKeySpec(encoded);
}

{
  try {
    Bootstrap.Stage stage=Bootstrap.transition();
    do {
      stage.load();
    }
 while ((stage=Bootstrap.transition()) != null);
  }
 catch (  BootstrapException e) {
    throw e;
  }
catch (  Throwable t) {
    LOG.fatal(t,t);
    System.exit(1);
    throw t;
  }
  Lifecycles.State.INITIALIZED.to(Lifecycles.State.LOADED).transition(Components.list());
  return true;
}

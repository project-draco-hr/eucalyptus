{
  if (BootstrapArgs.isInitializeSystem()) {
    try {
      Bootstrap.initializeSystem();
      System.exit(0);
    }
 catch (    Throwable t) {
      LOG.error(t,t);
      System.exit(1);
      throw t;
    }
  }
 else {
    SystemBootstrapper.runSystemStages();
    SystemBootstrapper.runComponentStages(Component.Transition.LOADING,Components.filterWhichCanLoad());
  }
  return true;
}

{
  LOG.info("Starting Eucalyptus.");
  try {
    for (    Bootstrapper b : this.bootstrappers) {
      LOG.info("-> Invoking bootstrapper " + b.getClass().getSimpleName() + ".start()Z");
      b.start();
    }
    LOG.info(LogUtils.header("Starting Eucalyptus."));
    context.start();
    return true;
  }
 catch (  Exception e) {
    LOG.error(e,e);
    return false;
  }
}

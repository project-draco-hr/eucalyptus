{
  try {
    Bootstrap.Stage stage=Bootstrap.transition();
    do {
      stage.start();
    }
 while ((stage=Bootstrap.transition()) != null);
  }
 catch (  BootstrapException e) {
    e.printStackTrace();
    throw e;
  }
catch (  Throwable t) {
    LOG.fatal(t,t);
    System.exit(1);
    throw t;
  }
  Lifecycles.State.LOADED.to(Lifecycles.State.STARTED).transition(Components.list());
  return true;
}

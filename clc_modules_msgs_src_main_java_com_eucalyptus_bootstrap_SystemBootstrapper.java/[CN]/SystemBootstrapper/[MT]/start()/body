{
  LOG.info("Starting Eucalyptus.");
  try {
    context.start();
    for (    Bootstrapper b : this.bootstrappers) {
      LOG.info("-> Invoking bootsrapper " + b.getClass().getSimpleName() + ".start()");
      b.load();
    }
    return true;
  }
 catch (  Exception e) {
    LOG.error(e,e);
    return false;
  }
}

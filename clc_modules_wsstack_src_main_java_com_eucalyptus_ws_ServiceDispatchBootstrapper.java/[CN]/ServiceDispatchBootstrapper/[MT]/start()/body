{
  boolean failed=false;
  for (  Component comp : Components.list()) {
    EventRecord.here(ServiceDispatchBootstrapper.class,EventClass.COMPONENT,EventType.COMPONENT_INFO).withDetails(comp.getName(),comp.getName(),"addresses",NetworkUtil.getAllAddresses().toString()).withDetails("local",comp.isLocal().toString()).withDetails("enabled",comp.isEnabled().toString());
    for (    ServiceConfiguration s : comp.list()) {
      try {
        if (comp.isEnabled()) {
          comp.startService(s);
        }
      }
 catch (      Throwable ex) {
        LOG.warn(ex);
        LOG.error(ex,ex);
        failed=true;
      }
    }
  }
  if (failed) {
    BootstrapException.throwFatal("Failures occurred while attempting to start component services.  See the log files for more information.");
  }
  return true;
}

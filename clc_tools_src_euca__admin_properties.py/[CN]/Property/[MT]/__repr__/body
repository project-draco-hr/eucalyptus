def __repr__(self):
    global VERBOSE
    str = ('PROPERTY %s %s' % (self.property_name, self.property_value))
    if (self.property_old_value is not None):
        str = ('%s was %s' % (str, self.property_old_value))
    elif VERBOSE:
        str = ('%s\nDESCRIPTION %s' % (str, self.property_description))
    return str

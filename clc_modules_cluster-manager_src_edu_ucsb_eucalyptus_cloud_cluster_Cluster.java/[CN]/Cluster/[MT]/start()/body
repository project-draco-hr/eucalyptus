{
  LOG.info("Starting cluster: " + this.clusterInfo.getUri());
  if (this.mqThread == null || this.mqThread.isAlive())   this.mqThread=this.startNamedThread(messageQueue);
  if (this.rscThread == null || this.rscThread.isAlive())   this.rscThread=this.startNamedThread(rscUpdater);
  if (this.vmThread == null || this.vmThread.isAlive())   this.vmThread=this.startNamedThread(vmUpdater);
  if (this.addrThread == null || this.addrThread.isAlive())   this.addrThread=this.startNamedThread(addrUpdater);
  this.fireNodeThreads();
}

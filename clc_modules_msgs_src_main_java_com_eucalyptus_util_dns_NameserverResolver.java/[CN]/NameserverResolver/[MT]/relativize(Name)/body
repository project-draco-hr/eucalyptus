{
  Name external=DomainNames.externalSubdomain(Eucalyptus.class);
  Name internal=DomainNames.internalSubdomain(Eucalyptus.class);
  Name relative=null;
  if (name.subdomain(external)) {
    relative=name.relativize(external);
  }
 else   if (name.subdomain(internal)) {
    relative=name.relativize(internal);
  }
 else {
    return name;
  }
  return relative;
}

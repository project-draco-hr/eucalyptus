{
  final FilteredPipeline candidate=null;
  for (  final FilteredPipeline f : pipelines) {
    if (f.checkAccepts(request)) {
      return f;
    }
  }
  if (candidate == null) {
    for (    final FilteredPipeline f : internalPipelines) {
      if (f.checkAccepts(request)) {
        return f;
      }
    }
  }
  return candidate;
}

{
  FilteredPipeline candidate=null;
  for (  FilteredPipeline f : pipelines) {
    if (f.checkAccepts(request)) {
      return f;
    }
  }
  if (candidate == null) {
    for (    FilteredPipeline f : internalPipelines) {
      if (f.checkAccepts(request)) {
        return f;
      }
    }
  }
  return candidate;
}

{
  for (  Entry<Integer,TransitionListener<P>> entry : this.listeners.entrySet()) {
    final TransitionListener<P> tl=entry.getValue();
    Logs.exhaust().trace(EventRecord.here(TransitionImpl.class,EventType.TRANSITION_LISTENER,"" + parent.getName(),this.toString(),phase.toString(),entry.getKey().toString(),tl.getClass().toString()));
    try {
      if (!pred.apply(entry.getValue())) {
        throw new TransitionListenerException(entry.getValue().getClass().getSimpleName() + "." + phase+ "( ) returned false.");
      }
    }
 catch (    Exception t) {
      Logs.exhaust().error(t,t);
      return false;
    }
  }
  return true;
}

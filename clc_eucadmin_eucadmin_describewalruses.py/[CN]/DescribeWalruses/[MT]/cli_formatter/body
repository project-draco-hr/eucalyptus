def cli_formatter(self, data):
    walruses = getattr(data, 'euca:registered')
    fmt = 'WALRUS\t%-15.15s\t%-15.15s\t%-25s\t%s\t%s'
    for w in walruses:
        if (w.get('euca:hostName', None) != 'detail'):
            print (fmt % (w.get('euca:partition', None), w.get('euca:name', None), w.get('euca:hostName', None), w.get('euca:state', None), w.get('euca:detail', None)))

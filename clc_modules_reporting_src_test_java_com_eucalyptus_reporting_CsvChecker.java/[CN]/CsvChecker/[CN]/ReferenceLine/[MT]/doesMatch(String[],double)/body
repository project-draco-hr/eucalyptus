{
  if (!(fields.length == otherFields.length))   return false;
  for (int i=0; i < fields.length; i++) {
    if (i == doublePlusFieldInd)     continue;
    if (fields[i].trim().length() == 0)     continue;
    if (!fieldMatches(fields[i],otherFields[i],errorMargin))     if (debug)     System.out.printf("Field %d doesn't match, field:%s other:%s\n",i,fields[i],otherFields[i]);
    return false;
  }
  return true;
}

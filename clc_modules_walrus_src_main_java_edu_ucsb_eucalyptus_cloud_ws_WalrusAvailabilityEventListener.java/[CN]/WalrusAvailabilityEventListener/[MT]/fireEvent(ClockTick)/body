{
  if (Bootstrap.isFinished() && Hosts.isCoordinator()) {
    try {
      long total=100;
      ListenerRegistry.getInstance().fireEvent(new ResourceAvailabilityEvent(StorageWalrus,new Availability(total,Math.max(0,total - (long)Math.ceil((double)WalrusUtil.countTotalObjectSize() / FileUtils.ONE_GB)))));
    }
 catch (    Exception ex) {
      logger.error(ex,ex);
    }
  }
}

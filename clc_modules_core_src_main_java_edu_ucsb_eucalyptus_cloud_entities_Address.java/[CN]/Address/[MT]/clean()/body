{
  this.canHas.writeLock().lock();
  try {
    String user=this.userId;
    if (!this.isPending()) {
      this.release();
      if (!Component.eucalyptus.name().equals(user) || !Addresses.doDynamicAddressing()) {
        this.allocate(userId);
      }
    }
  }
  finally {
    this.canHas.writeLock().unlock();
  }
}

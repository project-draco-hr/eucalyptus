{
  final String[] enbabledCipherSuites=ciphers().with("SSL_DHE_DSS_EXPORT_WITH_DES40_CBC_SHA:TLS_PSK_WITH_AES_256_CBC_SHA:SSL_DH_anon_WITH_RC4_128_MD5").enabledCipherSuites(new String[]{"SSL_DHE_DSS_EXPORT_WITH_DES40_CBC_SHA","SSL_DH_anon_WITH_RC4_128_MD5"});
  assertArrayEquals("Enabled cipher suites",new String[]{"SSL_DHE_DSS_EXPORT_WITH_DES40_CBC_SHA","SSL_DH_anon_WITH_RC4_128_MD5"},enbabledCipherSuites);
}

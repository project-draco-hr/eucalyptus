{
  EucaButton deleteEucaButton=new EucaButton("Delete",new ClickHandler(){
    public void onClick(    ClickEvent event){
      GWTUtils.redirect(GWT.getModuleBaseURL() + "?action=delete" + "&user="+ userName+ "&page="+ currentTabIndex);
    }
  }
);
  EucaButton okEucaButton=displayDialog("Sure?","Do you want to delete user '" + userName + "'?",deleteEucaButton);
  okEucaButton.setText("Cancel");
  label_box.setStyleName("euca-greeting-warning");
}

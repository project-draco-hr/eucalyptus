{
  Button deleteButton=new Button("Delete",new ClickListener(){
    public void onClick(    Widget sender){
      GWTUtils.redirect(GWT.getModuleBaseURL() + "?action=delete" + "&user="+ userName+ "&page="+ currentTabIndex);
    }
  }
);
  Button okButton=displayDialog("Sure?","Do you want to delete user '" + userName + "'?",deleteButton);
  okButton.setText("Cancel");
  label_box.setStyleName("euca-greeting-warning");
}

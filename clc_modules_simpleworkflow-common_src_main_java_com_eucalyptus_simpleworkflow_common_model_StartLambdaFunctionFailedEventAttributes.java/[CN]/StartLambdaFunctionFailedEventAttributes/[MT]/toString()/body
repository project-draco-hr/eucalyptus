{
  StringBuilder sb=new StringBuilder();
  sb.append("{");
  if (getScheduledEventId() != null)   sb.append("ScheduledEventId: " + getScheduledEventId() + ",");
  if (getCause() != null)   sb.append("Cause: " + getCause() + ",");
  if (getMessage() != null)   sb.append("Message: " + getMessage());
  sb.append("}");
  return sb.toString();
}

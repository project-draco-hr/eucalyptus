{
  try {
synchronized (SslSetup.class) {
      if (trusted == null) {
        KeyStore ks=EucaKeyStore.getInstance().getKeyStore();
        trusted=(X509Certificate)ks.getCertificate(Component.eucalyptus.name());
      }
      return trusted;
    }
  }
 catch (  KeyStoreException e) {
    LOG.error(e,e);
    System.exit(123);
    return null;
  }
}

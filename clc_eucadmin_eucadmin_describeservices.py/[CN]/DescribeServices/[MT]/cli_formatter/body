def cli_formatter(self, data):
    services = getattr(data, 'euca:serviceStatuses')
    fmt = 'SERVICE\t%-15.15s\t%-15s\t%-15s\t%-10s\t%-4s\t%s\t%s'
    for s in services:
        service_id = s['euca:serviceId']
        print (fmt % (service_id['euca:type'], service_id['euca:partition'], service_id['euca:name'], s['euca:localState'], s['euca:localEpoch'], service_id['euca:fullName'], service_id['euca:uri']))

{
  this.groupMap.clear();
  for (  ReportInfo r : result) {
    if (!this.groupMap.containsKey(r.getGroup())) {
      this.groupMap.put(r.getGroup(),new ArrayList<ReportInfo>());
    }
    this.groupMap.get(r.getGroup()).add(r);
    r.setParent(this);
  }
  this.setCurrentReport(ReportInfo.BOGUS);
  this.redraw();
}

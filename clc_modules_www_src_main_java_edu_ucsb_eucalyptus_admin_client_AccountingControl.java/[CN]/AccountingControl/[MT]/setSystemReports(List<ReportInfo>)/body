{
  this.systemReports.clear();
  this.systemReports.addAll(result);
  if (!this.systemReports.isEmpty()) {
    for (    ReportInfo r : this.systemReports) {
      r.setParent(this);
    }
    this.setCurrentReport(this.systemReports.get(0));
  }
 else {
    this.setCurrentReport(this.bogus);
  }
  this.redraw();
}

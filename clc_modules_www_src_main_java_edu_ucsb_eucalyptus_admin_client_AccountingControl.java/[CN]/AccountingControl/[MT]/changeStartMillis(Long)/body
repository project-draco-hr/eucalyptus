{
  Long currentTime=new Date().getTime();
  if (millis > this.endMillis) {
    this.startMillis=this.endMillis;
  }
 else   if ((millis > currentTime)) {
    this.startMillis=this.endMillis;
  }
 else   if (millis < 0) {
    this.startMillis=this.startMillis;
  }
 else {
    this.startMillis=millis;
  }
  return this.startMillis;
}

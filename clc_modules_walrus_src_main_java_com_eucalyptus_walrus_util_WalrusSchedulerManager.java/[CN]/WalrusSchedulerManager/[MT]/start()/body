{
  lock.lock();
  try {
    if (!initted) {
      LOG.error("cannot start quartz scheduler for Walrus because it is not initialized");
    }
 else {
      populateJobs();
    }
  }
 catch (  Exception e) {
    LOG.error("caught an exception while attempting to start the quartz scheduler for Walrus jobs, " + "the exception had the message - " + e.getMessage());
  }
 finally {
    lock.unlock();
  }
}

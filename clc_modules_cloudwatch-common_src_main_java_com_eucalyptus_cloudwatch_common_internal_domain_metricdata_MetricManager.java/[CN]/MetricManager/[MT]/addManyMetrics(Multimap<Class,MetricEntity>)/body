{
  for (  Class c : metricMap.keySet()) {
    EntityTransaction db=Entities.get(c);
    try {
      for (      MetricEntity me : metricMap.get(c)) {
        Entities.persist(me);
      }
      db.commit();
    }
 catch (    RuntimeException ex) {
      Logs.extreme().error(ex,ex);
      throw ex;
    }
 finally {
      if (db.isActive())       db.rollback();
    }
  }
}

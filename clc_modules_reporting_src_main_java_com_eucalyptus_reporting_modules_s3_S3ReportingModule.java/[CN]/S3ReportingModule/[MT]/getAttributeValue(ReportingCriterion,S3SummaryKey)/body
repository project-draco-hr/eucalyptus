{
  log.debug("owner id:" + key.getOwnerId() + " account id:"+ key.getAccountId());
switch (criterion) {
case ACCOUNT:
    return ReportingAccountDao.getInstance().getAccountName(key.getAccountId());
case USER:
  return ReportingUserDao.getInstance().getUserName(key.getOwnerId());
default :
return ReportingUserDao.getInstance().getUserName(key.getOwnerId());
}
}

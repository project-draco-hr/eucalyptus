{
  final Record query=request.getQuery();
  if (!Bootstrap.isOperational() || !dns_resolver_enabled) {
    return false;
  }
 else   if (DnsResolvers.RequestType.A.apply(query) && query.getName().subdomain(LoadBalancerDomainName.getLoadBalancerSubdomain())) {
    return true;
  }
  return false;
}

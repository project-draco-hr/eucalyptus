{
  EstimateTemplateCostResponseType reply=request.getReply();
  try {
    final Context ctx=Contexts.lookup();
    checkActionPermission(CloudFormationPolicySpec.CLOUDFORMATION_ESTIMATETEMPLATECOST,ctx);
  }
 catch (  Exception ex) {
    LOG.error(ex,ex);
    handleException(ex);
  }
  return reply;
}

{
  final String userData=B64.standard.encString(String.format("%s\n%s",ImagingServiceProperties.getCredentialsString(),ImagingServiceProperties.getWorkerUserData(ImagingServiceProperties.NTP_SERVER,ImagingServiceProperties.LOG_SERVER,ImagingServiceProperties.LOG_SERVER_PORT,ImagingServiceProperties.INIT_SCRIPT)));
  return userData;
}

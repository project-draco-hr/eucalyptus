{
  EntityWrapper<SANVolumeInfo> db=StorageProperties.getEntityWrapper();
  try {
    SANVolumeInfo snapInfo=db.getUnique(new SANVolumeInfo(snapshotId));
    return snapInfo.getSize();
  }
 catch (  EucalyptusCloudException ex) {
    LOG.error(ex);
    throw ex;
  }
 finally {
    db.commit();
  }
}

def decode(self, encoded):
    '\n        Decode a String that was encoded using the encode method in this Class\n\n        @param encoded: the string to decode\n        @return: the decoded string\n        '
    buf = ''
    pbs = PushbackString(encoded)
    while pbs.has_next():
        char = self.decode_character(pbs)
        if (char is not None):
            buf += char
        else:
            buf += pbs.next()
    return buf

{
  if (timestampMs == null)   throw new IllegalArgumentException("timestampMs can't be null");
  if (uuid == null)   throw new IllegalArgumentException("uuid can't be null");
  this.uuid=uuid;
  this.timestampMs=timestampMs;
  this.cumulativeNetIoMegs=cumulativeNetIoMegs;
  this.cumulativeDiskIoMegs=cumulativeDiskIoMegs;
  this.cpuUtilizationPercent=cpuUtilizationPercent;
}

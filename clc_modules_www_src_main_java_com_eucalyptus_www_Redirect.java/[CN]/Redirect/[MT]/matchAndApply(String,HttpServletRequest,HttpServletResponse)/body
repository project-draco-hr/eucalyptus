{
  String urlStr=req.getRequestURL().toString();
  URL url=new URL(urlStr);
  if (!urlStr.startsWith("https")) {
    resp.sendRedirect(String.format("https://%s:%d/%s",url.getHost(),HttpServerBootstrapper.HTTPS_PORT,url.getPath() + ((req.getQueryString() != null) ? "?" + req.getQueryString() : "")));
    return target;
  }
 else {
    return target;
  }
}

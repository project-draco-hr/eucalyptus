{
  final EncryptedSecurityToken encryptedSecurityToken;
  try {
    encryptedSecurityToken=EncryptedSecurityToken.decrypt(accessKeyId,getEncryptionKey(accessKeyId),token);
  }
 catch (  GeneralSecurityException e) {
    throw new AuthException("Unable to decode token",e);
  }
  return new SecurityTokenContentImpl(Optional.fromNullable(encryptedSecurityToken.getOriginatingAccessKeyId()),Optional.fromNullable(encryptedSecurityToken.getUserId()),Optional.fromNullable(encryptedSecurityToken.getRoleId()),encryptedSecurityToken.getNonce(),encryptedSecurityToken.getCreated(),encryptedSecurityToken.getExpires());
}

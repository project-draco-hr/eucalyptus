{
  try {
    final SecurityTokenOutput out=new SecurityTokenOutput();
    out.writeInt(2);
    out.writeString(originatingId);
    out.writeString(nonce);
    out.writeLong(created);
    out.writeLong(expires);
    return out.toByteArray();
  }
 catch (  IOException e) {
    throw Exceptions.toUndeclared(e);
  }
}

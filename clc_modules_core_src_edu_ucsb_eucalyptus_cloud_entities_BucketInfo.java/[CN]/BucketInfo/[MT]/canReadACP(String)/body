{
  if (ownerId.equals(userId)) {
    return true;
  }
 else   if (globalReadACP) {
    return true;
  }
 else {
    for (    GrantInfo grantInfo : grants) {
      if (grantInfo.getUserId().equals(userId) && grantInfo.isReadACP()) {
        return true;
      }
    }
  }
  if (UserManagement.isAdministrator(userId)) {
    return true;
  }
  return false;
}

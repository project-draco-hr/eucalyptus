{
  try (TransactionResource db=Entities.transactionFor(StackResourceEntityForUpdate.class)){
    Criteria criteria=Entities.createCriteria(StackResourceEntityForUpdate.class).add(Restrictions.eq("accountId",accountId)).add(Restrictions.eq("stackId",stackId)).add(Restrictions.eq("recordDeleted",Boolean.FALSE));
    for (    StackResourceEntity stackResourceEntity : (List<StackResourceEntityForUpdate>)criteria.list()) {
      stackResourceEntity.setRecordDeleted(Boolean.TRUE);
    }
    db.commit();
  }
 }

{
  try {
    String walrusUrl=EucalyptusProperties.getWalrusUrl();
    TRACKER_URL="http://" + new URI(walrusUrl).getHost() + ":"+ TRACKER_PORT+ "/announce";
  }
 catch (  EucalyptusCloudException e) {
    LOG.error(e);
  }
catch (  URISyntaxException e) {
    LOG.error(e);
  }
  return TRACKER_URL;
}

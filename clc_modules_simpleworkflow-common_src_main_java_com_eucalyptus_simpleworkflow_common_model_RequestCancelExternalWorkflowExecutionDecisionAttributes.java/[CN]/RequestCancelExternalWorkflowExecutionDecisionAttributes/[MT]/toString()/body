{
  StringBuilder sb=new StringBuilder();
  sb.append("{");
  if (getWorkflowId() != null)   sb.append("WorkflowId: " + getWorkflowId() + ",");
  if (getRunId() != null)   sb.append("RunId: " + getRunId() + ",");
  if (getControl() != null)   sb.append("Control: " + getControl());
  sb.append("}");
  return sb.toString();
}

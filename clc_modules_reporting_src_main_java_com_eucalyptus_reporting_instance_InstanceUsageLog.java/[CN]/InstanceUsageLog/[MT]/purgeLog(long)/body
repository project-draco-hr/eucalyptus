{
  log.info(String.format("purge earlierThan:%d ",earlierThanMs));
  EntityWrapper<InstanceAttributes> entityWrapper=EntityWrapper.get(InstanceAttributes.class);
  try {
    entityWrapper.createSQLQuery("DELETE FROM instance_usage_snapshot WHERE timestamp_ms < ?").setLong(0,new Long(earlierThanMs)).executeUpdate();
    entityWrapper.createSQLQuery("DELETE reporting_instance" + " FROM reporting_instance" + " LEFT OUTER JOIN instance_usage_snapshot"+ " ON reporting_instance.uuid = instance_usage_snapshot.uuid"+ " WHERE instance_usage_snapshot.uuid IS NULL").executeUpdate();
    entityWrapper.commit();
  }
 catch (  Exception ex) {
    log.error(ex);
    entityWrapper.rollback();
    throw new RuntimeException(ex);
  }
}

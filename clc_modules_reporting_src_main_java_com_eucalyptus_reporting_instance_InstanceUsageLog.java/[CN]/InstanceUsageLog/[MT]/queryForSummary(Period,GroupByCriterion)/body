{
  Map<String,UsageSummary> results=new HashMap<String,UsageSummary>();
  EntityWrapper entityWrapper=EntityWrapper.get(InstanceAttributes.class);
  Session sess=null;
  try {
    sess=entityWrapper.getSession();
    sess.createSQLQuery("SELECT min(), max() " + " FROM instance_attributes, instance_usage_snapshot " + " LEFT OUTER JOIN instance_usage_snapshot"+ " ON instance_attributes.uuid = instance_usage_snapshot.uuid"+ " WHERE instance_usage_snapshot.uuid IS NULL").executeUpdate();
    entityWrapper.commit();
  }
 catch (  Exception ex) {
    log.error(ex);
    entityWrapper.rollback();
    throw new RuntimeException(ex);
  }
  return results;
}

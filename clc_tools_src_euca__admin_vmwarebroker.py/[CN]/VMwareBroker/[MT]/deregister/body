def deregister(self, name, partition='vmwarebroker'):
    params = {'Name': name, }
    if partition:
        params['Partition'] = partition
    try:
        reply = self.euca.connection.get_object('DeregisterVMwareBroker', {'Name': name, }, BooleanResponse)
        print reply
    except EC2ResponseError as ex:
        self.euca.handle_error(ex)

@tornado.web.authenticated
def get(self, path):
    path = (config.get('eui', 'staticpath') + path)
    if path.endswith('.html'):
        self.render(path, username=self.get_current_user())
    elif os.path.exists(path):
        f = open(path, 'r')
        self.write(f.read())
    else:
        self.send_error(status_code=404)

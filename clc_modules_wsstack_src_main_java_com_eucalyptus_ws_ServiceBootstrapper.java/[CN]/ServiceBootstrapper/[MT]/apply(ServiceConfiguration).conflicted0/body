{
  boolean ret=config.getComponentId().isAlwaysLocal() || config.isVmLocal() || (BootstrapArgs.isCloudController() && config.getComponentId().isCloudLocal())|| Hosts.Coordinator.INSTANCE.isLocalhost();
  ret=(Eucalyptus.class.equals(config.getComponentId().getClass()) && !config.isHostLocal()) ? false : ret;
  LOG.debug("ServiceBootstrapper.shouldLoad(" + config.toString() + "):"+ ret);
  return ret;
}

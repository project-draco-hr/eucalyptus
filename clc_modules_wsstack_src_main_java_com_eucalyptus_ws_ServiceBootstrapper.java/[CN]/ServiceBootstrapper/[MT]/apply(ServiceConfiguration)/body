{
  final boolean ret=config.getComponentId().isAlwaysLocal() || config.isVmLocal() || (BootstrapArgs.isCloudController() && config.getComponentId().isCloudLocal())|| Hosts.Coordinator.INSTANCE.isLocalhost();
  LOG.debug("ServiceBootstrapper.shouldLoad(" + config.toString() + "):"+ ret);
  return ret;
}

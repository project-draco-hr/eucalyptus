{
  if (!BindingManager.bindingMap.containsKey(bindingName)) {
    try {
      BindingManager.getBinding(bindingName).seed(seedClass);
      LOG.info("Seeding binding " + bindingName + " with class "+ seedClass);
    }
 catch (    BindingException e) {
      LOG.info("Failed to seed binding " + bindingName + " with class "+ seedClass);
    }
  }
}

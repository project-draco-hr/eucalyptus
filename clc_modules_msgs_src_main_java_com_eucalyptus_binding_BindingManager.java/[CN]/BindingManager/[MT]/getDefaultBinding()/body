{
  if (DEFAULT != null) {
    return DEFAULT;
  }
 else {
synchronized (BindingManager.class) {
      if (DEFAULT != null) {
        return DEFAULT;
      }
 else {
        try {
          DEFAULT=BindingManager.getBinding(BindingManager.sanitizeNamespace(BindingManager.DEFAULT_BINDING_NAME));
          return DEFAULT;
        }
 catch (        BindingException ex) {
          LOG.error(ex,ex);
          throw new RuntimeException(ex);
        }
      }
    }
  }
}

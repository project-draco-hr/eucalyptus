{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  Resource other=(Resource)obj;
  if (this.origin == null) {
    if (other.origin != null)     return false;
  }
 else   if (!this.origin.equals(other.origin))   return false;
  return true;
}

def register(self, host, name='walrus', port=8773, partition='walrus'):
    if (host == None):
        self.euca.handle_error('Missing hostname')
    params = {'Partition': partition, 'Name': name, 'Host': host, 'Port': port, }
    try:
        reply = self.euca.connection.get_object('RegisterWalrus', params, BooleanResponse)
        print reply
    except EC2ResponseError as ex:
        self.euca.handle_error(ex)

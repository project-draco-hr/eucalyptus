{
  if (!Bootstrap.isOperational()) {
    return false;
  }
 else   if (AddressRegistry.getInstance().contains(input.getHostAddress())) {
    return true;
  }
 else   if (privateNetworksManagedSupplier.get() && input.isSiteLocalAddress()) {
    return true;
  }
 else {
    return lookupPublic(input).isPresent() || !privateNetworksManagedSupplier.get() && Predicates.or(clusterSubnetsSupplier.get()).apply(input);
  }
}

{
  try {
    final AccountFullName elbSystemAccount=AccountFullName.getInstance(Accounts.lookupAccountIdByAlias(AccountIdentifiers.ELB_SYSTEM_ACCOUNT));
    if (EucalyptusActivityTasks.getInstance().defaultVpc(elbSystemAccount).isPresent())     return true;
 else     return false;
  }
 catch (  final Exception ex) {
    return false;
  }
}

def parse_number(self, pbs):
    "\n        Parse a decimal number, such as those from Javascript's \n        String.fromCharCode(value)\n        \n        @param pbs: the PushbackString\n        @return: character representation of the decimal value\n        "
    buf = ''
    while pbs.has_next():
        peek_char = pbs.peek()
        if peek_char.isdigit():
            buf += peek_char
            pbs.next()
        elif (peek_char == ';'):
            pbs.next()
            break
        else:
            break
    try:
        i = int(buf)
        return unichr(i)
    except ValueError:
        return None

{
  StringBuilder sb=new StringBuilder();
  sb.append(ARN_PREFIX).append(this.getVendor()).append("::").append(this.getNamespace()).append(':').append(this.type).append(this.path);
  if (!"/".equals(this.path) && !this.path.endsWith("/")) {
    sb.append('/');
  }
  sb.append(this.name);
  return sb.toString();
}

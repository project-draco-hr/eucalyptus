{
  Map<String,Map<String,InstanceUsageSummary>> summaryMap=usageLog.scanSummarize(period,groupByCrit,crit);
  List<InstanceDisplayBean> items=new ArrayList<InstanceDisplayBean>(summaryMap.size());
  for (  String outerKey : summaryMap.keySet()) {
    Map<String,InstanceUsageSummary> innerMap=summaryMap.get(outerKey);
    for (    String innerKey : innerMap.keySet()) {
      items.add(new InstanceDisplayBean(innerKey,outerKey,innerMap.get(innerKey),displayUnits));
    }
  }
  return items;
}

{
  Map<String,InstanceUsageSummary> summaryMap=usageLog.scanSummarize(period,criterion);
  List<InstanceDisplayBean> items=new ArrayList<InstanceDisplayBean>(summaryMap.size());
  for (  String key : summaryMap.keySet()) {
    items.add(new InstanceDisplayBean(key,null,summaryMap.get(key),displayUnits));
  }
  return items;
}

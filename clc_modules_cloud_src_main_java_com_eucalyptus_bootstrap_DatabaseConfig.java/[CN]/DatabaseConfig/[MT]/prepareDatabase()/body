{
  File dbFile=new File(SubDirectory.DB.toString() + File.separator + this.getDatabaseName()+ ".script");
  if (!dbFile.exists()) {
    FileWriter dbOut=new FileWriter(dbFile);
    dbOut.write(String.format(DEFAULT,System.getProperty("euca.db.password")));
    dbOut.flush();
    dbOut.close();
  }
}

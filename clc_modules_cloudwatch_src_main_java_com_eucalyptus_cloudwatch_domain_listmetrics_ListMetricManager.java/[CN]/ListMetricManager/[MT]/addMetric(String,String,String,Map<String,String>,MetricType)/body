{
  EntityTransaction db=Entities.get(ListMetric.class);
  try {
    addMetric(db,accountId,metricName,namespace,dimensionMap,metricType);
    db.commit();
  }
 catch (  RuntimeException ex) {
    Logs.extreme().error(ex,ex);
    throw ex;
  }
 finally {
    if (db.isActive())     db.rollback();
  }
}

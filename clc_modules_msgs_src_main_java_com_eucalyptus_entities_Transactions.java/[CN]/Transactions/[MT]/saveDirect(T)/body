{
  checkParam(saveMe,notNullValue());
  final EntityTransaction db=Transactions.get(saveMe);
  try {
    final T entity=Entities.persist(saveMe);
    db.commit();
    return entity;
  }
 catch (  Exception t) {
    throw Transactions.transformException(t);
  }
 finally {
    if (db.isActive())     db.rollback();
    pop();
  }
}

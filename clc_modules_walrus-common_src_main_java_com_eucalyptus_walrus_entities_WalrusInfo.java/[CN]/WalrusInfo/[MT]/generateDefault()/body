{
  return new WalrusInfo(WalrusProperties.NAME,WalrusProperties.bucketRootDirectory,WalrusProperties.MAX_BUCKETS_PER_ACCOUNT,(int)(WalrusProperties.MAX_BUCKET_SIZE / WalrusProperties.M),WalrusProperties.MAX_TOTAL_SNAPSHOT_SIZE,estimateWalrusCapacity(),new Boolean(WalrusProperties.BUCKET_NAMES_REQUIRE_DNS_COMPLIANCE));
}

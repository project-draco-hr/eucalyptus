{
  if (queueUrl != null && queueUrl.getPath() != null) {
    List<String> pathParts=Splitter.on('/').omitEmptyStrings().splitToList(queueUrl.getPath());
    return (pathParts != null && "services".equals(pathParts.get(0)) && "simplequeue".equals(pathParts.get(1)) && pathParts.size() == 4);
  }
 else {
    return false;
  }
}

def register(self, partition, name, host, port=8774):
    params = {'Partition': partition, 'Name': name, 'Host': host, 'Port': port, }
    try:
        reply = self.euca.connection.get_object('RegisterCluster', params, BooleanResponse)
        print reply
    except EC2ResponseError as ex:
        self.euca.handle_error(ex)

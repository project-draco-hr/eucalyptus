{
  int port=Objects.firstNonNull(AuthenticationProperties.CREDENTIAL_DOWNLOAD_PORT == null ? null : Ints.tryParse(AuthenticationProperties.CREDENTIAL_DOWNLOAD_PORT),StackConfiguration.PORT);
  try {
    return port != uri.getPort() ? new URI(uri.getScheme(),uri.getUserInfo(),uri.getHost(),port,uri.getPath(),uri.getQuery(),uri.getFragment()) : uri;
  }
 catch (  URISyntaxException e) {
    return uri;
  }
}

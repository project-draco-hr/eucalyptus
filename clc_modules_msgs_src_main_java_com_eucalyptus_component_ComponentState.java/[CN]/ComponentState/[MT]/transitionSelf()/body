{
  try {
    this.transition(this.getState());
  }
 catch (  IllegalStateException ex) {
    LOG.error(Exceptions.filterStackTrace(ex));
  }
catch (  NoSuchElementException ex) {
    LOG.error(Exceptions.filterStackTrace(ex));
  }
catch (  ExistingTransitionException ex) {
    LOG.error(Exceptions.filterStackTrace(ex));
  }
}

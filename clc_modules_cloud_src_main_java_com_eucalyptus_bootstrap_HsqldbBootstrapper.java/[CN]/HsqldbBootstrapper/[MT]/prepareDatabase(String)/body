{
  File dbFile=new File(SubDirectory.DB.toString() + File.separator + general+ ".script");
  if (!dbFile.exists()) {
    FileWriter dbOut=new FileWriter(dbFile);
    dbOut.write("CREATE SCHEMA PUBLIC AUTHORIZATION DBA\n" + "CREATE USER SA PASSWORD \"" + System.getProperty("euca.db.password") + "\"\n"+ "GRANT DBA TO SA\n"+ "SET WRITE_DELAY 100 MILLIS\n"+ "SET SCHEMA PUBLIC\n");
    dbOut.flush();
    dbOut.close();
  }
}

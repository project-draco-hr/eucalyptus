{
  EventRecord.here(Topology.class,EventType.ENQUEUE,Topology.this.toString(),callable.toString()).info();
  final Long queueStart=System.currentTimeMillis();
  return this.getWorker().submit(new Callable<T>(){
    @Override public T call() throws Exception {
      Long serviceStart=System.currentTimeMillis();
      EventRecord.here(Topology.class,EventType.DEQUEUE,Topology.this.toString(),callable.toString()).append(EventType.QUEUE_TIME.name(),Long.toString(serviceStart - queueStart)).info();
      T result=callable.call();
      Long finish=System.currentTimeMillis();
      EventRecord.here(Topology.class,EventType.QUEUE,Topology.this.toString(),callable.toString()).append(EventType.SERVICE_TIME.name(),Long.toString(finish - serviceStart)).info();
      return result;
    }
  }
);
}

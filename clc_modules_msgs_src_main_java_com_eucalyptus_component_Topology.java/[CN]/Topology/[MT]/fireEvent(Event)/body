{
  if ((event instanceof Hertz) && ((Hertz)event).isAsserted(15l)) {
    try {
      Threads.enqueue(this.internalQueue,RunChecks.INSTANCE).get();
    }
 catch (    InterruptedException ex) {
      Thread.currentThread().interrupt();
    }
catch (    ExecutionException ex) {
      LOG.error(ex,ex);
    }
  }
}

{
  try {
    final Object conf=input.get(30,TimeUnit.SECONDS);
    LOG.trace("Operation succeeded for " + conf);
    return true;
  }
 catch (  final InterruptedException ex) {
    Thread.currentThread().interrupt();
  }
catch (  final Exception ex) {
    Logs.extreme().trace(ex,ex);
  }
  return false;
}

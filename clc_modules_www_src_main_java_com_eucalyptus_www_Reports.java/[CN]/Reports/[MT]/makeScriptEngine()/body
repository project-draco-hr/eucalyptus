{
  if (gse != null) {
    return gse;
  }
 else {
synchronized (Reports.class) {
      if (gse != null) {
        return gse;
      }
 else {
        try {
          return gse=new GroovyScriptEngine(SubDirectory.REPORTS.toString());
        }
 catch (        IOException e) {
          LOG.debug(e,e);
          throw new RuntimeException(e);
        }
      }
    }
  }
}

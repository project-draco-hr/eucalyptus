{
  this.timestamp=System.currentTimeMillis() / 1000;
  this.name=name;
  this.reportName=reportName;
  this.pendingPrint=pendingPrint;
  this.length=0;
  if (this.pendingPrint.isDone()) {
    try {
      this.jasperPrint=this.pendingPrint.get();
    }
 catch (    ExecutionException e) {
      LOG.error(e,e);
    }
catch (    Exception e) {
      LOG.debug(e,e);
    }
  }
}

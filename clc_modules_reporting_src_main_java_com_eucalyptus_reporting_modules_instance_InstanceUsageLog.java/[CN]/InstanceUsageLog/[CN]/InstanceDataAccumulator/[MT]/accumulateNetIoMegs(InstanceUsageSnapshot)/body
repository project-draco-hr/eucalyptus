{
  double remainingFactor=getRemainingFactor(snapshot.getTimestampMs());
  log.debug(String.format("remainingFactor, report:%d-%d (%d), period:%d-%d (%d), factor:%f",period.getBeginningMs(),period.getEndingMs(),period.getEndingMs() - period.getBeginningMs(),lastSnapshot.getTimestampMs(),snapshot.getTimestampMs(),snapshot.getTimestampMs() - lastSnapshot.getTimestampMs(),remainingFactor));
  this.accumulatedNetIoMegs+=(long)(remainingFactor * (snapshot.getCumulativeNetworkIoMegs() - lastCumulativeNetIoMegs));
  this.lastCumulativeNetIoMegs=snapshot.getCumulativeNetworkIoMegs();
}

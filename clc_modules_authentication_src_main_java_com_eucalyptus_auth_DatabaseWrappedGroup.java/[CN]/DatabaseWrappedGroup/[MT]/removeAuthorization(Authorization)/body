{
  final FinalReturn<Boolean> ret=FinalReturn.newInstance();
  try {
    Transactions.one(this.searchGroup,new Tx<GroupEntity>(){
      @Override public void fire(      GroupEntity t) throws Throwable {
        ret.set(t.removeAuthorization(auth));
        EventRecord.here(Groups.class,EventClass.GROUP,EventType.GROUP_AUTH_REVOKED,"group=" + t.getName(),"authorization=" + auth.getDisplayName(),"for=" + auth.getValue()).info();
      }
    }
);
  }
 catch (  TransactionException e) {
    LOG.debug(e,e);
  }
  return ret.get();
}

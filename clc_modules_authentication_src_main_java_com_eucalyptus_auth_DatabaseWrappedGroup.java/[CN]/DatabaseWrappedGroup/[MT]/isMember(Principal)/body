{
  EntityWrapper<UserEntity> db=Authentication.getEntityWrapper();
  try {
    boolean ret=this.group.belongs(db.getUnique(new UserEntity(member.getName())));
    db.commit();
    return ret;
  }
 catch (  EucalyptusCloudException e) {
    LOG.debug(e,e);
    db.rollback();
    return false;
  }
}

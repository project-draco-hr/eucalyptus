def check_for_extant_osg(self, debug=False):
    obj = describeservices.DescribeServices()
    response = obj.main(filter_partition='objectstorage', filter_type='objectstorage', debug=debug)
    statuses = response.get('euca:DescribeServicesResponseType', {}).get('euca:serviceStatuses', [])
    return (len(statuses) > 0)

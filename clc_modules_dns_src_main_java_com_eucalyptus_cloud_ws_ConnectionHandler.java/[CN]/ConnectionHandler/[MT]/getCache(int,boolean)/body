{
  Cache c=null;
  if (internal) {
    c=(Cache)caches.get(new Integer(dclass));
    if (c == null) {
      c=new Cache(dclass);
      caches.put(new Integer(dclass),c);
    }
  }
 else {
    c=(Cache)publicCaches.get(new Integer(dclass));
    if (c == null) {
      c=new Cache(dclass);
      publicCaches.put(new Integer(dclass),c);
    }
  }
  return c;
}

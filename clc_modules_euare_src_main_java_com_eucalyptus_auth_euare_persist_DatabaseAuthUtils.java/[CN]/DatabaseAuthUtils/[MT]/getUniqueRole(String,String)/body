{
  @SuppressWarnings("unchecked") final RoleEntity result=(RoleEntity)Entities.createCriteria(RoleEntity.class).add(Restrictions.eq("name",roleName)).createCriteria("account").add(Restrictions.eq("name",accountName)).setCacheable(true).uniqueResult();
  if (result == null) {
    throw new NoSuchElementException("Can not find role " + roleName + " in "+ accountName);
  }
  return result;
}

{
  try {
    do {
      if (!UpgradeState.nextState().callAndLog()) {
        break;
      }
    }
 while (!UpgradeState.isFinished());
    if (BootstrapArgs.isUpgradeSystem()) {
      System.exit(0);
      return false;
    }
  }
 catch (  Throwable ex) {
    LOG.error(ex,ex);
    if (BootstrapArgs.isUpgradeSystem()) {
      System.exit(1);
    }
    return false;
  }
  return true;
}

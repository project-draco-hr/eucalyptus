{
  this.clear();
  this.add(this.systemReports,XHTML.headerWithImage("System Events",AccountingControl.RESOURCES.systemReports(),AccountingControl.RESOURCES.REPORT_BAR_STYLE),true);
  this.add(this.resourceReports,XHTML.headerWithImage("Resource Usage",AccountingControl.RESOURCES.resourceReports(),AccountingControl.RESOURCES.REPORT_BAR_STYLE),true);
  this.add(this.serviceReports,XHTML.headerWithImage("Registered Components",AccountingControl.RESOURCES.serviceReports(),AccountingControl.RESOURCES.REPORT_BAR_STYLE),true);
  makeGroupPanel();
}

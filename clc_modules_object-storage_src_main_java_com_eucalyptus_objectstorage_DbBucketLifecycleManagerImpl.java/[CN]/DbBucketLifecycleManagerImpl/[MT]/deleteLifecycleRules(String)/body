{
  try (final TransactionResource tran=Entities.transactionFor(LifecycleRule.class)){
    BucketLifecycleManagers.getInstance().deleteLifecycleRules(bucketName,tran);
    tran.commit();
  }
 catch (  Exception ex) {
    LOG.error("caught exception while deleting object lifecycle rules for bucket - " + bucketName + ", with error - "+ ex.getMessage());
    throw new ObjectStorageException("InternalServerError","An exception was caught while deleting the object lifecycle rules for bucket - " + bucketName,"Bucket",bucketName,HttpResponseStatus.INTERNAL_SERVER_ERROR);
  }
}

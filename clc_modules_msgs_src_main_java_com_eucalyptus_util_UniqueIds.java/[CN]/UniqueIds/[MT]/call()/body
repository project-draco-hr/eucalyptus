{
  Sql sql=null;
  try {
    sql=Databases.getBootstrapper().getConnection("eucalyptus","eucalyptus_config");
    sql.execute("drop table if exists config_unique_ids");
    return true;
  }
 catch (  Exception ex) {
    LOG.error(ex,ex);
    return false;
  }
 finally {
    if (sql != null) {
      sql.close();
    }
  }
}

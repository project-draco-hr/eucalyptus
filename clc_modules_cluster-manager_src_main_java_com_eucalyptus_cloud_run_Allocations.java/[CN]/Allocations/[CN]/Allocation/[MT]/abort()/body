{
  for (  ResourceToken token : this.allocationTokens) {
    EntityTransaction db=Entities.get(VmInstance.class);
    try {
      token.abort();
      db.commit();
    }
 catch (    Exception ex) {
      LOG.warn(ex.getMessage());
      Logs.exhaust().error(ex,ex);
      db.rollback();
    }
  }
}

{
  if (BootstrapArgs.isCloudController()) {
    try {
      Host localHost=Hosts.localHost();
      for (      Host h : Hosts.listDatabases()) {
        if (!h.equals(localHost) && h.getStartedTime() > localHost.getStartedTime()) {
          return false;
        }
      }
    }
 catch (    Exception ex) {
      LOG.error(ex,ex);
    }
    return true;
  }
 else {
    return false;
  }
}

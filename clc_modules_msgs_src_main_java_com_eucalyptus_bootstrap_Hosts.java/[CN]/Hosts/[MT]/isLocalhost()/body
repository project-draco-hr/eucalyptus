{
  try {
    Host maxHost=null;
    for (    Host h : Hosts.listDatabases()) {
      maxHost=(maxHost == null ? h : (maxHost.getStartedTime() < h.getStartedTime() ? h : maxHost));
    }
    return maxHost != null ? maxHost.isLocalHost() : false;
  }
 catch (  final NoSuchElementException ex) {
    return false;
  }
}

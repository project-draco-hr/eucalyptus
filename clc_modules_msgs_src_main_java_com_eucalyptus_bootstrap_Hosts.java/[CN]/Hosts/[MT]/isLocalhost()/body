{
  try {
    Host minHost=null;
    for (    Host h : Hosts.listDatabases()) {
      minHost=(minHost == null ? h : (minHost.getStartedTime() > h.getStartedTime() ? h : minHost));
    }
    return minHost != null ? minHost.isLocalHost() : false;
  }
 catch (  final NoSuchElementException ex) {
    return false;
  }
}

{
  if (input.hasBootstrapped()) {
    try {
      setup(Empyrean.class,input.getBindAddress());
      if (input.hasDatabase()) {
        setup(Eucalyptus.class,input.getBindAddress());
      }
      return true;
    }
 catch (    Exception ex) {
      LOG.error(ex,ex);
      return false;
    }
  }
 else {
    return false;
  }
}

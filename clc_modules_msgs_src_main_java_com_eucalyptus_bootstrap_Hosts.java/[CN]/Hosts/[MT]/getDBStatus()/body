{
  final List<DBStatus> status=Lists.newArrayList();
  for (  final String cluster : Databases.listRegisteredDatabases()) {
    status.add(new DBStatus(cluster,Databases.listPrimaryActiveDatabases(cluster),Databases.listSecondaryActiveDatabases(cluster),Databases.listInactiveDatabases(cluster)));
  }
  return ImmutableList.copyOf(status);
}

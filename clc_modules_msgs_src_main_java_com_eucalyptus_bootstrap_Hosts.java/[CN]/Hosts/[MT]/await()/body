{
  Host coord=get();
  if (!BootstrapArgs.isCloudController()) {
    Coordinator.loggedWait(JoinShouldWait.NON_CLOUD_CONTROLLER);
    return JoinShouldWait.NON_CLOUD_CONTROLLER.get();
  }
 else {
    Coordinator.loggedWait(JoinShouldWait.NON_CLOUD_CONTROLLER);
    if (coord == null) {
      return Hosts.localHost();
    }
 else {
      return coord;
    }
  }
}

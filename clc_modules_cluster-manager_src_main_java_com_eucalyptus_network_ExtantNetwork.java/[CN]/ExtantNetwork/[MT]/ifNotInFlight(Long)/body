{
  final NetworkIndexKey key=new NetworkIndexKey(getTag(),index);
  final Long reservedTime=inFlightNetworkIndexes.putIfAbsent(key,System.currentTimeMillis());
  return reservedTime == null || (reservedTime + TimeUnit.MINUTES.toMillis(1) < System.currentTimeMillis() && inFlightNetworkIndexes.replace(key,reservedTime,System.currentTimeMillis()));
}

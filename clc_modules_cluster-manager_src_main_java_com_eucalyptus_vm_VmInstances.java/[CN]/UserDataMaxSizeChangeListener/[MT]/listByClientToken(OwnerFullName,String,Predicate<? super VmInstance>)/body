{
  return list(new Supplier<List<VmInstance>>(){
    @Override public List<VmInstance> get(){
      return Entities.query(VmInstance.withToken(ownerFullName,clientToken));
    }
  }
,Predicates.and(CollectionUtils.propertyPredicate(clientToken,VmInstanceFilterFunctions.CLIENT_TOKEN),RestrictedTypes.filterByOwner(ownerFullName),checkPredicate(predicate)));
}

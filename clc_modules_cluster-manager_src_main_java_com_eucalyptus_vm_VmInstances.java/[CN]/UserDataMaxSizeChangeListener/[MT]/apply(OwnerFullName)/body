{
  final EntityTransaction db=Entities.get(VmInstance.class);
  final long i;
  try {
    i=((Number)Entities.createCriteria(VmInstance.class).add(Example.create(VmInstance.named(input,null))).setReadOnly(true).setCacheable(false).setProjection(Projections.rowCount()).uniqueResult()).longValue();
  }
  finally {
    db.rollback();
  }
  return i;
}

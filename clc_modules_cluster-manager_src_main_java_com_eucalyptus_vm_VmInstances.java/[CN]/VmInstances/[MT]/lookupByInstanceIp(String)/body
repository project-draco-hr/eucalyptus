{
  EntityTransaction db=Entities.get(VmInstance.class);
  try {
    VmInstance vm=Iterables.find(list(),vmWithPublicAddress(ip));
    db.commit();
    return vm;
  }
 catch (  Exception ex) {
    Logs.exhaust().error(ex,ex);
    db.rollback();
    throw new NoSuchElementException(ex.getMessage());
  }
}

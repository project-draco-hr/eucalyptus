{
  final EntityTransaction db=Entities.get(VmInstance.class);
  final long i;
  try {
    i=(Long)Entities.createCriteria(VmInstance.class).add(Example.create(VmInstance.named(input,null))).setReadOnly(true).setCacheable(false).setProjection(Projections.rowCount()).uniqueResult();
  }
  finally {
    db.rollback();
  }
  return (long)i;
}

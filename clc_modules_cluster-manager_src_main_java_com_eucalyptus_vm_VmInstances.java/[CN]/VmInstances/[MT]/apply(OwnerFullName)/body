{
  final EntityTransaction db=Entities.get(VmInstance.class);
  final int i;
  try {
    i=(Integer)Entities.createCriteria(VmInstance.class).add(Example.create(VmInstance.named(input,null))).setReadOnly(true).setCacheable(false).setProjection(Projections.rowCount()).uniqueResult();
  }
  finally {
    db.rollback();
  }
  return (long)i;
}

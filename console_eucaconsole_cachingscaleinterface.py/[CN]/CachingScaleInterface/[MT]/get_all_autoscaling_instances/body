def get_all_autoscaling_instances(self, instance_ids=None, max_records=None, next_token=None, callback=None):
    params = {'instance_ids': instance_ids, 'max_records': max_records, 'next_token': next_token, }
    Threads.instance().runThread(self.__get_all_autoscaling_instances_cb__, (params, callback))

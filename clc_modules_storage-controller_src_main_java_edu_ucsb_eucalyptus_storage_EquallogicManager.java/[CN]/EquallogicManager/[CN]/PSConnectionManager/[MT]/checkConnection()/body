{
  eucalyptusUserName=System.getProperty("euca.user");
  if (eucalyptusUserName == null) {
    LOG.error("Unable to get property eucalyptus username");
    enabled=false;
  }
  if (!enabled) {
    checkConnection();
    if (!enabled) {
      LOG.error("Not enabled. Will not run command. ");
      return;
    }
  }
  addUser(TARGET_USERNAME);
}

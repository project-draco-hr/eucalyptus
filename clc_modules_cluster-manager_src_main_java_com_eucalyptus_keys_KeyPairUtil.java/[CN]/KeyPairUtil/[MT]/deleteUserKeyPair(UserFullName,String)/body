{
  EntityTransaction db=Entities.get(SshKeyPair.class);
  SshKeyPair key=null;
  try {
    key=Entities.uniqueResult(new SshKeyPair(userFullName,keyName));
    Entities.delete(key);
    db.commit();
  }
 catch (  Exception e) {
    db.rollback();
    throw new EucalyptusCloudException("Failed to find key pair: " + keyName,e);
  }
  return key;
}

{
  boolean tagFound=false;
  try {
    final List<TagInfo> tags=EucalyptusActivityTasks.getInstance().describeTags(Lists.newArrayList("value"),Lists.newArrayList(ImagingServiceProperties.DEFAULT_LAUNCHER_TAG));
    if (tags != null) {
      for (      final TagInfo tag : tags) {
        if (ImagingServiceProperties.DEFAULT_LAUNCHER_TAG.equals(tag.getValue())) {
          if ("security-group".equals(tag.getResourceType())) {
            tagFound=true;
            break;
          }
        }
      }
    }
  }
 catch (  final Exception ex) {
    tagFound=true;
  }
  return tagFound;
}

{
  String returnValue;
  returnValue=SystemUtil.run(new String[]{WalrusProperties.eucaHome + WalrusProperties.EUCA_ROOT_WRAPPER,"drbdadm","status"});
  if (returnValue.length() == 0) {
    LifecycleEvents.fireExceptionEvent(Components.lookup(Walrus.class).getLocalServiceConfiguration(),ServiceChecks.Severity.INFO,new EucalyptusCloudException("drbdadm not found: Is drbd installed?"));
  }
}

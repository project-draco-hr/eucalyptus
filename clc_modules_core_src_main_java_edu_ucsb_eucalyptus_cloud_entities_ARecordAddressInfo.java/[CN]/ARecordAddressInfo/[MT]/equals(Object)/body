{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  ARecordAddressInfo other=(ARecordAddressInfo)obj;
  if (this.address == null) {
    if (other.address != null)     return false;
  }
 else   if (!this.address.equals(other.address))   return false;
  if (this.nameinfo == null) {
    if (other.nameinfo != null)     return false;
  }
 else   if (!this.nameinfo.equals(other.nameinfo))   return false;
  return true;
}

{
  try {
    if (!isConnected()) {
      throw new EucalyptusCloudException("Resource is not connected to peer.");
    }
    if (!isUpToDate()) {
      throw new EucalyptusCloudException("Resource is not up to date");
    }
  }
 catch (  ExecutionException ex) {
    throw new EucalyptusCloudException(ex);
  }
}

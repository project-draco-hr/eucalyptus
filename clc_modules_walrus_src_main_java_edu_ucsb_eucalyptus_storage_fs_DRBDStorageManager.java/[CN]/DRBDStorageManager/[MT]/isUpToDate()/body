{
  String dstateString=getDataStatus();
  String[] dstateParts=dstateString.split("/");
  if (dstateParts.length > 1) {
    if (DSTATE_UPTODATE.equals(dstateParts[0])) {
      return true;
    }
 else {
      return false;
    }
  }
 else {
    throw new EucalyptusCloudException("Unable to get resource dstate.");
  }
}

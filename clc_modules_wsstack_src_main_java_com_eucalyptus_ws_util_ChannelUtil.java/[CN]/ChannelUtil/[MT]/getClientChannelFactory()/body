{
  canHas.lock();
  try {
    if (clientSocketFactory == null) {
      clientSocketFactory=new NioClientSocketChannelFactory(ChannelUtil.getClientBossThreadPool(),ChannelUtil.getClientWorkerThreadPool(),Runtime.getRuntime().availableProcessors() * 2);
    }
  }
  finally {
    canHas.unlock();
  }
  return clientSocketFactory;
}

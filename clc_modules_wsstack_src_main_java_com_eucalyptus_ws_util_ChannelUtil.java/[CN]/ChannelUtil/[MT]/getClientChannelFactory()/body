{
  canHas.lock();
  try {
    if (clientSocketFactory == null) {
      clientSocketFactory=new NioClientSocketChannelFactory(ChannelUtil.getSharedBossThreadPool(),ChannelUtil.getClientWorkerThreadPool(),Runtime.getRuntime().availableProcessors() * (2 + 1));
    }
  }
  finally {
    canHas.unlock();
  }
  return clientSocketFactory;
}

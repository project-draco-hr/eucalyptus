{
  canHas.lock();
  try {
    if (clientBossThreadPool == null) {
      clientBossThreadPool=new OrderedMemoryAwareThreadPoolExecutor(CLIENT_POOL_MAX_THREADS,CLIENT_POOL_MAX_MEM_PER_CONN,CLIENT_POOL_TOTAL_MEM,CLIENT_POOL_TIMEOUT_MILLIS,TimeUnit.MILLISECONDS);
    }
  }
  finally {
    canHas.unlock();
  }
  return clientBossThreadPool;
}

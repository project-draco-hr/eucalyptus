{
  canHas.lock();
  try {
    if (serverWorkerThreadPool == null) {
      serverWorkerThreadPool=new OrderedMemoryAwareThreadPoolExecutor(SERVER_POOL_MAX_THREADS,SERVER_POOL_MAX_MEM_PER_CONN,SERVER_POOL_TOTAL_MEM,SERVER_POOL_TIMEOUT_MILLIS,TimeUnit.MILLISECONDS);
    }
  }
  finally {
    canHas.unlock();
  }
  return serverWorkerThreadPool;
}

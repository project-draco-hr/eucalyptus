{
  try {
    if (!msg.get_return()) {
      LOG.debug(EventRecord.here(AssignAddressCallback.class,Address.State.assigned,LogUtil.FAIL,LogUtil.lineObject(parentAddr)));
    }
 else {
      LOG.debug(EventRecord.here(AssignAddressCallback.class,Address.State.assigned,LogUtil.lineObject(parentAddr)));
    }
  }
  finally {
    this.parentAddr.clearPending();
  }
}

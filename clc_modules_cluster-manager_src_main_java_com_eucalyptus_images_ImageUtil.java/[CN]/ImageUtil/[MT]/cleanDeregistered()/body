{
  EntityWrapper<ImageInfo> db=new EntityWrapper<ImageInfo>();
  try {
    List<ImageInfo> imgList=db.query(Images.exampleWithImageState(Image.State.deregistered));
    for (    ImageInfo deregImg : imgList) {
      try {
        db.delete(deregImg);
      }
 catch (      Throwable e1) {
      }
    }
    db.commit();
  }
 catch (  Throwable e1) {
    db.rollback();
  }
}

{
  List<ClusterInfoType> ret=Lists.newArrayList();
  String clusterName=c.getName();
  ret.add(new ClusterInfoType(c.getConfiguration().getPartition(),c.getConfiguration().getHostName() + " " + c.getConfiguration().getFullName()));
  NavigableSet<String> tagList=new ConcurrentSkipListSet<String>();
  if (tagList.size() == 1)   tagList=c.getNodeTags();
 else   tagList.retainAll(c.getNodeTags());
  return ret;
}

{
  GrantEntry e=new GrantEntry();
  match("grant");
  match("{");
  while (!peek("}")) {
    if (peek("Permission")) {
      CryptoPermissionEntry pe=parsePermissionEntry(processedPermissions);
      e.add(pe);
      match(";");
    }
 else {
      throw new ParsingException(st.lineno(),"expected permission entry");
    }
  }
  match("}");
  return e;
}

{
  boolean found=false;
switch (lookahead) {
case StreamTokenizer.TT_WORD:
    if (expect.equalsIgnoreCase(st.sval))     found=true;
  break;
case StreamTokenizer.TT_NUMBER:
if (expect.equalsIgnoreCase("number")) {
  found=true;
}
break;
case ',':
if (expect.equals(",")) found=true;
break;
case '{':
if (expect.equals("{")) found=true;
break;
case '}':
if (expect.equals("}")) found=true;
break;
case '"':
if (expect.equals("\"")) found=true;
break;
case '*':
if (expect.equals("*")) found=true;
break;
case ';':
if (expect.equals(";")) found=true;
break;
default :
break;
}
return found;
}

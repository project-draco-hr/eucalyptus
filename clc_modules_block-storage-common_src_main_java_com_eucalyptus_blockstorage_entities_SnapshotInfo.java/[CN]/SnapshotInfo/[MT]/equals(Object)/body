{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  SnapshotInfo other=(SnapshotInfo)obj;
  if (scName == null) {
    if (other.scName != null)     return false;
  }
 else   if (!scName.equals(other.scName))   return false;
  if (snapshotId == null) {
    if (other.snapshotId != null)     return false;
  }
 else   if (!snapshotId.equals(other.snapshotId))   return false;
  return true;
}

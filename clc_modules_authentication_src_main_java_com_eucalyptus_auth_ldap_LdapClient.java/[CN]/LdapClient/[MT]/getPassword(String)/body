{
  try {
    Matcher matcher=ENCRYPTED_PATTERN.matcher(licCred);
    if (matcher.matches()) {
      return sc.decryptOpenssl(matcher.group(1),matcher.group(2));
    }
 else {
      return licCred;
    }
  }
 catch (  GeneralSecurityException e) {
    LOG.error(e,e);
    throw new LdapException("Decryption failure",e);
  }
}

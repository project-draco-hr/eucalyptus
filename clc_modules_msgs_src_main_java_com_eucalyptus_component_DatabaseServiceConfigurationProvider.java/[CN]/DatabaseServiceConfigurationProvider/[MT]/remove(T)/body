{
  EntityWrapper<T> db=ServiceConfigurations.getEntityWrapper();
  try {
    T searchConfig=(T)t.getClass().newInstance();
    searchConfig.setName(t.getName());
    T exists=db.getUnique(searchConfig);
    db.delete(exists);
    db.commit();
    EventRecord.here(Configurations.class,EventClass.COMPONENT,EventType.COMPONENT_DEREGISTERED).withDetails(t.getComponent().name(),t.getName(),"host",t.getHostName());
  }
 catch (  Exception e) {
    db.rollback();
    LOG.error(e,e);
    throw new ServiceRegistrationException(e);
  }
  return t;
}

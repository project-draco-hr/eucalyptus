{
  EntityWrapper<T> db=EntityWrapper.get(type);
  List<T> existingHosts=null;
  try {
    T searchConfig=(T)type.getClass().newInstance();
    existingHosts=db.query(searchConfig);
    db.rollback();
  }
 catch (  Exception e) {
    db.rollback();
    existingHosts=Lists.newArrayList();
  }
  return existingHosts;
}

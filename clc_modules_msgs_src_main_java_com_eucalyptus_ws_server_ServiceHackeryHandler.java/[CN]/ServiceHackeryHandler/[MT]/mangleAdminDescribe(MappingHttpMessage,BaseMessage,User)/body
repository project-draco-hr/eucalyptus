{
  final String userAgent=request.getHeader(HttpHeaders.Names.USER_AGENT);
  if ((userAgent != null) && userAgent.matches(".*EucalyptusAdminAccess") && msg.getClass().getSimpleName().startsWith("Describe")) {
    msg.markUnprivileged();
  }
 else   if ((user != null)) {
    msg.setUser(user);
  }
}

{
  EntityWrapper<ImageInfo> db=EntityWrapper.get(ImageInfo.class);
  try {
    List<ImageInfo> images=db.query(new ImageInfo(Image.Type.ramdisk));
    if (images.size() > 0) {
      db.commit();
      return images.get(0).getDisplayName();
    }
 else {
      db.commit();
      return null;
    }
  }
 catch (  Exception ex) {
    db.rollback();
    LOG.error(ex,ex);
    return null;
  }
}

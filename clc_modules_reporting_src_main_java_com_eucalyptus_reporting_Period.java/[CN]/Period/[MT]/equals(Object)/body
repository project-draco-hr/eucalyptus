{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  Period other=(Period)obj;
  if (beginningMs == null) {
    if (other.beginningMs != null)     return false;
  }
 else   if (!beginningMs.equals(other.beginningMs))   return false;
  if (endingMs == null) {
    if (other.endingMs != null)     return false;
  }
 else   if (!endingMs.equals(other.endingMs))   return false;
  return true;
}

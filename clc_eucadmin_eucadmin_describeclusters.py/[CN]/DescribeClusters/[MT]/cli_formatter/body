def cli_formatter(self, data):
    data = data['euca:DescribeClustersResponseType']
    data = data['euca:DescribeComponentsResponseType']
    data = data['euca:registered']
    for cluster in data:
        print ('CLUSTER\t%s\t%s\t%s\t%s\t%s' % (cluster['euca:partition'], cluster['euca:name'], cluster['euca:hostName'], cluster['euca:state'], cluster['euca:detail']))

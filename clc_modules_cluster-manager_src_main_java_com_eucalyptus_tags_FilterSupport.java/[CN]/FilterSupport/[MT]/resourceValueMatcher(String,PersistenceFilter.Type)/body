{
  final Object value=type.valueFunction().apply(filterPattern);
  return value == null ? Predicates.<Set<T>>alwaysFalse() : new Predicate<Set<T>>(){
    @SuppressWarnings("SuspiciousMethodCalls") @Override public boolean apply(    final Set<T> values){
      return values.contains(value);
    }
  }
;
}

{
  lic=config;
  if (Bootstrap.isFinished()) {
    if (lic.isSyncEnabled() && lic.isAutoSync()) {
      ListenerRegistry.getInstance().register(ClockTick.class,TIMER_LISTENER);
    }
 else {
      ListenerRegistry.getInstance().deregister(ClockTick.class,TIMER_LISTENER);
    }
    doForceSync();
  }
}

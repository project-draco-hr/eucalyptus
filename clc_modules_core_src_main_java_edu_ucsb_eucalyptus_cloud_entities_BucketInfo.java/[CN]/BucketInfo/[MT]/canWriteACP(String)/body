{
  if (globalWriteACP) {
    return true;
  }
  if (ownerId.equals(userId)) {
    return true;
  }
  for (  GrantInfo grantInfo : grants) {
    if (grantInfo.getUserId().equals(userId)) {
      if (grantInfo.isWriteACP()) {
        return true;
      }
    }
  }
  if (UserManagement.isAdministrator(userId)) {
    return true;
  }
  return false;
}

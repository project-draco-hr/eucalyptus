{
  if (Topology.guard().tryEnable(config)) {
    try {
      return super.apply(config);
    }
 catch (    Exception ex) {
      Topology.guard().tryDisable(config);
      throw Exceptions.toUndeclared(ex);
    }
  }
 else {
    throw new IllegalStateException("Failed to ENABLE " + config.getFullName());
  }
}

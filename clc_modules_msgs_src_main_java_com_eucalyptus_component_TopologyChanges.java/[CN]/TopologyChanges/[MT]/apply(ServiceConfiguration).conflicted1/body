{
  try {
    Components.lookup(input.getComponentId()).setup(input);
    return ServiceTransitions.pathTo(input,State.NOTREADY).get();
  }
 catch (  InterruptedException ex) {
    Thread.currentThread().interrupt();
    throw Exceptions.toUndeclared(ex);
  }
catch (  Exception ex) {
    throw Exceptions.toUndeclared(ex);
  }
}

{
  if (interceptor != null && interceptor instanceof LogMonitorInterceptor) {
    return interceptor;
  }
 else   if (interceptor != null && !(interceptor instanceof LogMonitorInterceptor)) {
    final Interceptor i=new LogMonitorInterceptor();
    interceptor=i;
    return i;
  }
 else {
    final Interceptor i=new LogMonitorInterceptor();
    return i;
  }
}

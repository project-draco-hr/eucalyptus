{
  if (PolicySpec.qualifiedName(PolicySpec.VENDOR_EC2,PolicySpec.EC2_RESOURCE_VOLUME).equals(resourceType) && (PolicySpec.qualifiedName(PolicySpec.VENDOR_EC2,PolicySpec.EC2_CREATEVOLUME).equals(action) || PolicySpec.qualifiedName(PolicySpec.VENDOR_EC2,PolicySpec.EC2_RUNINSTANCES).equals(action))) {
    return true;
  }
  return false;
}

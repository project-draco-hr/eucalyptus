{
switch (type) {
case UNKNOWN:
    return "unknown";
case NXDOMAIN:
  return "NXDOMAIN";
case NXRRSET:
return "NXRRSET";
case DELEGATION:
return "delegation: " + data;
case CNAME:
return "CNAME: " + data;
case DNAME:
return "DNAME: " + data;
case SUCCESSFUL:
return "successful";
default :
return null;
}
}

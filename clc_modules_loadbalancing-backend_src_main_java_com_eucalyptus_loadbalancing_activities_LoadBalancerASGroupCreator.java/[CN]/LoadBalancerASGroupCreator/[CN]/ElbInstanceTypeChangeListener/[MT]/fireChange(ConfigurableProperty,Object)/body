{
  try {
    if (newValue instanceof String) {
      if (newValue.equals(""))       throw new EucalyptusCloudException("Instance type cannot be unset");
      if (t.getValue() != null && !t.getValue().equals(newValue))       onPropertyChange(null,(String)newValue,null);
    }
  }
 catch (  final Exception e) {
    throw new ConfigurablePropertyException("Could not change instance type",e);
  }
}

{
  if (CloudMetadatas.isMachineImageIdentifier(LoadBalancerASGroupCreator.LOADBALANCER_EMI)) {
    if (CheckCounter == 3) {
      try {
        final List<ImageDetails> emis=EucalyptusActivityTasks.getInstance().describeImages(Lists.newArrayList(LoadBalancerASGroupCreator.LOADBALANCER_EMI));
        if (LoadBalancerASGroupCreator.LOADBALANCER_EMI.equals(emis.get(0).getImageId()))         EmiCheckResult=true;
 else         EmiCheckResult=false;
      }
 catch (      final Exception ex) {
        EmiCheckResult=false;
      }
      CheckCounter=0;
    }
 else     CheckCounter++;
    return EmiCheckResult;
  }
 else {
    imageNotConfiguredFaultRunnable.run();
    return false;
  }
}

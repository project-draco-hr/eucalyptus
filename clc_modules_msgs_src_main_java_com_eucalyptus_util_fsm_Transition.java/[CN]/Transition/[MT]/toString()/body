{
  String actionName="null";
  if (this.action != null) {
    actionName=this.action.getClass().getName().replaceAll("^(\\w.)*","");
  }
  return String.format("Transition parent=%s name=%s from=%s/%s to=%s/%s action=%s",this.getName(),this.getFromState(),this.getFromStateMark(),this.getToState(),this.getToStateMark(),"" + this.action);
}

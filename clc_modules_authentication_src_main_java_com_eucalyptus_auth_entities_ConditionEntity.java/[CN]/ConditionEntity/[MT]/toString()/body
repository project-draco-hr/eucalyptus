{
  StringBuilder sb=new StringBuilder();
  sb.append("Condition(");
  sb.append("ID=").append(this.getId()).append(", ");
  sb.append("type=").append(this.getType()).append(", ");
  sb.append("key=").append(this.getKey()).append(", ");
  sb.append("values=").append(this.getValues());
  sb.append(")");
  return sb.toString();
}

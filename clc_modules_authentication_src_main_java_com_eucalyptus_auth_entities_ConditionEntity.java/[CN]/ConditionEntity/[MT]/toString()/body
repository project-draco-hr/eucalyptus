{
  StringBuilder sb=new StringBuilder();
  sb.append("Condition(");
  sb.append("ID=").append(this.getId()).append(", ");
  sb.append("type=").append(this.type).append(", ");
  sb.append("key=").append(this.key).append(", ");
  sb.append("value=").append(this.values);
  sb.append(")");
  return sb.toString();
}

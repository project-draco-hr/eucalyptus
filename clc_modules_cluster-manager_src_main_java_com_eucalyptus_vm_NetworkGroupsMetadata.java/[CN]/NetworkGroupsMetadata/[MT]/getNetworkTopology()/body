{
  if (topoString.get() != null && checkInterval()) {
    return topoString.get();
  }
 else {
    lock.lock();
    try {
      if (topoString.get() != null && checkInterval()) {
        return topoString.get();
      }
 else {
        StringBuilder buf=generateTopology();
        topoString.set(buf.toString());
        lastTime=System.currentTimeMillis();
      }
      return topoString.get();
    }
  finally {
      lock.unlock();
    }
  }
}

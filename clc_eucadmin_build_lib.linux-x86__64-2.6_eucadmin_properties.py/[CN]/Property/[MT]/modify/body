def modify(self, modify_list):
    if modify_list:
        for i in modify_list:
            new_prop = split(i, '=')
            if (not (len(new_prop) == 2)):
                print ('ERROR Options must be of the form KEY=VALUE.  Illegally formatted option: %s' % i)
                sys.exit(1)
            self._modify(new_prop[0], new_prop[1])

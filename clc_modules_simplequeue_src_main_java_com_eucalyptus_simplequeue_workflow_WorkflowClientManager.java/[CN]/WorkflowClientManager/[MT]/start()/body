{
  final AmazonSimpleWorkflow simpleWorkflowClient=Config.buildClient(SimpleQueueAWSCredentialsProvider.SimpleQueueUserSupplier.INSTANCE,SimpleQueueService.SWF_CLIENT_CONFIG);
  workflowClient=new WorkflowClient(SimpleQueue.class,simpleWorkflowClient,SimpleQueueService.SWF_DOMAIN,SimpleQueueService.SWF_TASKLIST,SimpleQueueService.SWF_WORKFLOW_WORKER_CONFIG,SimpleQueueService.SWF_ACTIVITY_WORKER_CONFIG);
  workflowClient.start();
  LOG.debug("SimpleQueueService SWF client has started");
}

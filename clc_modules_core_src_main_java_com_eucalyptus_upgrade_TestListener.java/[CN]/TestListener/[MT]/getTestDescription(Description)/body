{
  TestDescription d;
  try {
    Class testClass=Class.forName(description.getClassName());
    d=(TestDescription)testClass.getDeclaredMethod(description.getMethodName(),new Class[]{}).getAnnotation(TestDescription.class);
    return d;
  }
 catch (  Throwable e) {
    LOG.error(e,e);
    return null;
  }
}

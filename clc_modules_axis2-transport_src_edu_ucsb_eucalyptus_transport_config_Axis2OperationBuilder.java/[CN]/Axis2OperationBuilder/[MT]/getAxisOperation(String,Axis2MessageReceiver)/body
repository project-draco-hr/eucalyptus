{
  if (Mep.IN_OUT.equals(msgReceiver.getProperties().getMep()))   return getInOutOperation(operationName,msgReceiver);
 else   if (Mep.IN_ONLY.equals(msgReceiver.getProperties().getMep()))   return getInOnlyOperation(operationName,msgReceiver);
  return getInOutOperation(operationName,msgReceiver);
}

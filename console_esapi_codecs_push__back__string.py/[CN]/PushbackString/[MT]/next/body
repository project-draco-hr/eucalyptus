def next(self):
    '\n        Retrieves the next character in the string, according to the current \n        index.\n        '
    if (self._pushback is not None):
        save = self._pushback
        self._pushback = None
        return save
    if (self._input is None):
        return None
    if (len(self._input) == 0):
        return None
    if (self._index >= len(self._input)):
        return None
    ret = self._input[self._index]
    self._index += 1
    return ret

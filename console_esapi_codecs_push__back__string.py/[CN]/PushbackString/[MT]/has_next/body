def has_next(self):
    '\n        Returns True if there is another character accesible by next(), False\n        if not.\n        '
    if (self._pushback is not None):
        return True
    if (self._input is None):
        return False
    if (len(self._input) == 0):
        return False
    if (self._index >= len(self._input)):
        return False
    return True

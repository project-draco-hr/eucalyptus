{
  if (param instanceof IMarshallable) {
    if (bindingFactory == null)     try {
      bindingFactory=BindingDirectory.getFactory(this.name,param.getClass());
    }
 catch (    JiBXException e) {
      LOG.error(e,e);
      throw new RuntimeException(this.bindingErrorMsg);
    }
    return (mappedChild(param,factory));
  }
 else   if (param == null)   throw new RuntimeException("Cannot bind null value");
 else   throw new RuntimeException("No JiBX <mapping> defined for class " + param.getClass());
}

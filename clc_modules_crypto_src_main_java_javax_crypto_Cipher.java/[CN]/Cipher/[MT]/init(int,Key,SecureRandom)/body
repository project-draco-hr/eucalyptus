{
  initialized=false;
  checkOpmode(opmode);
  if (spi != null) {
    spi.engineInit(opmode,key,random);
  }
 else {
    try {
      chooseProvider(I_KEY,opmode,key,null,null,random);
    }
 catch (    InvalidAlgorithmParameterException e) {
      throw new InvalidKeyException(e);
    }
  }
  initialized=true;
  this.opmode=opmode;
}

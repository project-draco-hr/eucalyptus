{
switch (type) {
case I_KEY:
    thisSpi.engineInit(opmode,key,random);
  break;
case I_PARAMSPEC:
thisSpi.engineInit(opmode,key,paramSpec,random);
break;
case I_PARAMS:
thisSpi.engineInit(opmode,key,params,random);
break;
case I_CERT:
thisSpi.engineInit(opmode,key,random);
break;
default :
throw new AssertionError("Internal Cipher error: " + type);
}
}

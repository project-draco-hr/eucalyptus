{
  try (TransactionResource db=Entities.transactionFor(StackEntity.class)){
    Criteria criteria=Entities.createCriteria(StackEntity.class).add(Restrictions.eq("accountId",input.getAccountNumber())).add(Restrictions.eq("recordDeleted",Boolean.FALSE));
    List<StackEntity> entityList=criteria.list();
    long retVal=entityList == null ? 0L : entityList.size();
    db.rollback();
    return retVal;
  }
 }

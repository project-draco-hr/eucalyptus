{
  final OwnerFullName name;
switch (scope) {
case Account:
    name=AccountFullName.getInstance(id);
  break;
case Group:
return NOT_SUPPORTED;
case User:
return NOT_SUPPORTED;
default :
throw new AuthException("Invalid scope");
}
return Long.toString(RestrictedTypes.quantityMetricFunction(CloudFormationMetadata.StackMetadata.class).apply(name) + quantity);
}

{
  if (this.parent != null) {
    return;
  }
 else {
    Class targetType=c;
    do {
      this.parent=targetType;
    }
 while ((targetType=targetType.getSuperclass()) != null && !BaseMessage.class.equals(targetType.getSuperclass()) && !java.lang.Object.class.equals(targetType.getSuperclass()));
  }
}

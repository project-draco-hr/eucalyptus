{
  return requestUser.isSystemAdmin() || requestUser.getUserId().equals(user.getUserId()) || (requestUser.getAccount().getAccountNumber().equals(account.getAccountNumber()) && (requestUser.isAccountAdmin() || Permissions.isAuthorized(PolicySpec.VENDOR_IAM,PolicySpec.IAM_RESOURCE_USER,Accounts.getUserFullName(user),account,PolicySpec.IAM_LISTSIGNINGCERTIFICATES,requestUser)));
}

{
  boolean allowed=requestUser.isSystemAdmin() || (requestUser.getAccount().getAccountNumber().equals(account.getAccountNumber()) && (requestUser.isAccountAdmin() || Permissions.isAuthorized(PolicySpec.VENDOR_IAM,PolicySpec.IAM_RESOURCE_GROUP,Accounts.getGroupFullName(group),account,PolicySpec.IAM_DELETEGROUP,requestUser)));
  if (!allowed) {
    throw new AuthException("Failed to authorize group deletion");
  }
}

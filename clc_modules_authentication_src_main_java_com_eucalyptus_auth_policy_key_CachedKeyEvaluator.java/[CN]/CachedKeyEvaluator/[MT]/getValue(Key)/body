{
  String value=cache.get(key.getClass());
  if (value == null) {
    final PolicyKey policyKey=Ats.from(key.getClass()).get(PolicyKey.class);
    value=policyKey == null ? null : evaluatedKeys.get(policyKey.value());
    if (value == null) {
      value=key.value();
    }
    cache.put(key.getClass(),value);
  }
  return value;
}

{
  try {
    EntityTransaction db=Entities.get(ObjectEntity.class);
    try {
      ObjectEntity searchExample=new ObjectEntity();
      Criteria search=Entities.createCriteria(ObjectEntity.class);
      List results=search.add(Example.create(searchExample)).add(Restrictions.or(ObjectEntity.QueryHelpers.getDeletedRestriction(),ObjectEntity.QueryHelpers.getFailedRestriction())).list();
      db.commit();
      return (List<ObjectEntity>)results;
    }
  finally {
      if (db != null && db.isActive()) {
        db.rollback();
      }
    }
  }
 catch (  NoSuchElementException e) {
  }
catch (  Exception e) {
    LOG.error("Error fetching failed or deleted object records");
    throw e;
  }
  return new ArrayList<ObjectEntity>(0);
}

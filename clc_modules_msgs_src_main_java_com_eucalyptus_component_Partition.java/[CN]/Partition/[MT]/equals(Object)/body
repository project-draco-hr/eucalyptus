{
  if (this == obj) {
    return true;
  }
  if (!super.equals(obj)) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  Partition other=(Partition)obj;
  if (this.name == null) {
    if (other.name != null) {
      return false;
    }
  }
 else   if (!this.name.equals(other.name)) {
    return false;
  }
  return true;
}

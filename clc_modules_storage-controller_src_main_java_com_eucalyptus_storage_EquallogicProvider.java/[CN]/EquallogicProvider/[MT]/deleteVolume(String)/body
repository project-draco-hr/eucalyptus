{
  if (!enabled) {
    checkConnection();
    if (!enabled) {
      LOG.error("Not enabled. Will not run command. ");
      return false;
    }
  }
  try {
    if (volumeExists(volumeName)) {
      String returnValue=execCommand("stty hardwrap off\rvolume select " + volumeName + " offline\rvolume delete "+ volumeName+ "\r");
      if (returnValue.split(VOLUME_DELETE_PATTERN.toString()).length > 1)       return true;
 else       return false;
    }
 else {
      LOG.error("Volume not found: " + volumeName);
      return true;
    }
  }
 catch (  EucalyptusCloudException e) {
    LOG.error(e);
    return false;
  }
}

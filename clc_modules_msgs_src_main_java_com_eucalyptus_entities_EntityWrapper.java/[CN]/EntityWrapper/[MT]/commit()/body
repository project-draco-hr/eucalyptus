{
  if (TRACE)   EventRecord.here(EntityWrapper.class,EventType.PERSISTENCE,DbEvent.COMMIT.begin(),this.tx.getTxUuid()).trace();
  try {
    this.tx.commit();
  }
 catch (  RuntimeException e) {
    if (TRACE)     EventRecord.here(EntityWrapper.class,EventType.PERSISTENCE,DbEvent.COMMIT.fail(),Long.toString(this.tx.splitOperation()),this.tx.getTxUuid()).trace();
    PersistenceErrorFilter.exceptionCaught(e);
    throw e;
  }
catch (  Throwable e) {
    if (TRACE)     EventRecord.here(EntityWrapper.class,EventType.PERSISTENCE,DbEvent.COMMIT.fail(),Long.toString(this.tx.splitOperation()),this.tx.getTxUuid()).trace();
    PersistenceErrorFilter.exceptionCaught(e);
    throw new RuntimeException(e);
  }
  if (TRACE)   EventRecord.here(EntityWrapper.class,EventType.PERSISTENCE,DbEvent.COMMIT.end(),Long.toString(this.tx.splitOperation()),this.tx.getTxUuid()).trace();
}

{
  final Example qbe=Example.create(example).enableLike(MatchMode.EXACT);
  final List<T> resultList=this.getSession().createCriteria(example.getClass()).setLockMode(LockMode.NONE).setResultTransformer(Criteria.DISTINCT_ROOT_ENTITY).setCacheable(true).add(qbe).setReadOnly(readOnly).list();
  return Lists.newArrayList(Sets.newHashSet(resultList));
}

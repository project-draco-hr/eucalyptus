{
  try {
    newObject=this.getEntityManager().merge(newObject);
    this.commit();
    return newObject;
  }
 catch (  RuntimeException ex) {
    PersistenceErrorFilter.exceptionCaught(ex);
    this.rollback();
    throw ex;
  }
catch (  Throwable ex) {
    LOG.error(ex,ex);
    this.rollback();
    throw new RecoverablePersistenceException(ex);
  }
}

{
  if (LogLevels.EXTREME)   LOG.debug(Join.join(":",EventType.PERSISTENCE,DbEvent.ROLLBACK.begin(),this.tx.getTxUuid()));
  try {
    this.tx.rollback();
  }
 catch (  Throwable e) {
    if (LogLevels.EXTREME)     LOG.debug(Join.join(":",EventType.PERSISTENCE,DbEvent.ROLLBACK.fail(),Long.toString(this.tx.splitOperation()),this.tx.getTxUuid()));
    PersistenceErrorFilter.exceptionCaught(e);
  }
  if (LogLevels.EXTREME)   LOG.debug(Join.join(":",EventType.PERSISTENCE,DbEvent.ROLLBACK.end(),Long.toString(this.tx.splitOperation()),this.tx.getTxUuid()));
}

{
  if (TRACE)   EventRecord.here(EntityWrapper.class,EventType.PERSISTENCE,DbEvent.ROLLBACK.begin(),this.tx.getTxUuid()).trace();
  try {
    this.tx.rollback();
  }
 catch (  Throwable e) {
    if (TRACE)     EventRecord.here(EntityWrapper.class,EventType.PERSISTENCE,DbEvent.ROLLBACK.fail(),Long.toString(this.tx.splitOperation()),this.tx.getTxUuid()).trace();
    PersistenceErrorFilter.exceptionCaught(e);
  }
  if (TRACE)   EventRecord.here(EntityWrapper.class,EventType.PERSISTENCE,DbEvent.ROLLBACK.end(),Long.toString(this.tx.splitOperation()),this.tx.getTxUuid()).trace();
}

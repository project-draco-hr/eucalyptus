def getDevices():
    'Get a list of devices\n\n    :return: A list of devices\n    :rtype: list of strings\n\n    .. note::\n\n       This function does not filter out any devices such as bridges or\n       slaves.  Higher level functions should do this.\n    '
    devices = set()
    po = subprocess.Popen('ip -4 -o link show'.split(), stdout=subprocess.PIPE)
    out = po.communicate()[0]
    for line in out.strip().split('\n'):
        m = re.match('(\\d+):\\s+(\\S+):\\s+.*', line)
        if (not m):
            pass
        else:
            devices.add(m.groups()[1])
    return devices

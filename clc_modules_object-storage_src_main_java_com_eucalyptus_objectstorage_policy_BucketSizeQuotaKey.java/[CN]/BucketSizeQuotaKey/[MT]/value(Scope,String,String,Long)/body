{
  String bucketName=resource.split("/",2)[0];
switch (scope) {
case ACCOUNT:
    return Long.toString(toMb(ObjectStorageQuotaUtil.getBucketSize(bucketName) + quantity));
case GROUP:
  return NOT_SUPPORTED;
case USER:
return Long.toString(toMb(ObjectStorageQuotaUtil.getBucketSize(bucketName) + quantity));
}
throw new AuthException("Invalid scope");
}

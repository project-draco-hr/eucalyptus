{
switch (scope) {
case ACCOUNT:
    return Long.toString(toMb(ObjectStorageQuotaUtil.countBucketSize(resource) + quantity));
case GROUP:
  return NOT_SUPPORTED;
case USER:
return Long.toString(toMb(ObjectStorageQuotaUtil.countBucketSize(resource) + quantity));
}
throw new AuthException("Invalid scope");
}

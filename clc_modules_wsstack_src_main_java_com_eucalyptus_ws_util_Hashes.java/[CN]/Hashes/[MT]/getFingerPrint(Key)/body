{
  try {
    byte[] fp=Digest.SHA1.get().digest(privKey.getEncoded());
    StringBuffer sb=new StringBuffer();
    for (    byte b : fp)     sb.append(String.format("%02X:",b));
    return sb.substring(0,sb.length() - 1).toLowerCase();
  }
 catch (  Exception e) {
    LOG.error(e,e);
    return null;
  }
}

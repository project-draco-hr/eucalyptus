{
  return requestUser.isSystemUser() && Permissions.isAuthorized(requestUser.evaluationContext(VENDOR_IAM,IAM_RESOURCE_ACCOUNT,IAM_LISTACCOUNTPOLICIES),account.getAccountNumber(),Accounts.getAccountFullName(account)) && Permissions.isAuthorized(requestUser.evaluationContext(VENDOR_IAM,IAM_RESOURCE_ACCOUNT,IAM_GETACCOUNTPOLICY),account.getAccountNumber(),Accounts.getAccountFullName(account));
}

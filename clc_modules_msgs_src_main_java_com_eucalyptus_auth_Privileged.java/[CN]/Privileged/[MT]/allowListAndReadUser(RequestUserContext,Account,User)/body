{
  return requestUser.getUserId().equals(user.getUserId()) || (Permissions.isAuthorized(requestUser.evaluationContext(VENDOR_IAM,IAM_RESOURCE_USER,IAM_GETUSER),account.getAccountNumber(),Accounts.getUserFullName(user)) && Permissions.isAuthorized(requestUser.evaluationContext(VENDOR_IAM,IAM_RESOURCE_USER,IAM_LISTUSERS),account.getAccountNumber(),Accounts.getUserFullName(user)));
}

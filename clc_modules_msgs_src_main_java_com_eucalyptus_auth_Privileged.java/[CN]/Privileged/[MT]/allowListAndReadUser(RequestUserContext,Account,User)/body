{
  return requestUser.isSystemAdmin() || requestUser.getUserId().equals(user.getUserId()) || (requestUser.getAccountNumber().equals(account.getAccountNumber()) && (Permissions.isAuthorized(requestUser.evaluationContext(PolicySpec.VENDOR_IAM,PolicySpec.IAM_RESOURCE_USER,PolicySpec.IAM_GETUSER),account.getAccountNumber(),Accounts.getUserFullName(user)) && Permissions.isAuthorized(requestUser.evaluationContext(PolicySpec.VENDOR_IAM,PolicySpec.IAM_RESOURCE_USER,PolicySpec.IAM_LISTUSERS),account.getAccountNumber(),Accounts.getUserFullName(user))));
}

{
  return requestUser.isSystemUser() && Permissions.isAuthorized(requestUser.evaluationContext(VENDOR_IAM,IAM_RESOURCE_ACCOUNT,IAM_LISTACCOUNTPOLICIES),account.getAccountNumber(),account.getName()) && Permissions.isAuthorized(requestUser.evaluationContext(VENDOR_IAM,IAM_RESOURCE_ACCOUNT,IAM_GETACCOUNTPOLICY),account.getAccountNumber(),account.getName());
}

{
  return requestUser.isSystemAdmin() || (requestUser.getAccountNumber().equals(account.getAccountNumber()) && Permissions.isAuthorized(requestUser.evaluationContext(PolicySpec.VENDOR_IAM,PolicySpec.IAM_RESOURCE_INSTANCE_PROFILE,PolicySpec.IAM_LISTINSTANCEPROFILES),account.getAccountNumber(),Accounts.getInstanceProfileFullName(instanceProfile)));
}

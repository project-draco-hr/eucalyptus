{
  return requestUser.isSystemAdmin() || (requestUser.getAccount().getAccountNumber().equals(account.getAccountNumber()) && (requestUser.isAccountAdmin() || (!user.isAccountAdmin() && Permissions.isAuthorized(PolicySpec.VENDOR_IAM,PolicySpec.IAM_RESOURCE_USER,Accounts.getUserFullName(user),account,PolicySpec.IAM_LISTUSERPOLICIES,requestUser))));
}

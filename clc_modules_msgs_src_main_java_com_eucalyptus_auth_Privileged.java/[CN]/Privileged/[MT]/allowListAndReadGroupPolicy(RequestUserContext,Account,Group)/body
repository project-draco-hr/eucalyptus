{
  return !group.isUserGroup() && Permissions.isAuthorized(requestUser.evaluationContext(VENDOR_IAM,IAM_RESOURCE_GROUP,IAM_LISTGROUPPOLICIES),account.getAccountNumber(),Accounts.getGroupFullName(group)) && Permissions.isAuthorized(requestUser.evaluationContext(VENDOR_IAM,IAM_RESOURCE_GROUP,IAM_GETGROUPPOLICY),account.getAccountNumber(),Accounts.getGroupFullName(group));
}

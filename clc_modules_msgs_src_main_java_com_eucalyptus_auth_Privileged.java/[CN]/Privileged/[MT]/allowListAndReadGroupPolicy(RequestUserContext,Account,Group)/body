{
  return requestUser.isSystemAdmin() || (requestUser.getAccountNumber().equals(account.getAccountNumber()) && (requestUser.isAccountAdmin() || (!group.isUserGroup() && Permissions.isAuthorized(requestUser.evaluationContext(PolicySpec.VENDOR_IAM,PolicySpec.IAM_RESOURCE_GROUP,PolicySpec.IAM_LISTGROUPPOLICIES),account.getAccountNumber(),Accounts.getGroupFullName(group)) && Permissions.isAuthorized(requestUser.evaluationContext(PolicySpec.VENDOR_IAM,PolicySpec.IAM_RESOURCE_GROUP,PolicySpec.IAM_GETGROUPPOLICY),account.getAccountNumber(),Accounts.getGroupFullName(group)))));
}

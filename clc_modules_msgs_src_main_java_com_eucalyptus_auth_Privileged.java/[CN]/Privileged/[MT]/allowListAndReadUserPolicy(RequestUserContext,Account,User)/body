{
  return !user.isAccountAdmin() && Permissions.isAuthorized(requestUser.evaluationContext(VENDOR_IAM,IAM_RESOURCE_USER,IAM_LISTUSERPOLICIES),account.getAccountNumber(),Accounts.getUserFullName(user)) && Permissions.isAuthorized(requestUser.evaluationContext(VENDOR_IAM,IAM_RESOURCE_USER,IAM_GETUSERPOLICY),account.getAccountNumber(),Accounts.getUserFullName(user));
}

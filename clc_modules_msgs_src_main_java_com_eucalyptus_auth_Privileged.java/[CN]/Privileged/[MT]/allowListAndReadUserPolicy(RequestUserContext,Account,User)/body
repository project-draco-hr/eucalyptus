{
  return requestUser.isSystemAdmin() || (requestUser.getAccountNumber().equals(account.getAccountNumber()) && (requestUser.isAccountAdmin() || (!user.isAccountAdmin() && Permissions.isAuthorized(requestUser.evaluationContext(PolicySpec.VENDOR_IAM,PolicySpec.IAM_RESOURCE_USER,PolicySpec.IAM_LISTUSERPOLICIES),account.getAccountNumber(),Accounts.getUserFullName(user)) && Permissions.isAuthorized(requestUser.evaluationContext(PolicySpec.VENDOR_IAM,PolicySpec.IAM_RESOURCE_USER,PolicySpec.IAM_GETUSERPOLICY),account.getAccountNumber(),Accounts.getUserFullName(user)))));
}

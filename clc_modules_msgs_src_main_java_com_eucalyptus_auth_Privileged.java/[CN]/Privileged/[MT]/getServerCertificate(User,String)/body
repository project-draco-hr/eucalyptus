{
  final Account acct=requestUser.getAccount();
  if (!requestUser.isSystemAdmin()) {
    if (!Permissions.isAuthorized(PolicySpec.VENDOR_IAM,PolicySpec.IAM_GETSERVERCERTIFICATE,certName,acct,PolicySpec.IAM_GETSERVERCERTIFICATE,requestUser))     throw new AuthException(AuthException.ACCESS_DENIED);
  }
  try {
    return acct.lookupServerCertificate(certName);
  }
 catch (  final AuthException ex) {
    throw ex;
  }
catch (  final Exception ex) {
    throw ex;
  }
}

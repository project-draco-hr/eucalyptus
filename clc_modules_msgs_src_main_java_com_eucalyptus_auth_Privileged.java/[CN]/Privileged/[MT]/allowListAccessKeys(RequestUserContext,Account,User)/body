{
  if (!requestUser.isSystemAdmin()) {
    if (!requestUser.getAccountNumber().equals(account.getAccountNumber())) {
      return false;
    }
    if (!Permissions.isAuthorized(requestUser.evaluationContext(PolicySpec.VENDOR_IAM,PolicySpec.IAM_RESOURCE_USER,PolicySpec.IAM_LISTACCESSKEYS),account.getAccountNumber(),Accounts.getUserFullName(user))) {
      return false;
    }
  }
  return true;
}

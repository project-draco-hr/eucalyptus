{
  if (!requestUser.isSystemUser() || !Permissions.isAuthorized(VENDOR_IAM,IAM_RESOURCE_ACCOUNT,Accounts.getAccountFullName(account),account,IAM_DELETEACCOUNTPOLICY,requestUser)) {
    throw new AuthException(AuthException.ACCESS_DENIED);
  }
  User admin=account.lookupAdmin();
  admin.removePolicy(name);
}

{
  final EntityTransaction db=Entities.get(VmInstance.class);
  try {
    final VmInstance entityObj=Entities.merge(arg0);
    db.commit();
    return entityObj;
  }
 catch (  final RuntimeException ex) {
    Logs.extreme().error(ex,ex);
    throw ex;
  }
 finally {
    if (db.isActive())     db.rollback();
  }
}

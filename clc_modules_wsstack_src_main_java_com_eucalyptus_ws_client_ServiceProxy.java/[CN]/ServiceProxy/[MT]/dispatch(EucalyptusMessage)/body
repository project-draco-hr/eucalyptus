{
  MuleEvent context=RequestContext.getEvent();
  try {
    if (component.isLocal()) {
      this.getMuleClient().dispatch(this.address.toASCIIString(),msg,null);
    }
 else {
      this.getNioClient().dispatch(msg);
    }
  }
 catch (  Exception e) {
    LOG.error(e);
  }
 finally {
    RequestContext.setEvent(context);
  }
}

{
  try {
    if (input.isDirectory()) {
      LOG.info("Cleaning up class cache: " + input.getCanonicalPath());
      Iterables.all(Arrays.asList(input.listFiles()),DeleteRecursively.PREDICATE);
      input.delete();
    }
 else {
      input.delete();
    }
  }
 catch (  SecurityException ex) {
    LOG.error(ex);
    throw ex;
  }
catch (  Exception ex) {
    LOG.error(ex);
  }
  return true;
}

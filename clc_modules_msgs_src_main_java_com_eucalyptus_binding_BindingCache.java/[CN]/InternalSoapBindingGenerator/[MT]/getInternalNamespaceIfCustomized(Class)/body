{
  String namespace=null;
  final ComponentId.ComponentMessage componentMessage=Ats.inClassHierarchy(klass).get(ComponentId.ComponentMessage.class);
  if (componentMessage != null) {
    try {
      final String namespaceSuffix=componentMessage.value().newInstance().getInternalNamespaceSuffix();
      if (namespaceSuffix != null) {
        namespace=ns + namespaceSuffix;
      }
    }
 catch (    Exception e) {
      Logs.extreme().error(e,e);
    }
  }
  return namespace;
}

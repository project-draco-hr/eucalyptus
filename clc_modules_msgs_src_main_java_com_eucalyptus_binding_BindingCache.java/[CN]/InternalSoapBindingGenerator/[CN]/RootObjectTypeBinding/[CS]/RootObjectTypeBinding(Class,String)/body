{
  InternalSoapBindingGenerator.this.indent=2;
  this.type=type;
  this.namespace=namespace;
  if (Object.class.equals(type.getSuperclass())) {
    this.abs=true;
  }
 else   if (type.getSuperclass().getSimpleName().equals("EucalyptusData")) {
    this.abs=true;
  }
 else {
    this.abs=false;
  }
}

{
  HoldMe.canHas.lock();
  try {
    xpath=XPathFactory.newInstance().newXPath();
    docFactory=DocumentBuilderFactory.newInstance();
    try {
      docBuilder=docFactory.newDocumentBuilder();
    }
 catch (    ParserConfigurationException ex) {
      ex.printStackTrace();
    }
  }
  finally {
    HoldMe.canHas.unlock();
  }
}

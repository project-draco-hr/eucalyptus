{
  EntityWrapper<NetworkRulesGroup> db=NetworkGroupUtil.getEntityWrapper();
  NetworkRulesGroup group=null;
  try {
    group=db.getUnique(new NetworkRulesGroup(userFullName,groupName));
    db.commit();
  }
 catch (  EucalyptusCloudException e) {
    db.rollback();
    throw e;
  }
  return group;
}

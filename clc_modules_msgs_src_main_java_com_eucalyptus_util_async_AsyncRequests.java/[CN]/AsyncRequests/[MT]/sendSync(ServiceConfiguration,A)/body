{
  try {
    return newRequest(new MessageCallback<A,B>(){
      @Override public void fire(      B msg){
        LOG.debug(msg);
      }
    }
).sendSync(config);
  }
 catch (  ExecutionException ex) {
    LOG.error(ex,ex);
    throw ex.getCause();
  }
catch (  InterruptedException ex) {
    Thread.currentThread().interrupt();
    throw ex;
  }
}

{
  if (config.isVmLocal()) {
    return ServiceContext.send(config.getComponentId(),msg);
  }
 else {
    Request<A,B> req=newRequest(new MessageCallback<A,B>(){
{
        this.setRequest(msg);
      }
      @Override public void fire(      B msg){
        Logs.extreme().debug(msg.toSimpleString());
      }
    }
);
    return req.dispatch(config);
  }
}

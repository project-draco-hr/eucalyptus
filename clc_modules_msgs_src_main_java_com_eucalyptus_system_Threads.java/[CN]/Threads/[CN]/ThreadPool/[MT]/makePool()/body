{
  ExecutorService newPool=(this.numThreads == -1) ? Executors.newFixedThreadPool(NUM_QUEUE_WORKERS,this) : Executors.newFixedThreadPool(this.numThreads,this);
  if (newPool instanceof ThreadPoolExecutor) {
    ((ThreadPoolExecutor)newPool).setRejectedExecutionHandler(new ThreadPoolExecutor.CallerRunsPolicy());
  }
  return newPool;
}

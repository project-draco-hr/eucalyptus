{
  if (this.pool != null) {
    return this.pool;
  }
 else {
synchronized (this) {
      if ((this.pool == null) && (this.numThreads == -1)) {
        this.pool=Executors.newCachedThreadPool(this);
      }
 else {
        this.pool=Executors.newFixedThreadPool(this.numThreads);
      }
    }
    return this;
  }
}

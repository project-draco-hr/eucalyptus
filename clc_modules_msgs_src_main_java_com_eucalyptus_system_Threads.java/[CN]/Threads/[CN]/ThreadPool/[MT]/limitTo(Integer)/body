{
  if (this.numThreads.equals(numThreads)) {
    return this;
  }
 else {
synchronized (this) {
      if (this.numThreads.equals(numThreads)) {
        return this;
      }
 else {
        this.numThreads=numThreads;
        final ExecutorService oldExec=this.pool;
        this.pool=null;
        if (oldExec != null) {
          oldExec.shutdown();
        }
        this.pool=this.makePool();
      }
    }
  }
  return this;
}

def get_tags(self):
    ret = {}
    index = 1
    name_p = 'Tag.%d.Key'
    value_p = 'Tag.%d.Value'
    done = False
    while (not done):
        name = self.get_argument((name_p % index), None)
        if (not name):
            done = True
            break
        val = self.get_argument((value_p % index), None)
        if val:
            ret[name] = val
        index += 1
    return ret

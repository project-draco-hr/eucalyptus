{
  EntityWrapper<ClusterConfiguration> db=Configuration.getEntityWrapper();
  try {
    List<ClusterConfiguration> componentList=db.query(new ClusterConfiguration());
    db.commit();
    return componentList;
  }
 catch (  Exception e) {
    db.rollback();
    LOG.error(e,e);
    throw new EucalyptusCloudException(e);
  }
}

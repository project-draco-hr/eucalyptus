{
  if (this.timestamp.compareTo(that.timestamp) < 0) {
    return -1;
  }
 else   if (this.timestamp.compareTo(that.timestamp) > 0) {
    return 1;
  }
  if (this.metricValue.compareTo(that.metricValue) < 0) {
    return -1;
  }
 else   if (this.metricValue.compareTo(that.metricValue) > 0) {
    return 1;
  }
  return 0;
}

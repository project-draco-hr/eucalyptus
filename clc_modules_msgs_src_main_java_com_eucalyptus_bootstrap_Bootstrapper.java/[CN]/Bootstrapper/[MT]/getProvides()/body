{
  if (!From(this.getClass()).has(Provides.class)) {
    Exceptions.eat("Bootstrap class does not specify the component which it @Provides.  Fine.  For now we pretend you had put @Provides(Component.any) instead of System.exit(-1): " + this.getClass());
    return Component.any;
  }
 else {
    return From(this.getClass()).get(Provides.class).value();
  }
}

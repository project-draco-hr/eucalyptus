{
  if (!Ats.from(this.getClass()).has(Provides.class)) {
    Exceptions.eat("Bootstrap class does not specify the component which it @Provides.  Fine.  For now we pretend you had put @Provides(ComponentId.class) or something similar instead of System.exit(-1): " + this.getClass());
    return (Class<T>)ComponentId.class;
  }
 else {
    return (Class<T>)Ats.from(this.getClass()).get(Provides.class).value();
  }
}

{
  return new Predicate<ReportingInstanceUsageEvent>(){
    @Override public boolean apply(    final ReportingInstanceUsageEvent event){
      if (event == null || event.getTimestampMs() == null || event.getDimension() == null || event.getMetric() == null || event.getSequenceNum() == null || event.getUuid() == null || event.getValue() == null) {
        log.debug("Ignoring invalid usage event: " + event);
        return true;
      }
      return callback.apply(event);
    }
  }
;
}

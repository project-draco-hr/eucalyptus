{
  return new Predicate<ReportingInstanceCreateEvent>(){
    @Override public boolean apply(    final ReportingInstanceCreateEvent event){
      if (event == null || event.getTimestampMs() == null || event.getAvailabilityZone() == null || event.getInstanceId() == null || event.getInstanceType() == null || event.getUserId() == null || event.getUuid() == null) {
        log.debug("Ignoring invalid create event: " + event);
        return true;
      }
      return callback.apply(event);
    }
  }
;
}

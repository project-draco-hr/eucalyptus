{
  if (accounts.keySet().size() == 0) {
    EntityWrapper<ReportingAccount> entityWrapper=EntityWrapper.get(ReportingAccount.class);
    try {
      @SuppressWarnings("rawtypes") List reportingAccounts=(List)entityWrapper.createQuery("from ReportingAccount").list();
      for (      Object obj : reportingAccounts) {
        ReportingAccount Account=(ReportingAccount)obj;
        accounts.put(Account.getId(),Account.getName());
      }
      entityWrapper.commit();
    }
 catch (    Exception ex) {
      LOG.error(ex);
      entityWrapper.rollback();
      throw new RuntimeException(ex);
    }
  }
}

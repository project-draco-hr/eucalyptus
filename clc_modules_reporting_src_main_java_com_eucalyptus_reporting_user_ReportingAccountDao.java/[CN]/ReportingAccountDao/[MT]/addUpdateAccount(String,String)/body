{
  loadIfNeededFromDb();
  if (accounts.containsKey(id) && accounts.get(id).equals(name)) {
    return;
  }
 else   if (accounts.containsKey(id)) {
    updateInDb(id,name);
    accounts.put(id,name);
  }
 else {
    try {
      addToDb(id,name);
      accounts.put(id,name);
    }
 catch (    RuntimeException e) {
      LOG.trace(e,e);
    }
  }
}

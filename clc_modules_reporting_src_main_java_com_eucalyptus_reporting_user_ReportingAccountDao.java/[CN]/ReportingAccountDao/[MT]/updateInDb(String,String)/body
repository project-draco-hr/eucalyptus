{
  EntityWrapper<ReportingAccount> entityWrapper=EntityWrapper.get(ReportingAccount.class);
  try {
    ReportingAccount reportingAccount=(ReportingAccount)entityWrapper.createQuery("from ReportingAccount where id = ?").setString(0,id).uniqueResult();
    reportingAccount.setName(name);
    entityWrapper.commit();
  }
 catch (  Exception ex) {
    log.error(ex);
    entityWrapper.rollback();
    throw new RuntimeException(ex);
  }
}

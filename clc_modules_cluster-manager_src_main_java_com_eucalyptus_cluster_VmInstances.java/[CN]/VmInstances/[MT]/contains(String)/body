{
  final EntityTransaction db=Entities.get(VmInstance.class);
  try {
    final VmInstance vm=Entities.uniqueResult(VmInstance.named(null,name));
    db.commit();
    return true;
  }
 catch (  RuntimeException ex) {
    db.rollback();
    return false;
  }
catch (  TransactionException ex) {
    db.rollback();
    return false;
  }
}

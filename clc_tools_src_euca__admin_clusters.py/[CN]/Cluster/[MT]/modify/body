def modify(self, partition, name, modify_list):
    for entry in modify_list:
        (key, value) = entry.split('=')
        try:
            reply = self.euca.connection.get_object('ModifyClusterAttribute', {'Partition': partition, 'Name': name, 'Attribute': key, 'Value': value, }, BooleanResponse)
            print reply
        except EC2ResponseError as ex:
            self.euca.handle_error(ex)

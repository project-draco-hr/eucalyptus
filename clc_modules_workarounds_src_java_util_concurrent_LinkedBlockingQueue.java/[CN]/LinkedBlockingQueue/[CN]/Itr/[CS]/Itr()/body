{
  final ReentrantLock putLock=LinkedBlockingQueue.this.putLock;
  final ReentrantLock takeLock=LinkedBlockingQueue.this.takeLock;
  putLock.lock();
  takeLock.lock();
  try {
    current=head.next;
    if (current != null)     currentElement=current.item;
  }
  finally {
    takeLock.unlock();
    putLock.unlock();
  }
}

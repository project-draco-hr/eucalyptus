{
  EventRecord.here(ServiceBuilder.class,EventType.SERVICE_TRANSITION,this.name(),parent.lookupState().toString(),parent.getFullName().toString(),parent.toString()).debug();
  try {
    ServiceTransitions.processTransition(parent,transitionCallback,this);
  }
 catch (  Throwable ex) {
    if (ServiceExceptions.filterExceptions(parent,ex)) {
      transitionCallback.fireException(ex);
      throw new RuntimeException(ex);
    }
 else {
      transitionCallback.fire();
    }
  }
}

{
  try {
    this.updateState();
    this.address.clearPending();
  }
 catch (  IllegalStateException e) {
    AddressCategory.unassign(address).dispatch(address.getCluster());
  }
catch (  Exception e) {
    LOG.debug(e,e);
    AddressCategory.unassign(address).dispatch(address.getCluster());
  }
}

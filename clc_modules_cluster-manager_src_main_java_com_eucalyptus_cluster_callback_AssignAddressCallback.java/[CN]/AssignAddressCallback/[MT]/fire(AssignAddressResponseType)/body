{
  try {
    this.updateState();
  }
 catch (  IllegalStateException e) {
    AsyncRequests.newRequest(address.unassign().getCallback()).dispatch(address.getPartition());
  }
catch (  Exception e) {
    LOG.debug(e,e);
    AsyncRequests.newRequest(address.unassign().getCallback()).dispatch(address.getPartition());
  }
}

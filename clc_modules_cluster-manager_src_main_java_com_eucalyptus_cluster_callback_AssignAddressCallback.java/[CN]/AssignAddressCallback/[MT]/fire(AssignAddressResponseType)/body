{
  try {
    this.updateState();
  }
 catch (  IllegalStateException e) {
    LOG.debug(e,e);
    if (this.vm != null) {
      AsyncRequests.newRequest(address.unassign().getCallback()).dispatch(this.vm.getPartition());
    }
  }
catch (  Exception e) {
    LOG.debug(e,e);
    if (this.vm != null) {
      AsyncRequests.newRequest(address.unassign().getCallback()).dispatch(this.vm.getPartition());
    }
  }
}

{
  try {
    if (!this.checkVmState()) {
      this.clearState();
    }
 else {
      this.address.clearPending();
      EventRecord.here(AssignAddressCallback.class,EventType.ADDRESS_ASSIGNED,Address.State.assigned.toString(),this.address.toString()).info();
    }
  }
 catch (  Exception e) {
    LOG.debug(e,e);
    this.clearState();
  }
}

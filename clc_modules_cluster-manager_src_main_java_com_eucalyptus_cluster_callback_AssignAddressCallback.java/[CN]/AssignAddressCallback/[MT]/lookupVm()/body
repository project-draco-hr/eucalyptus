{
  try {
    VmInstance foundVm=VmInstances.lookup(super.getRequest().getInstanceId());
    if (VmStateSet.RUN.apply(foundVm)) {
      return foundVm;
    }
 else {
      return null;
    }
  }
 catch (  TerminatedInstanceException ex) {
    return null;
  }
catch (  NoSuchElementException ex) {
    return null;
  }
}

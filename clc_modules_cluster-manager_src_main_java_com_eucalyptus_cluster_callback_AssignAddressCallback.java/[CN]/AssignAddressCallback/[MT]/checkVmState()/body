{
  try {
    if (this.vm != null) {
      VmInstance foundVm=VmInstances.lookup(this.vm.getInstanceId());
      if (!VmStateSet.RUN.apply(foundVm)) {
        return false;
      }
 else {
        return true;
      }
    }
 else {
      return false;
    }
  }
 catch (  NoSuchElementException e) {
    LOG.debug(e,e);
    return false;
  }
}

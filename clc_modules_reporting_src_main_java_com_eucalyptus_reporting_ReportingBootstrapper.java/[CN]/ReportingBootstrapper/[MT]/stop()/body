{
  try {
    instanceListener.flush();
    timer.cancel();
    storagePoller.writeEvents();
    queueFactory.shutdown();
    queueBroker.shutdown();
    log.info("ReportingBootstrapper stopped");
    return true;
  }
 catch (  Exception ex) {
    log.error("ReportingBootstrapper failed to stop",ex);
    return false;
  }
}

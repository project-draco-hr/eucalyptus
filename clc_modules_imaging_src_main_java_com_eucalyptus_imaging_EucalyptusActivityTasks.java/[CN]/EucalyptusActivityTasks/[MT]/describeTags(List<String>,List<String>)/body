{
  final DescribeTagsTask task=new DescribeTagsTask(names,values);
  final CheckedListenableFuture<Boolean> result=task.dispatch(new EucalyptusSystemActivity());
  try {
    if (result.get()) {
      return task.getTags();
    }
 else     throw new EucalyptusActivityException("failed to describe tags");
  }
 catch (  Exception ex) {
    throw Exceptions.toUndeclared(ex);
  }
}

{
  final AutoScalingUpdateGroupTask task=new AutoScalingUpdateGroupTask(groupName,zones,capacity,launchConfigName);
  final CheckedListenableFuture<Boolean> result=task.dispatch(new AutoScalingSystemActivity());
  try {
    if (result.get()) {
      return;
    }
 else     throw new EucalyptusActivityException("failed to enable zones in autoscaling group");
  }
 catch (  Exception ex) {
    throw Exceptions.toUndeclared(ex);
  }
}

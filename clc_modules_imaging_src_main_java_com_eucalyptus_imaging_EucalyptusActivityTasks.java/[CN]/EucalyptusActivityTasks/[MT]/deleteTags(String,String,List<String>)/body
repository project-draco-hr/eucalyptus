{
  final EucaDeleteTagsTask task=new EucaDeleteTagsTask(tagKey,tagValue,resources);
  final CheckedListenableFuture<Boolean> result=task.dispatch(new EucalyptusSystemActivity());
  try {
    if (result.get()) {
      return;
    }
 else     throw new EucalyptusActivityException("failed to delete tags");
  }
 catch (  Exception ex) {
    throw Exceptions.toUndeclared(ex);
  }
}

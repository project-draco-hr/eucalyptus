{
  try {
    E msg=clazz.newInstance();
    msg.setUserId(Component.eucalyptus.name());
    msg.setEffectiveUserId(Component.eucalyptus.name());
    StackTraceElement elem=new Throwable().fillInStackTrace().getStackTrace()[1];

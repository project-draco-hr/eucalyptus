{
  StringBuilder sb=new StringBuilder();
  sb.append("Cert(");
  sb.append("ID=").append(this.getId()).append(", ");
  sb.append("active=").append(this.isActive()).append(", ");
  sb.append("revoked=").append(this.isRevoked()).append(", ");
  sb.append("pem=").append(this.getPem());
  sb.append(")");
  return sb.toString();
}

def initialize(self):
    logging.info('initialized websocket handler')
    session_id = dict(re.findall('(?P<name>.*?)=(?P<value>.*?);? ', (self.request.headers['Cookie'] + '; ')))['session-id']
    logging.info(('session-id = ' + session_id))
    eucaconsole.sessions[session_id].push_handler = self
    push_handler = self
    self._lock = threading.Condition()
    self._timer = None
    self._queue = []

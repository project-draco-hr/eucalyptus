{
  Stack stack=null;
  try (TransactionResource db=Entities.transactionFor(StackEntity.class)){
    Criteria criteria=Entities.createCriteria(StackEntity.class).add(Restrictions.eq("accountId",accountId)).add(Restrictions.eq("stackName",stackName));
    List<StackEntity> entityList=criteria.list();
    if (entityList != null && !entityList.isEmpty()) {
      stack=stackEntityToStack(entityList.get(0));
    }
    db.commit();
  }
   return stack;
}

{
  StackEntity stackEntity=null;
  try (TransactionResource db=Entities.transactionFor(StackEntity.class)){
    Criteria criteria=Entities.createCriteria(StackEntity.class).add(Restrictions.eq("accountId",accountId)).add(Restrictions.eq("stackId",stackId)).add(Restrictions.eq("recordDeleted",Boolean.FALSE));
    List<StackEntity> entityList=criteria.list();
    if (entityList != null && !entityList.isEmpty()) {
      stackEntity=entityList.get(0);
    }
  }
   return stackEntity;
}

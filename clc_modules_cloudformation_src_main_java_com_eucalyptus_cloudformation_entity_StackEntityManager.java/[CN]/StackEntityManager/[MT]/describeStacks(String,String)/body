{
  List<StackEntity> returnValue=Lists.newArrayList();
  try (TransactionResource db=Entities.transactionFor(StackEntity.class)){
    Criteria criteria=Entities.createCriteria(StackEntity.class).add(Restrictions.eq("accountId",accountId)).add(Restrictions.or(Restrictions.eq("stackName",stackNameOrId),Restrictions.eq("stackId",stackNameOrId))).add(Restrictions.eq("recordDeleted",Boolean.FALSE));
    returnValue=criteria.list();
    db.commit();
  }
   return returnValue;
}

{
  LOG.info("Enabling cluster: " + config);
  EventRecord.here(ClusterBuilder.class,EventType.COMPONENT_SERVICE_ENABLED,config.getComponentId().name(),config.getName(),ServiceUris.remote(config).toASCIIString()).info();
  try {
    try {
      final Cluster newCluster=Clusters.getInstance().lookupDisabled(config.getName());
      newCluster.enable();
    }
 catch (    final NoSuchElementException ex) {
      final Cluster newCluster=Clusters.getInstance().lookup(config.getName());
      newCluster.enable();
    }
  }
 catch (  final NoSuchElementException ex) {
    LOG.error(ex,ex);
  }
}

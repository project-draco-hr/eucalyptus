{
  try {
    Clusters.lookup(config).check();
  }
 catch (  final NoSuchElementException ex) {
    throw Faults.failure(config,ex);
  }
catch (  final IllegalStateException ex) {
    Logs.exhaust().error(ex,ex);
    throw Faults.failure(config,ex);
  }
catch (  final Exception ex) {
    Logs.exhaust().error(ex,ex);
    throw Faults.failure(config,ex);
  }
}

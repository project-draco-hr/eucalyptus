{
  try {
    CredentialProvider.getUser("admin");
    return true;
  }
 catch (  NoSuchUserException e) {
    try {
      CredentialProvider.addUser("admin",true,true);
      return true;
    }
 catch (    UserExistsException e1) {
      LOG.fatal(e1,e1);
      return false;
    }
catch (    UnsupportedOperationException e1) {
      LOG.fatal(e1,e1);
      return false;
    }
  }
}

{
  final EntityTransaction db=Entities.get(Volume.class);
  Volume volume=null;
  try {
    volume=Entities.uniqueResult(Volume.named(ownerFullName,volumeId));
    db.commit();
  }
 catch (  final Exception ex) {
    LOG.debug(ex,ex);
    db.rollback();
    throw Exceptions.toUndeclared(ex);
  }
  return volume;
}

{
  EntityWrapper<Volume> db=Entities.get(Volume.class);
  List<Volume> vols=db.createCriteria(Volume.class).add(Example.create(Snapshots.named(input,null))).setReadOnly(true).setCacheable(false).list();
  Long size=0l;
  for (  Volume v : vols) {
    size+=v.getSize();
  }
  db.rollback();
  return size;
}

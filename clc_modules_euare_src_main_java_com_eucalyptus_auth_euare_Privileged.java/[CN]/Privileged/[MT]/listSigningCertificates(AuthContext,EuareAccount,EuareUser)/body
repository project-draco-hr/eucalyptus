{
  if (!allowListSigningCertificates(requestUser,account,user)) {
    throw new AuthException(AuthException.ACCESS_DENIED);
  }
  List<Certificate> certs=Lists.newArrayList();
  for (  Certificate cert : user.getCertificates()) {
    if (!cert.isRevoked()) {
      certs.add(cert);
    }
  }
  return certs;
}

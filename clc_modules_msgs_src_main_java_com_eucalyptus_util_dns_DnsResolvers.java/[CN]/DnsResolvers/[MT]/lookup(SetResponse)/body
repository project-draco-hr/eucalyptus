{
  if (sr.isCNAME()) {
    return CNAME;
  }
 else   if (sr.isDelegation()) {
    return DELEGATION;
  }
 else   if (sr.isDNAME()) {
    return DNAME;
  }
 else   if (sr.isNXDOMAIN()) {
    return NXDOMAIN;
  }
 else   if (sr.isNXRRSET()) {
    return NXRRSET;
  }
 else   if (sr.isSuccessful()) {
    return SUCCESSFUL;
  }
 else {
    return UNKNOWN;
  }
}

{
  return ipPermissionType.getIpProtocol().equals("-1") && ipPermissionType.getFromPort() == null && ipPermissionType.getToPort() == null && ipPermissionType.getCidrIpRanges() != null && ipPermissionType.getCidrIpRanges().size() == 1 && ipPermissionType.getCidrIpRanges().get(0).equals("0.0.0.0/0");
}

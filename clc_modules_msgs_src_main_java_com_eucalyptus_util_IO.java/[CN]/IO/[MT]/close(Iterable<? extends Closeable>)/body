{
  final List<Optional<Throwable>> results=Lists.newArrayList();
  if (closeables != null)   for (  final Closeable closeable : closeables) {
    Optional<Throwable> error=Optional.absent();
    try {
      closeable.close();
    }
 catch (    Throwable e) {
      error=Optional.of(e);
    }
    results.add(error);
  }
  return results;
}

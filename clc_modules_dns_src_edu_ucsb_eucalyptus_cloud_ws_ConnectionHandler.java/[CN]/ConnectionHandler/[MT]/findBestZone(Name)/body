{
  Zone foundzone=null;
  foundzone=(Zone)ZoneManager.getZone(name);
  if (foundzone != null)   return foundzone;
  int labels=name.labels();
  for (int i=1; i < labels; i++) {
    Name tname=new Name(name,i);
    foundzone=(Zone)ZoneManager.getZone(tname);
    if (foundzone != null)     return foundzone;
  }
  return null;
}

{
  final StringBuilder sb=new StringBuilder();
  for (  String key : kvMap.keySet()) {
    String value=kvMap.get(key);
    sb.append(String.format("%s=%s;",key,value));
  }
  final String userData=B64.standard.encString(String.format("%s\n%s","euca-" + B64.standard.encString("setup-credential"),sb.toString()));
  return userData;
}

{
  this.networkGroup=networkGroup;
  try {
    this.tag=this.networkGroup.extantNetwork().getTag();
  }
 catch (  NotEnoughResourcesException ex) {
    this.tag=-1;
  }
  StopNetworkType msg=new StopNetworkType(this.networkGroup.getOwnerUserId(),this.networkGroup.getNaturalId(),this.tag).regarding();
  msg.setUserId(this.networkGroup.getOwnerUserId());
  this.setRequest(msg);
}

def get(self):
    action = self.get_argument('Action')
    if (action == 'About'):
        ret = {'version': '3.2.0', 'admin_url': (('https://' + server.config.get('server', 'clchost')) + ':8443'), 'help_page': server.config.get('locale', 'help.page'), 'cloud_admin_page': server.config.get('locale', 'admin.page'), }
        data = json.dumps(ret)
        self.write(data)
    else:
        self.write('What are you doing here?')

{
  try {
    ServiceConfiguration conf=builder.newInstance();
    conf.setName(name);
    ServiceConfigurations.lookup(conf);
    return true;
  }
 catch (  PersistenceException e) {
    throw Exceptions.toUndeclared(e);
  }
catch (  Exception e) {
    Logs.extreme().error(e,e);
    return true;
  }
}

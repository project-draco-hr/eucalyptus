{
  EntityWrapper<Address> db=new EntityWrapper<Address>();
  Address clusterAddr=new Address();
  clusterAddr.setCluster(cluster);
  List<Address> addrList=Lists.newArrayList();
  try {
    addrList=db.query(clusterAddr);
    db.commit();
  }
 catch (  Exception e1) {
    db.rollback();
  }
  return addrList;
}

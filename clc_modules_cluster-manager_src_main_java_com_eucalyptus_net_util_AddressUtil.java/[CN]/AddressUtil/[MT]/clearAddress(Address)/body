{
  if (!address.isPending()) {
    try {
      markAddressUnassigned(address);
      dispatchUnassignAddress(address);
    }
 catch (    Throwable e1) {
      AddressManager.LOG.debug(e1,e1);
    }
  }
 else   if (Address.UNALLOCATED_USERID.equals(address.getUserId())) {
    address.clean();
  }
}

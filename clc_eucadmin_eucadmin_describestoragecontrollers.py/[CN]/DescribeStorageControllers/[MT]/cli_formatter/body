def cli_formatter(self, data):
    data = data['euca:DescribeStorageControllersResponseType']
    data = data['euca:DescribeComponentsResponseType']
    data = data['euca:registered']
    for sc in data:
        print ('STORAGE\t%s\t%s\t%s\t%s\t%s' % (sc['euca:partition'], sc['euca:name'], sc['euca:hostName'], sc['euca:state'], sc['euca:detail']))

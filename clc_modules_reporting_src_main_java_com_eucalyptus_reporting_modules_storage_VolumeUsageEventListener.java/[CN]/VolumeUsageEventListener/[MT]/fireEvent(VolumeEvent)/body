{
  Preconditions.checkNotNull(event,"Event is required");
  final long timeInMs=getCurrentTimeMillis();
  final ReportingAccountDao reportingAccountDao=getReportingAccountDao();
  reportingAccountDao.addUpdateAccount(event.getAccountId(),event.getAccountName());
  final ReportingUserDao reportingUserDao=getReportingUserDao();
  reportingUserDao.addUpdateUser(event.getOwnerId(),event.getOwnerName());
  final ReportingVolumeEventStore eventStore=getReportingVolumeEventStore();
switch (event.getActionInfo().getAction()) {
case VOLUMECREATE:
    eventStore.insertCreateEvent(event.getUuid(),event.getDisplayName(),timeInMs,event.getOwnerId(),event.getAvailabilityZone(),event.getSizeGB());
  break;
case VOLUMEDELETE:
eventStore.insertDeleteEvent(event.getUuid(),timeInMs);
break;
case VOLUMEATTACH:
eventStore.insertAttachEvent(event.getUuid(),((InstanceActionInfo)event.getActionInfo()).getInstanceUuid(),event.getSizeGB(),timeInMs);
break;
case VOLUMEDETACH:
eventStore.insertDetachEvent(event.getUuid(),((InstanceActionInfo)event.getActionInfo()).getInstanceUuid(),event.getSizeGB(),timeInMs);
break;
}
}

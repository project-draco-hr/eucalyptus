{
  this.canHasWrite.lock();
  try {
    if (this.context != null) {
      try {
        this.context.stop();
      }
 catch (      final Exception ex) {
        LOG.error(ex,ex);
      }
      try {
        this.context.close();
      }
 catch (      final Exception ex) {
        LOG.error(ex,ex);
      }
    }
  }
  finally {
    this.canHasWrite.unlock();
  }
}

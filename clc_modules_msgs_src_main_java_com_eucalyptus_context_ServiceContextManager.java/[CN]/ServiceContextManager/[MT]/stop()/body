{
  this.canHasWrite.lock();
  try {
    if (this.context != null) {
      try {
        this.context.stop();
        this.context.dispose();
      }
 catch (      final MuleException ex) {
        LOG.error(ex,ex);
      }
      this.context=null;
    }
  }
  finally {
    this.canHasWrite.unlock();
  }
}

{
  ctxMgmtThreadPool.submit(new Callable<MuleContext>(){
    @Override public MuleContext call() throws Exception {
      try {
        while (!Bootstrap.isFinished()) {
          TimeUnit.MILLISECONDS.sleep(30);
        }
        startup();
      }
 catch (      Throwable ex) {
        LOG.error(ex,ex);
      }
      return context.getReference();
    }
  }
);
}

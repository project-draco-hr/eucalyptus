{
  singleton.update();
  String dest=service;
  if ((!service.startsWith("vm://") && !singleton.serviceToEndpoint.containsKey(service)) || (service == null)) {
    dest="vm://RequestQueue";
  }
 else   if (!service.startsWith("vm://")) {
    dest=singleton.serviceToEndpoint.get(dest);
  }
  return dest;
}

{
  if (checkStateChanged(Components.whichAreEnabledLocally())) {
    ServiceContextManager.restart();
  }
 else   if (context.getReference() == null) {
    try {
      ServiceContextManager.startup();
    }
 catch (    ServiceInitializationException ex) {
      LOG.error(ex,ex);
    }
  }
  return true;
}

{
  List<StackUpdateWorkflowSignalEntity> signals=Lists.newArrayList();
  try (TransactionResource db=Entities.transactionFor(StackUpdateWorkflowSignalEntity.class)){
    Criteria criteria=Entities.createCriteria(StackUpdateWorkflowSignalEntity.class).add(Restrictions.eq("stackId",stackId)).add(Restrictions.eq("accountId",accountId)).add(Restrictions.eq("outerStackArn",outerStackArn));
    List<StackUpdateWorkflowSignalEntity> results=criteria.list();
    if (results != null) {
      signals.addAll(results);
    }
  }
   return signals;
}

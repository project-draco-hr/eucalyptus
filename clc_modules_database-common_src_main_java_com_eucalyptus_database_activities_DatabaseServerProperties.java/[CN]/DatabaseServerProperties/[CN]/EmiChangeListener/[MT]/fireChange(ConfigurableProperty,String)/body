{
  try {
    if (t.getValue() != null && !t.getValue().equals(newValue) && newValue.length() > 0)     onPropertyChange(newValue,null,null,null,null,null,null,null);
  }
 catch (  final Exception e) {
    throw new ConfigurablePropertyException("Could not change EMI ID due to: " + e.getMessage());
  }
}

{
  try {
    if (newValue instanceof String) {
      if (newValue == null || ((String)newValue).equals(""))       throw new EucalyptusCloudException("Instance type cannot be unset");
      if (t.getValue() != null && !t.getValue().equals(newValue))       onPropertyChange(null,null,(String)newValue,null,null,null,null,null);
    }
  }
 catch (  final Exception e) {
    throw new ConfigurablePropertyException("Could not change instance type due to: " + e.getMessage());
  }
}

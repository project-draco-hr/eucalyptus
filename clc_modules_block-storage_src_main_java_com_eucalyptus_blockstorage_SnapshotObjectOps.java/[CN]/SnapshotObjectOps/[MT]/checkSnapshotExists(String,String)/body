{
  refreshOsgURI();
  try {
    s3Client.getS3Client().getObjectMetadata(bucket,key);
    return Boolean.TRUE;
  }
 catch (  AmazonServiceException aex) {
    return Boolean.FALSE;
  }
catch (  Exception ex) {
    LOG.error("Snapshot download failed for: ",ex);
    throw new EucalyptusCloudException(ex);
  }
}

{
  long now=SimpleQueueService.currentTimeSeconds();
  try (TransactionResource db=Entities.transactionFor(MessageEntity.class)){
    List<MessageEntity> messageEntities=Entities.criteriaQuery(MessageEntity.class).whereEqual(MessageEntity_.accountId,queue.getAccountId()).whereEqual(MessageEntity_.queueName,queue.getQueueName()).where(Entities.restriction(MessageEntity.class).gt(MessageEntity_.expiredTimestampSecs,now)).orderBy(MessageEntity_.sentTimestampSecs).list();
    if (messageEntities == null || messageEntities.size() == 0)     return 0L;
    return now - messageEntities.get(0).getSentTimestampSecs();
  }
 }

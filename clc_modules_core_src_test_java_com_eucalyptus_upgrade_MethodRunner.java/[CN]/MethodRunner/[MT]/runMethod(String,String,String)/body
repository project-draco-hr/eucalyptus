{
  String[] methodArgsArray=new String[0];
  if (args != null) {
    methodArgsArray=args.split(",");
    System.out.printf("Executing class:%s method:%s args:%s\n",className,methodName,args);
  }
 else {
    System.out.printf("Executing class:%s method:%s\n",className,methodName);
  }
  Class[] params=new Class[methodArgsArray.length];
  for (int j=0; j < params.length; j++) {
    params[j]=String.class;
  }
  Class clazz=Class.forName(className);
  @SuppressWarnings("unchecked") Integer retVal=(Integer)clazz.getDeclaredMethod(methodName,params).invoke(null,(Object[])methodArgsArray);
  return retVal.intValue();
}

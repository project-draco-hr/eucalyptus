{
  StringBuilder sb=new StringBuilder();
  sb.append(EventType.TRANSITION).append(" ").append(this.name).append(" Active").append(this.transition != null ? this.transition.toString() : "null").append(" id=").append(this.id).append(" startTime=").append(new Date(this.startTime));
  Logs.exhaust().info(sb.toString());
  Logs.exhaust().info(Exceptions.string(this.startStackTrace));
  return sb.toString();
}

{
  StringBuilder sb=new StringBuilder();
  sb.append(EventType.TRANSITION).append(" ").append(this.txName).append(" Active").append(this.transition != null ? this.transition.toString() : "null").append(" id=").append(this.txId).append(" startTime=").append(new Date(this.startTime));
  Logs.exhaust().trace(sb.toString());
  return sb.toString();
}

{
  try {
    this.transition.enter(AtomicMarkedState.this.parent);
    try {
      AtomicMarkedState.this.commit();
    }
 catch (    IllegalStateException t) {
      LOG.trace(t,t);
    }
  }
 catch (  Throwable t) {
    LOG.error(t,t);
    AtomicMarkedState.this.rollback();
  }
  try {
    this.transition.after(AtomicMarkedState.this.parent);
  }
 catch (  Throwable t) {
    LOG.error(t,t);
  }
}

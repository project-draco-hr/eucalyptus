{
  final ActiveTransition tid;
  try {
    tid=this.request(transitionName);
  }
 catch (  ExistingTransitionException t) {
    throw t;
  }
catch (  Throwable t) {
    this.rollback(t);
    throw Exceptions.debug(new IllegalStateException(String.format("Failed to apply transition %s because request() threw an exception.",transitionName.toString()),t));
  }
  return tid;
}

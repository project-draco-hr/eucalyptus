{
  try {
    return DomainNames.absolute(Name.fromString(Iterables.getFirst(hostSplitter.split(fullHostHeader),fullHostHeader))).subdomain(DomainNames.externalSubdomain(ObjectStorage.class)) || DomainNames.absolute(Name.fromString(Iterables.getFirst(hostSplitter.split(fullHostHeader),fullHostHeader))).subdomain(Name.fromString("walrus." + DomainNames.externalSubdomain().toString()));
  }
 catch (  Exception e) {
    LOG.error("Error parsing domain name from hostname: " + fullHostHeader,e);
    return false;
  }
}

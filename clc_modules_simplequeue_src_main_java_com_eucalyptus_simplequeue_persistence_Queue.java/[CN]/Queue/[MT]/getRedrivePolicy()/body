{
  if (!attributes.containsKey(SimpleQueueService.AttributeName.RedrivePolicy.toString())) {
    return null;
  }
 else {
    try {
      return new ObjectMapper().readTree(attributes.get(SimpleQueueService.AttributeName.RedrivePolicy.toString()));
    }
 catch (    IOException e) {
      return null;
    }
  }
}

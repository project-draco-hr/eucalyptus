{
  try {
    FileChannel out=new FileOutputStream(new File(name)).getChannel();
    for (    String partName : parts) {
      FileChannel in=new FileInputStream(new File(partName)).getChannel();
      in.transferTo(0,in.size(),out);
      in.close();
    }
    out.close();
  }
 catch (  Exception ex) {
    LOG.error(ex);
  }
}

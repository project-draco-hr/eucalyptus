{
synchronized (Fault.class) {
    PrintWriter out=null;
    try {
      out=new PrintWriter(new FileWriter(logFile,true));
      out.println(toString());
    }
 catch (    IOException ex) {
      ex.printStackTrace();
    }
 finally {
      if (out != null) {
        out.close();
      }
    }
  }
}

{
  int i=0;
  EntityTransaction db=Entities.get(ImageInfo.class);
  try {
    i=Entities.createCriteria(ImageInfo.class).add(Example.create(ImageInfo.named(input,null))).setReadOnly(true).setCacheable(false).list().size();
  }
 catch (  Exception ex) {
    LOG.error(ex,ex);
  }
 finally {
    db.rollback();
  }
  return (long)i;
}

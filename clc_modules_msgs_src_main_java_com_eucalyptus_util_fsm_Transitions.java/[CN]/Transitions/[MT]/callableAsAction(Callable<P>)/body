{
  TransitionAction<P> action=new AbstractTransitionAction<P>(){
    @Override public void leave(    P parent,    Callback.Completion transitionCallback){
      try {
        callable.call();
        transitionCallback.fire();
      }
 catch (      Throwable ex) {
        LOG.error(ex);
        transitionCallback.fireException(ex);
      }
    }
  }
;
  return action;
}

{
  TransitionAction<P> action=new AbstractTransitionAction<P>(){
    @Override public void leave(    P parent,    Completion transitionCallback){
      try {
        function.run();
        transitionCallback.fire();
      }
 catch (      Exception ex) {
        LOG.error(ex,ex);
        transitionCallback.fireException(ex);
      }
    }
  }
;
  return action;
}

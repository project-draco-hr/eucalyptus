{
  VmInstance findVm=null;
  if (!Databases.isVolatile()) {
    try {
      findVm=VmInstances.lookupByPublicIp(requestIp);
    }
 catch (    Exception ex2) {
      try {
        findVm=VmInstances.lookupByPrivateIp(requestIp);
      }
 catch (      Exception ex) {
        Logs.exhaust().error(ex);
      }
    }
  }
  return findVm;
}

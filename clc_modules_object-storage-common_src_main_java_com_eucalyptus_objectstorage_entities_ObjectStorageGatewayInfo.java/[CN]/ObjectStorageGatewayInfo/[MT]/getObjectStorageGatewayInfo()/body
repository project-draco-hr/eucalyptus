{
  EntityWrapper<ObjectStorageGatewayInfo> db=EntityWrapper.get(ObjectStorageGatewayInfo.class);
  ObjectStorageGatewayInfo gatewayInfo=null;
  try {
    gatewayInfo=db.getUnique(new ObjectStorageGatewayInfo());
  }
 catch (  Exception ex) {
    gatewayInfo=new ObjectStorageGatewayInfo(ObjectStorageProperties.NAME,ObjectStorageProperties.MAX_BUCKETS_PER_ACCOUNT,(int)(ObjectStorageProperties.MAX_BUCKET_SIZE / ObjectStorageProperties.M),(int)(ObjectStorageProperties.IMAGE_CACHE_SIZE / ObjectStorageProperties.M),ObjectStorageProperties.MAX_TOTAL_SNAPSHOT_SIZE,Integer.MAX_VALUE);
    gatewayInfo.setFailedPutTimeoutHours(ObjectStorageProperties.DEFAULT_PUT_TIMEOUT_HOURS);
    gatewayInfo.setCleanupTaskIntervalSeconds(ObjectStorageProperties.DEFAULT_CLEANUP_INTERVAL_SEC);
    db.add(gatewayInfo);
  }
 finally {
    db.commit();
  }
  return gatewayInfo;
}

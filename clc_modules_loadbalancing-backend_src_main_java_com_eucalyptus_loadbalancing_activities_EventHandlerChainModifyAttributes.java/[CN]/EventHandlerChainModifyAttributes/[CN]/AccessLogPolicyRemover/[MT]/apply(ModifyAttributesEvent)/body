{
  final AccessLog accessLog=evt.getAttributes().getAccessLog();
  if (accessLog == null || accessLog.getEnabled())   return;
  final String roleName=String.format("%s-%s-%s",EventHandlerChainNew.IAMRoleSetup.ROLE_NAME_PREFIX,evt.getContext().getAccount().getAccountNumber(),evt.getLoadBalancer());
  final String policyName=AccessLogPolicyCreator.ACCESSLOG_ROLE_POLICY_NAME;
  try {
    EucalyptusActivityTasks.getInstance().deleteRolePolicy(roleName,policyName);
  }
 catch (  final Exception ex) {
    ;
  }
}

{
  if (e instanceof MessageEvent) {
    LOG.debug(EventRecord.create(this.getClass().getSimpleName(),Component.eucalyptus.name(),"CONNECT",ctx.getChannel().getLocalAddress(),ctx.getChannel().getRemoteAddress().toString()));
  }
  if (this.getRequest() == null) {
    LOG.debug("Request is null, waiting for message to send.");
  }
 else {
    this.fireMessage(ctx.getChannel());
  }
  super.channelConnected(ctx,e);
}

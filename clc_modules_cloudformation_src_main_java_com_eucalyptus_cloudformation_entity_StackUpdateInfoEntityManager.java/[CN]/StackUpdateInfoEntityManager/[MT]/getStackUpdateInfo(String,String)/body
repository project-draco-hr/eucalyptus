{
  StackUpdateInfoEntity stackUpdateInfoEntity=null;
  try (TransactionResource db=Entities.transactionFor(StackUpdateInfoEntity.class)){
    Criteria criteria=Entities.createCriteria(StackUpdateInfoEntity.class).add(Restrictions.eq("accountId",accountId)).add(Restrictions.eq("stackId",stackId)).add(Restrictions.eq("recordDeleted",Boolean.FALSE));
    List<StackUpdateInfoEntity> stackUpdateInfoEntityList=criteria.list();
    if (stackUpdateInfoEntityList != null && !stackUpdateInfoEntityList.isEmpty()) {
      stackUpdateInfoEntity=stackUpdateInfoEntityList.get(0);
    }
    db.commit();
  }
   return stackUpdateInfoEntity;
}

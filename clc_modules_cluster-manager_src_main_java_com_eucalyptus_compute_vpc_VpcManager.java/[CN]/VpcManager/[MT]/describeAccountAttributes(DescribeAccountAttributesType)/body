{
  final DescribeAccountAttributesResponseType reply=request.getReply();
  final Context ctx=Contexts.lookup();
  final AccountFullName accountFullName=ctx.getUserFullName().asAccountFullName();
  String vpcId="none";
  try {
    vpcId=vpcs.lookupDefault(accountFullName,CloudMetadatas.toDisplayName());
  }
 catch (  VpcMetadataNotFoundException e) {
  }
catch (  Exception e) {
    throw handleException(e);
  }
  reply.getAccountAttributeSet().getItem().add(new AccountAttributeSetItemType("supported-platforms",Lists.newArrayList("EC2","VPC")));
  reply.getAccountAttributeSet().getItem().add(new AccountAttributeSetItemType("default-vpc",Lists.newArrayList(vpcId)));
  return reply;
}

{
  LOG.info("processing phase one for ruleId '" + ruleId + "' for bucket "+ bucketName+ " against objects prefixed '"+ prefix+ "', marking matches for expiration if it is now past "+ expirationDate.toString());
  List<String> expiredObjectKeys=findMatchingObjects(ruleId,bucketName,prefix,expirationDate);
  ObjectInfoProcessor processor=new ObjectInfoProcessor(expiredObjectKeys,bucketName){
    @Override public void handle(    ObjectEntity retrieved){
      retrieved.setDeletedTimestamp(new Date());
      Entities.merge(retrieved);
    }
  }
;
  processor.process();
}

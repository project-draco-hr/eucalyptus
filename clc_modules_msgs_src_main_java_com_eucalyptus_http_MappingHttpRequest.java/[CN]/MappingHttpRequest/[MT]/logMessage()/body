{
  LOG.trace("============================================");
  LOG.trace("HTTP" + this.getProtocolVersion() + " "+ this.getMethod()+ " "+ this.getUri());
  for (  String s : this.getHeaderNames()) {
    LOG.trace(s + ": " + this.getHeader(s));
  }
  LOG.trace("============================================");
  LOG.trace(this.getContent().toString("UTF-8"));
  LOG.trace("============================================");
}

{
  FilteredPipeline candidate=null;
  for (  final FilteredPipeline f : this.pipelines) {
    if (f.accepts(request)) {
      if (candidate != null) {
        throw new DuplicatePipelineException();
      }
      candidate=f;
    }
  }
  if (candidate == null) {
    throw new NoAcceptingPipelineException();
  }
  return candidate;
}

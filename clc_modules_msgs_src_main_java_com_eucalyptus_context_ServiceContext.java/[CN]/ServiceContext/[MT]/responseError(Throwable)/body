{
  try {
    Context ctx=Contexts.lookup();
    EventRecord.here(ReplyQueue.class,EventType.MSG_REPLY,cause.getClass().getCanonicalName(),cause.getMessage(),String.format("%.3f ms",(System.nanoTime() - ctx.getCreationTime()) / 1000000.0)).debug();
    Channels.fireExceptionCaught(ctx.getChannel(),cause);
    if (!(cause instanceof BaseException)) {
      Contexts.clear(ctx);
    }
  }
 catch (  Exception ex) {
    LOG.error(ex);
    LOG.error(cause,cause);
  }
}

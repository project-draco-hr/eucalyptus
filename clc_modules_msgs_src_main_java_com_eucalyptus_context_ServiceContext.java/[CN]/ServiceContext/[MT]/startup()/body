{
  try {
    LOG.info("Starting up system bus.");
    createContext();
  }
 catch (  Exception e) {
    LOG.fatal("Failed to configure services.",e);
    return false;
  }
  try {
    startContext();
  }
 catch (  Exception e) {
    LOG.fatal("Failed to start services.",e);
    return false;
  }
  return true;
}

{
  try {
    if (!super.disable())     return false;
    if (stackWorkflowWorker != null) {
      stackWorkflowWorker.shutdown();
    }
    if (stackActivityWorker != null) {
      stackActivityWorker.shutdown();
    }
    return true;
  }
 catch (  Exception ex) {
    LOG.error("Unable to disable CloudFormation Bootstrapper",ex);
    return false;
  }
}

{
  Example accountExample=Example.create(new AccountEntity(accountName)).enableLike(MatchMode.EXACT);
  @SuppressWarnings("unchecked") List<AccountEntity> accounts=(List<AccountEntity>)session.createCriteria(AccountEntity.class).setCacheable(true).add(accountExample).list();
  if (accounts.size() != 1) {
    throw new AuthException("Found " + accounts.size() + " account(s)");
  }
  return accounts.get(0);
}

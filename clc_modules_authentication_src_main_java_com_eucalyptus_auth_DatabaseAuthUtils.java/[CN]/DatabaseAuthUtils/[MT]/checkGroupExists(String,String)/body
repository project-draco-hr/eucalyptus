{
  if (groupName == null) {
    throw new AuthException(AuthException.EMPTY_GROUP_NAME);
  }
  if (accountName == null) {
    throw new AuthException(AuthException.EMPTY_ACCOUNT_NAME);
  }
  EntityWrapper<GroupEntity> db=EntityWrapper.get(GroupEntity.class);
  try {
    @SuppressWarnings("unchecked") GroupEntity result=(GroupEntity)db.createCriteria(GroupEntity.class).setCacheable(true).add(Restrictions.eq("name",groupName)).createCriteria("account").setCacheable(true).add(Restrictions.eq("name",accountName)).uniqueResult();
    db.commit();
    return result != null;
  }
 catch (  Exception e) {
    db.rollback();
    throw new AuthException("Failed to find group",e);
  }
}

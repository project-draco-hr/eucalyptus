{
switch (type) {
case TEXT:
    return new HasValueWidget(){
      private TextBox input=new TextBox();
      @Override public Widget getWidget(){
        return input;
      }
      @Override public String getValue(){
        return this.input.getValue();
      }
    }
;
case TEXTAREA:
  return new HasValueWidget(){
    private TextArea input=new TextArea();
    @Override public Widget getWidget(){
      return input;
    }
    @Override public String getValue(){
      return this.input.getValue();
    }
  }
;
default :
return null;
}
}

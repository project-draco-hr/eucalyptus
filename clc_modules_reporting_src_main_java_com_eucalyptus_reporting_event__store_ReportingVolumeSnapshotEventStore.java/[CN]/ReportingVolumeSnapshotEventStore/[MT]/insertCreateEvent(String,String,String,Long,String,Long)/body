{
  EntityWrapper<ReportingVolumeSnapshotCreateEvent> entityWrapper=EntityWrapper.get(ReportingVolumeSnapshotCreateEvent.class);
  try {
    ReportingVolumeSnapshotCreateEvent volumeSnapshot=new ReportingVolumeSnapshotCreateEvent(uuid,volumeSnapshotUuid,volumeId,timestampMs,userId,sizeGB);
    entityWrapper.add(volumeSnapshot);
    entityWrapper.commit();
    LOG.debug("Added reporting volumeSnapshot to db:" + volumeSnapshot);
  }
 catch (  Exception ex) {
    LOG.error(ex);
    entityWrapper.rollback();
    throw new RuntimeException(ex);
  }
}

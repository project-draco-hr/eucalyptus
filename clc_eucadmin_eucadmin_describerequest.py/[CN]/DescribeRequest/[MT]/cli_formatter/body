def cli_formatter(self, data):
    services = getattr(data, 'euca:registered')
    fmt = '%s\t%-15.15s\t%-15.15s\t%-25s\t%s\t%s'
    for s in services:
        if (s.get('euca:hostName', None) != 'detail'):
            print (fmt % (self.ServiceName.upper(), s.get('euca:partition', None), s.get('euca:name', None), s.get('euca:hostName', None), s.get('euca:state', None), s.get('euca:detail', None)))

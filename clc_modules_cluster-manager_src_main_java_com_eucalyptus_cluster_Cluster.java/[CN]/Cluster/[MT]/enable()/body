{
  try {
    Clusters.getInstance().enable(this);
    final Callable<CheckedListenableFuture<Cluster>> transition=Automata.sequenceTransitions(this,State.PENDING,State.STARTING,State.STARTING_AUTHENTICATING,State.STARTING_NOTREADY,State.NOTREADY,State.DISABLED,State.ENABLING,State.ENABLING_RESOURCES,State.ENABLING_NET,State.ENABLING_VMS,State.ENABLING_ADDRS,State.ENABLING_VMS_PASS_TWO,State.ENABLING_ADDRS_PASS_TWO,State.ENABLED);
    Threads.lookup(ClusterController.class,Cluster.class).submit(transition).get();
  }
 catch (  NoSuchElementException ex) {
    this.configuration.error(ex);
  }
catch (  InterruptedException ex) {
    this.configuration.error(ex);
  }
catch (  ExecutionException ex) {
    this.configuration.error(ex);
  }
}

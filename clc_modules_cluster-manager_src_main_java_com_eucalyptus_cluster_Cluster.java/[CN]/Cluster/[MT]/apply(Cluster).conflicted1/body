{
  try {
    AsyncRequests.newRequest(new EnableServiceCallback(input)).sendSync(input.configuration);
    Clusters.getInstance().register(input);
    return true;
  }
 catch (  Throwable t) {
    if (!input.filterExceptions(t)) {
      return false;
    }
 else {
      Clusters.getInstance().register(input);
      return true;
    }
  }
}

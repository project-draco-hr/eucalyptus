{
  if (Component.State.ENABLED.equals(input.getConfiguration().lookupState()) || Component.State.DISABLED.equals(input.getConfiguration().lookupState()) || Component.State.NOTREADY.equals(input.getConfiguration().lookupState())) {
    try {
      Clusters.getInstance().disable(input.getName());
    }
 catch (    Exception ex) {
      LOG.error(ex,ex);
    }
    try {
      AsyncRequests.newRequest(new DisableServiceCallback(input)).sendSync(input.configuration);
      return true;
    }
 catch (    Throwable t) {
      return input.filterExceptions(t);
    }
  }
 else {
    return false;
  }
}

{
  try {
    if (channel != null) {
      if (!channel.isConnected())       connect();
    }
 else {
      connect();
    }
  }
 catch (  JSchException ex) {
    LOG.error(ex);
    throw new EucalyptusCloudException(ex);
  }
catch (  IOException ex) {
    LOG.error(ex);
    throw new EucalyptusCloudException(ex);
  }
}

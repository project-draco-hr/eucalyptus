{
  Object o=null;
  if ((e instanceof MessageEvent) && this.first.compareAndSet(true,false) && ((o=((MessageEvent)e).getMessage()) instanceof ChannelBuffer)&& !maybeSsl((ChannelBuffer)o)) {
    ctx.getPipeline().removeFirst();
    ctx.sendUpstream(e);
  }
 else {
    super.handleUpstream(ctx,e);
  }
}

{
  EntityWrapper<SshKeyPair> db=KeyPairUtil.getEntityWrapper();
  SshKeyPair key=null;
  try {
    SshKeyPair searchKey=new SshKeyPair(userName);
    searchKey.setPublicKey(keyValue);
    key=db.getUnique(searchKey);
    db.commit();
  }
 catch (  EucalyptusCloudException e) {
    db.rollback();
    throw e;
  }
  return key;
}

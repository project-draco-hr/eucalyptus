{
  List<T> results=Lists.newArrayList();
  JSONArray array=getByType(JSONArray.class,map,key);
  for (  Object o : array) {
    if (o.getClass() == type) {
      results.add((T)o);
    }
 else {
      throw new JSONException("Expecting array element type " + type.getName() + " but got "+ o.getClass().getName());
    }
  }
  return results;
}
